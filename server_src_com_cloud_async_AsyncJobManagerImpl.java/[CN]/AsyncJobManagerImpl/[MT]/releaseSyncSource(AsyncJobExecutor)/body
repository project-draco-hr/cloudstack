{
  if (executor.getSyncSource() != null) {
    if (s_logger.isDebugEnabled())     s_logger.debug("Release sync source for job-" + executor.getJob().getId() + " sync source: "+ executor.getSyncSource().getContentType()+ "-"+ executor.getSyncSource().getContentId());
    _queueMgr.purgeItem(executor.getSyncSource().getId());
    checkQueue(executor.getSyncSource().getQueueId());
  }
}
