{
  if (s_logger.isDebugEnabled())   s_logger.debug("submit async job-" + job.getId() + ", details: "+ job.toString());
  Transaction txt=Transaction.currentTxn();
  try {
    txt.start();
    job.setInitMsid(getMsid());
    _jobDao.persist(job);
    txt.commit();
    scheduleExecution(job,scheduleJobExecutionInContext);
    return job.getId();
  }
 catch (  Exception e) {
    s_logger.error("Unexpected exception: ",e);
    txt.rollback();
  }
  return 0L;
}
