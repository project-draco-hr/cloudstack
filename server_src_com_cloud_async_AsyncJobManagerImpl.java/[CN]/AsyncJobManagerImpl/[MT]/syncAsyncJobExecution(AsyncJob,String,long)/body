{
  if (job.getSyncSource() != null) {
    return;
  }
  if (s_logger.isDebugEnabled())   s_logger.debug("Sync job-" + job.getId() + " execution on object "+ syncObjType+ "."+ syncObjId);
  SyncQueueVO queue=null;
  Random random=new Random();
  for (int i=0; i < 5; i++) {
    queue=_queueMgr.queue(syncObjType,syncObjId,"AsyncJob",job.getId());
    if (queue != null)     break;
    try {
      Thread.sleep(1000 + random.nextInt(5000));
    }
 catch (    InterruptedException e) {
    }
  }
  if (queue == null) {
    throw new CloudRuntimeException("Unable to insert queue item into database, DB is full?");
  }
 else {
    throw new AsyncCommandQueued(queue,"job-" + job.getId() + " queued");
  }
}
