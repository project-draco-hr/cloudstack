@attr(tags=['advanced', 'advancedns', 'selfservice'])
def test_03_delete_detached_volume(self):
    'Delete a Volume unattached to an VM\n        '
    self.debug(('Deleting volume: %s' % self.volume.id))
    cmd = deleteVolume.deleteVolumeCmd()
    cmd.id = self.volume.id
    self.apiclient.deleteVolume(cmd)
    time.sleep(self.services['sleep'])
    list_volume_response = Volume.list(self.apiclient, id=self.volume.id)
    self.assertEqual(list_volume_response, None, ('Volume %s was not deleted' % self.volume.id))
    return
