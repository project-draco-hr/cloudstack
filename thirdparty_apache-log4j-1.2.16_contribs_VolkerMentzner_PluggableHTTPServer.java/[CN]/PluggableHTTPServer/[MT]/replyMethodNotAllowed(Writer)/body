{
  try {
    out.write("HTTP/1.1 405 Method Not Allowed\r\n");
    out.write("Allow: GET, PUT\r\n");
    out.write("<HTML><HEAD><TITLE>Method Not Allowed</TITLE></HEAD>\r\n");
    out.write("<BODY><H1>Method Not Allowed</H1>\r\n");
    out.write("</BODY></HTML>\r\n");
    out.flush();
  }
 catch (  IOException e) {
  }
}
