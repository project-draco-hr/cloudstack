def post_configure(self):
    ' The steps that must be done after a device is configured '
    route = csRoute(self.dev)
    route.routeTable()
    CsRule(self.dev).addMark()
    CsDevice(self.dev).setUp()
    self.arpPing()
    route.add(self.address)
    csRpsrfs(self.dev).enable()
    route.flush()
