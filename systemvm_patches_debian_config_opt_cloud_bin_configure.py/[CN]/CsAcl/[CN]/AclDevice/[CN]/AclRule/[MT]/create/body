def create(self):
    rstr = ''
    rstr = ('%s -A %s -p %s %s' % (rstr, self.chain, self.protocol, self.dest))
    if (self.type == 'icmp'):
        rstr = ('%s -icmp_type %s' % (rstr, self.icmp_type))
    rstr = ('%s %s -j %s' % (rstr, self.dport, self.action))
    fw.append([self.table, 'front', rstr])
