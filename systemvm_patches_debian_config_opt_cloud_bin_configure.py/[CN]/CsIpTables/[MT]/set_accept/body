def set_accept(self, table, method='add'):
    ' Add an accept rule\n        First version - very simple - once I find out what the patterns\n        are this will be refactored\n        '
    slist = [self.devChain, 'ACCEPT']
    if (method == 'add'):
        cmds = '-A %s -j %s'
    else:
        cmds = '-D %s -j %s'
    self.change_rule(table, slist, cmds)
