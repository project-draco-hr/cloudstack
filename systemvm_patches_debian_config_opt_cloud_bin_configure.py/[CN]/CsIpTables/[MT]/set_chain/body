def set_chain(self, table, method):
    ' Create a chain if it does not already exist '
    slist = [self.devChain]
    cmd = ''
    if (not self.has_rule(table, slist)):
        if (table != ''):
            cmd = ('-t %s ' % table)
        cmd += ('-N %s' % tuple(slist))
        CsHelper.execute(('iptables %s' % cmd))
        logging.info('iptables %s', cmd)
