def has_rule(self, table, list):
    ' Check if a particular rule exists '
    cmd = 'iptables-save '
    if (table != ''):
        cmd += ('-t %s' % table)
    for line in CsHelper().execute(cmd):
        matches = len([i for i in list if (i in line)])
        if (matches == len(list)):
            return True
    return False
