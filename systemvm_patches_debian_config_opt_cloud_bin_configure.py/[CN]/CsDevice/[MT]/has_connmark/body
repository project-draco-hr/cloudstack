def has_connmark(self):
    cmd = 'iptables-save -t mangle'
    for line in CsHelper().execute(cmd):
        if (not ('PREROUTING' in line)):
            continue
        if (not ('state' in line)):
            continue
        if (not ('CONNMARK' in line)):
            continue
        if (not ('set-xmark' in line)):
            continue
        if (not (self.dev in line)):
            continue
        return True
    return False
