def buildlist(self):
    '\n        List all available network devices on the system\n        '
    self.devlist = []
    for line in open('/proc/net/dev'):
        vals = line.lstrip().split(':')
        if (not vals[0].startswith('eth')):
            continue
        if (vals[0] == 'eth0'):
            continue
        self.devlist.append(vals[0])
