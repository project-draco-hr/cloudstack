def get_services(self, version=None):
    return {'call': call, 'get_vncport': getVncPort, 'exec_domr': domrExec, 'check_domr_port': domrCheckPort, 'check_domr_ssh': domrCheckSsh, 'ovs_control_interface': ovsControlInterface, 'ovs_mkdir': ovsMkdir, 'ovs_check_file': ovsCheckFile, 'ovs_upload_ssh_key': ovsUploadSshKey, 'ovs_dom0_stats': ovsDom0Stats, 'ovs_domU_stats': ovsDomUStats, 'get_module_version': getModuleVersion, 'ping': ping, }
