{
  super.setUp();
  Annotation[] annotations=getClass().getAnnotations();
  if (annotations != null) {
    for (    Annotation annotation : annotations) {
      if (annotation instanceof ComponentSetup) {
        LegacyComponentLocator.getLocator(((ComponentSetup)annotation).managerName(),((ComponentSetup)annotation).setupXml(),((ComponentSetup)annotation).log4j());
        break;
      }
    }
  }
}
