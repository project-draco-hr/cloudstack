@classmethod
def create(cls, apiclient, volume_id, account=None, domainid=None):
    'Create Snapshot'
    cmd = createSnapshot.createSnapshotCmd()
    cmd.volumeid = volume_id
    if account:
        cmd.account = account
    if domainid:
        cmd.domainid = domainid
    return Snapshot(apiclient.createSnapshot(cmd).__dict__)
