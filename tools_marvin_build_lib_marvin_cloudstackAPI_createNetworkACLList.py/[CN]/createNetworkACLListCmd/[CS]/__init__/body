def __init__(self):
    self.isAsync = 'true'
    'Name of the network ACL List'
    'Required'
    self.name = None
    'Id of the VPC associated with this network ACL List'
    'Required'
    self.vpcid = None
    'Description of the network ACL List'
    self.description = None
    self.required = ['name', 'vpcid']
