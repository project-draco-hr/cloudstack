{
  Object p2=RESOLVER.getValue(field,event);
  boolean result=!((p2 == null) || (p2.toString().equals("")));
  if (result && matches != null) {
    Set entries=(Set)matches.get(field.toUpperCase());
    if (entries == null) {
      entries=new HashSet();
      matches.put(field.toUpperCase(),entries);
    }
    entries.add(p2);
  }
  return result;
}
