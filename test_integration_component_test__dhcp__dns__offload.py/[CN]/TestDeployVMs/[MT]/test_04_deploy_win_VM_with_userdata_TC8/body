@attr(tags=['advanced'], required_hardware='True')
def test_04_deploy_win_VM_with_userdata_TC8(self):
    '\n        1.Create shared network without any services\n        2.Deploy windows vm in the above network\n        3.Verify IP address retrieval\n        '
    self.skipTest('Skip windows tests for now')
    self.services['virtual_machine']['userdata'] = self.userdata
    self.shared_network = Network.create(self.apiclient, self.services['network2'], networkofferingid=self.network_offering1.id, zoneid=self.zone.id)
    self.cleanup.append(self.shared_network)
    self.vm = VirtualMachine.create(self.apiclient, self.services['virtual_machine'], templateid=self.win_template.id, accountid='admin', domainid=1, serviceofferingid=self.service_offering.id, zoneid=self.zone.id, networkids=[self.shared_network.id])
    self.assertIsNotNone(self.vm, 'FAIL:VM Creation failed with windows template in a shared network without any services')
    self.cleanup.append(self.vm)
    (ip_status, ip_addr) = self.getVMIPAddress(self.vm.id)
    self.assertEqual(ip_status, 1, 'Failed to retrieve vm ip address')
    self.list_nics(self.vm_id)
    return
