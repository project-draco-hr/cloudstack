  String rs="<?xml version='1.0'?>" + "<methodResponse>" + "<fault>"+ "<value><struct>"+ "<member>"+ "<name>faultCode</name>"+ "<value><int>" + faultCode + "</int></value>"+ "</member>"+ "<member>"+ "<name>faultString</name>"+ "<value><string>"+ message+ "</string></value>"+ "</member>"+ "</struct></value>"+ "</fault>"+ "</methodResponse>";
