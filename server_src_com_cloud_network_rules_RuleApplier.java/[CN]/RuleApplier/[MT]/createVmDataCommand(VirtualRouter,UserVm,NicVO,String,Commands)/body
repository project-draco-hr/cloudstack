{
  final String serviceOffering=_serviceOfferingDao.findByIdIncludingRemoved(vm.getId(),vm.getServiceOfferingId()).getDisplayText();
  final String zoneName=_dcDao.findById(router.getDataCenterId()).getName();
  cmds.addCommand("vmdata",generateVmDataCommand(router,nic.getIp4Address(),vm.getUserData(),serviceOffering,zoneName,nic.getIp4Address(),vm.getHostName(),vm.getInstanceName(),vm.getId(),vm.getUuid(),publicKey,nic.getNetworkId()));
}
