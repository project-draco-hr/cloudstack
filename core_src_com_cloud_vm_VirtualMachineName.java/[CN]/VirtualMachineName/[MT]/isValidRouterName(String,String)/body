{
  String[] tokens=name.split(SEPARATOR);
  if (tokens.length != 3) {
    return false;
  }
  if (!tokens[0].equals("r")) {
    return false;
  }
  try {
    Long.parseLong(tokens[1]);
  }
 catch (  NumberFormatException ex) {
    return false;
  }
  return instance == null || tokens[2].equals(instance);
}
