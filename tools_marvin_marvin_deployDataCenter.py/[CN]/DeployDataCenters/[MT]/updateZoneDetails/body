def updateZoneDetails(self, zoneid, details):
    zoneCmd = updateZone.updateZoneCmd()
    zoneCmd.id = zoneid
    zoneCmd.details = details
    ret = self.__apiClient.updateZone(zoneCmd)
    if (ret == FAILED):
        self.__tcRunLogger.exception('==== Update Zone  Failed=====')
        self.__cleanAndExit()
    return ret
