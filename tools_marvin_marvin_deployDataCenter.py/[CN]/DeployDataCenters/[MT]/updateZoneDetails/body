def updateZoneDetails(self, zoneid, details):
    zoneCmd = updateZone.updateZoneCmd()
    zoneCmd.id = zoneid
    zoneCmd.details = details
    ret = self.apiClient.updateZone(zoneCmd)
    if (ret == FAILED):
        self.tcRunLogger.exception('==== Update Zone  Failed=====')
    else:
        return ret
