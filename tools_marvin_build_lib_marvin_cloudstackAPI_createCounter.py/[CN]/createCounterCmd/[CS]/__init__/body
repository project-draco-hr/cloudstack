def __init__(self):
    self.isAsync = 'true'
    'Name of the counter.'
    'Required'
    self.name = None
    'Source of the counter.'
    'Required'
    self.source = None
    'Value of the counter e.g. oid in case of snmp.'
    'Required'
    self.value = None
    self.required = ['name', 'source', 'value']
