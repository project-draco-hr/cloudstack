def get_api_list():
    apilist = []
    for api in apis.get_all_apis():
        api_module = api
        api_name = api.__name__.split('.')[(-1)]
        if ((not api_name.startswith('_')) and hasattr(api_module, '__doc__')):
            apilist.append(('    %20s     %s' % (api_name.replace('_', '-'), api_module.__doc__)))
    return apilist
