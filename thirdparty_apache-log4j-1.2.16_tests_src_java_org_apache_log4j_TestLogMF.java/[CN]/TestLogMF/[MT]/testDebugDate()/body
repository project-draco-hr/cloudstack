{
  LogCapture capture=new LogCapture(Level.DEBUG);
  Date epoch=new Date(0);
  LogMF.debug(logger,"Iteration {0}",epoch);
  String expected=MessageFormat.format("Iteration {0}",new Object[]{epoch});
  String expected2="Iteration " + DateFormat.getDateTimeInstance(DateFormat.SHORT,DateFormat.SHORT).format(epoch);
  String actual=capture.getMessage();
  if (System.getProperty("java.vendor").indexOf("Free") == -1) {
    assertEquals(expected,actual);
  }
  assertEquals(expected2,actual);
}
