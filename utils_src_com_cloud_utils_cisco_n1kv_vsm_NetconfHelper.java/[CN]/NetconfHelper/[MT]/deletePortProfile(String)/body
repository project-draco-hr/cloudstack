{
  String command="<?xml version=\"1.0\"?>" + "<nf:rpc xmlns=\"http://www.cisco.com/nxos:1.0:ppm\" " + "xmlns:nf=\"urn:ietf:params:xml:ns:netconf:base:1.0\" message-id=\"110\">"+ "  <nf:edit-config>"+ "    <nf:target>"+ "      <nf:running/>"+ "    </nf:target>"+ "    <nf:config>"+ "      <nxos:configure xmlns:nxos=\"http://www.cisco.com/nxos:1.0:ppm\">"+ "        <nxos:__XML__MODE__exec_configure>"+ "          <no>"+ "            <port-profile>"+ "              <name>"+ "                <__XML__PARAM_value isKey=\"true\">@name</__XML__PARAM_value>"+ "              </name>"+ "            </port-profile>"+ "          </no>"+ "        </nxos:__XML__MODE__exec_configure>"+ "      </nxos:configure>"+ "    </nf:config>"+ "  </nf:edit-config>"+ "</nf:rpc>"+ SSH_NETCONF_TERMINATOR;
  command=command.replace("@name",name);
  send(command);
  String reply=receive();
  return true;
}
