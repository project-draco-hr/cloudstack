{
  Boolean x=false;
  Integer sleep=interval;
  try {
    while (!x && retries > 0) {
      x=(Boolean)nullIsFalseCallWrapper("check_dom0_port",ip,port,interval);
      retries--;
      Thread.sleep(sleep * 1000);
    }
  }
 catch (  Exception e) {
    LOGGER.error("Dom0 port check failed: " + e);
  }
  return x;
}
