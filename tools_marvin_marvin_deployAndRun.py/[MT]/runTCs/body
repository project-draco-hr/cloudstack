def runTCs(num_iter, inp1, inp2):
    '\n    Run Test Cases based upon number of iterations\n    '
    n = 0
    while (n < num_iter):
        engine = TestCaseExecuteEngine(inp2['tc_client'], inp2['tc_parsedcfg'], inp2['tc_runlogger'], inp2['tc_debugstream'])
        if (inp1['tc_file'] is not None):
            engine.loadTestsFromFile(inp1['tc_file'])
        else:
            engine.loadTestsFromDir(inp1['tc_dir'])
        engine.run()
        n = (n + 1)
