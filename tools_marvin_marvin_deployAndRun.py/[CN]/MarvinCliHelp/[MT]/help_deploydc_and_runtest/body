@classmethod
def help_deploydc_and_runtest(cls, deploy=False):
    msg = 'marvincli [deploydc_and_runtest] \n\t[config-file=<path_to_marvin_cfg> \n\ttc-path=<test suite or test suite folder path>\n\tzone=<name of the zone> \n\thyp-type=<hypervisor_type> \n\trequired_hardware=<true\\false>]'
    cls.print_msg(msg)
