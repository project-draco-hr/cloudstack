{
  String path="";
  String query=null;
  try {
    URL url=new URL("http://localhost" + request);
    path=url.getPath();
    query=url.getPath();
    if (path.equals(handledPath) == false) {
      return false;
    }
    out.write("HTTP/1.0 200 OK\r\n");
    if (ReplyType != null)     out.write(ReplyType);
    if (ReplyHTML != null)     out.write(ReplyHTML);
    out.flush();
    return true;
  }
 catch (  Exception ex) {
    return false;
  }
}
