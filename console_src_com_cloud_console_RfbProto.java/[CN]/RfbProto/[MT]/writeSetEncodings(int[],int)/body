{
  byte[] b=new byte[4 + 4 * len];
  b[0]=(byte)SetEncodings;
  b[2]=(byte)((len >> 8) & 0xff);
  b[3]=(byte)(len & 0xff);
  for (int i=0; i < len; i++) {
    b[4 + 4 * i]=(byte)((encs[i] >> 24) & 0xff);
    b[5 + 4 * i]=(byte)((encs[i] >> 16) & 0xff);
    b[6 + 4 * i]=(byte)((encs[i] >> 8) & 0xff);
    b[7 + 4 * i]=(byte)(encs[i] & 0xff);
  }
  os.write(b);
}
