{
  Transaction txn=Transaction.currentTxn();
  txn.start();
  SearchCriteria<VMComputeTagVO> sc=VmIdSearch.create();
  sc.setParameters("vmId",vmId);
  expunge(sc);
  for (  String tag : computeTags) {
    tag=tag.trim();
    if (tag.length() > 0) {
      VMComputeTagVO vo=new VMComputeTagVO(vmId,tag);
      persist(vo);
    }
  }
  txn.commit();
}
