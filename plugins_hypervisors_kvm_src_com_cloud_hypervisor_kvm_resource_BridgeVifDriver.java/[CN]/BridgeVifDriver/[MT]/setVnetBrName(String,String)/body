{
  String brName=null;
  if (bridgeNameSchema != null) {
    if (bridgeNameSchema.equalsIgnoreCase("3.0")) {
      brName="cloudVirBr" + vnetId;
    }
 else     if (bridgeNameSchema.equalsIgnoreCase("4.0")) {
      brName="br" + pifName + "-"+ vnetId;
    }
  }
 else {
    brName="br" + pifName + "-"+ vnetId;
  }
  return brName;
}
