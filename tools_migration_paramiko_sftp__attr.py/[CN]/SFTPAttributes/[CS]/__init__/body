def __init__(self):
    '\n        Create a new (empty) SFTPAttributes object.  All fields will be empty.\n        '
    self._flags = 0
    self.st_size = None
    self.st_uid = None
    self.st_gid = None
    self.st_mode = None
    self.st_atime = None
    self.st_mtime = None
    self.attr = {}
