def config(self):
    try:
        if (bash('service apparmor status').getStdout() == ''):
            self.spRunning = False
            return True
        bash('service apparmor stop')
        bash('update-rc.d -f apparmor remove')
        return True
    except:
        raise CloudRuntimeException('Failed to configure apparmor, please see the /var/log/cloud/setupAgent.log for detail,                                         or you can manually disable it before starting myCloud')
