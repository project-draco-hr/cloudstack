{
  Transaction txn=Transaction.currentTxn();
  txn.start();
  SearchCriteria<VMNetworkMapVO> sc=VmIdSearch.create();
  sc.setParameters("vmId",vmId);
  expunge(sc);
  for (  Long networkId : networks) {
    VMNetworkMapVO vo=new VMNetworkMapVO(vmId,networkId);
    persist(vo);
  }
  txn.commit();
}
