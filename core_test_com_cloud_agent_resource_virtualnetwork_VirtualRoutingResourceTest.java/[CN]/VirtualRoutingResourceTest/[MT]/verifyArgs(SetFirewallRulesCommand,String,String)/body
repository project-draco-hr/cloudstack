{
  assertEquals(script,VRScripts.FIREWALL_INGRESS);
  assertTrue(args.startsWith(" -F -a "));
  assertTrue(args.contains("64.10.10.10:ICMP:0:0:10.10.1.1/24-10.10.1.2/24:"));
