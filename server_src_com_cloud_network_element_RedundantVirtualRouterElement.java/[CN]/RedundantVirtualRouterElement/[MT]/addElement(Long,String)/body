{
  long serviceOfferingId=_routerMgr.getDefaultVirtualRouterServiceOfferingId();
  if (serviceOfferingId == 0) {
    return false;
  }
  VirtualRouterElementsVO element=new VirtualRouterElementsVO(nspId,uuid,serviceOfferingId,false,VirtualRouterElementsType.RedundantVirtualRouterElement,false,false,false,false,false,false,false);
  _vrElementsDao.persist(element);
  return true;
}
