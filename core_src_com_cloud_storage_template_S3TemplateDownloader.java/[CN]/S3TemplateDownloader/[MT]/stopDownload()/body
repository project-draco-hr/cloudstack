{
switch (getStatus()) {
case IN_PROGRESS:
    if (request != null) {
      request.abort();
    }
  status=TemplateDownloader.Status.ABORTED;
return true;
case UNKNOWN:
case NOT_STARTED:
case RECOVERABLE_ERROR:
case UNRECOVERABLE_ERROR:
case ABORTED:
status=TemplateDownloader.Status.ABORTED;
case DOWNLOAD_FINISHED:
try {
S3Utils.deleteObject(s3,s3.getBucketName(),s3Key);
}
 catch (Exception ex) {
}
return true;
default :
return true;
}
}
