{
  try {
    Driver d=(Driver)(Class.forName("oracle.jdbc.driver.OracleDriver").newInstance());
    DriverManager.registerDriver(d);
  }
 catch (  Exception e) {
  }
  cat.setPriority(Priority.INFO);
  PropertyConfigurator.configure("log4jtestprops.txt");
  cat.debug("debug");
  cat.info("info");
  cat.error("error");
  cat.fatal("fatal");
}
