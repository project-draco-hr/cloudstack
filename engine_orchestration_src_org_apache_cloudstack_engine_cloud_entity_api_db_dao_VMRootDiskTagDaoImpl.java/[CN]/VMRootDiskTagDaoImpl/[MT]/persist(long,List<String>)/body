{
  Transaction txn=Transaction.currentTxn();
  txn.start();
  SearchCriteria<VMRootDiskTagVO> sc=VmIdSearch.create();
  sc.setParameters("vmId",vmId);
  expunge(sc);
  for (  String tag : rootDiskTags) {
    if (tag != null) {
      tag=tag.trim();
      if (tag.length() > 0) {
        VMRootDiskTagVO vo=new VMRootDiskTagVO(vmId,tag);
        persist(vo);
      }
    }
  }
  txn.commit();
}
