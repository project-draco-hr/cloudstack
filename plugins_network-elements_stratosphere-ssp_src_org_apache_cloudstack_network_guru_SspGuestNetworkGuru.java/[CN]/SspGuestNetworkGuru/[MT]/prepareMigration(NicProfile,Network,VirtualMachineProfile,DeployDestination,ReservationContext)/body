{
  try {
    reserve(nic,network,vm,dest,context);
  }
 catch (  InsufficientVirtualNetworkCapacityException e) {
    logger.error("prepareForMigration failed",e);
    return false;
  }
catch (  InsufficientAddressCapacityException e) {
    logger.error("prepareForMigration failed",e);
    return false;
  }
  return true;
}
