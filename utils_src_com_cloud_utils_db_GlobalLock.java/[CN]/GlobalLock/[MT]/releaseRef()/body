{
  int refCount;
synchronized (s_lockMap) {
synchronized (this) {
      referenceCount--;
      refCount=referenceCount;
      if (referenceCount < 0)       s_logger.warn("Unmatched Global lock " + name + " reference usage detected, check your code!");
      if (referenceCount == 0)       releaseInternLock(name);
    }
  }
  return refCount;
}
