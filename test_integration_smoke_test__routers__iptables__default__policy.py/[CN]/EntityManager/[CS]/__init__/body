def __init__(self, apiclient, services, service_offering, account, zone, logger):
    self.apiclient = apiclient
    self.services = services
    self.service_offering = service_offering
    self.account = account
    self.zone = zone
    self.logger = logger
    self.cleanup = []
    self.networks = []
    self.routers = []
    self.ips = []
