def __init__(self, testclient, config, tc_logger=None, debug_stream=None):
    '\n        Initialize the testcase execution engine, just the basics here\n        @var testcaseLogFile: client log file\n        @var testResultLogFile: summary report file\n        '
    self.testclient = testclient
    self.config = config
    self.tcRunLogger = tc_logger
    self.debugStream = debug_stream
    self.loader = unittest.loader.TestLoader()
    self.suite = None
