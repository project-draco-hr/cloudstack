{
  PropertyConfigurator.configure("input/patternLayout4.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT2,EXCEPTION1,EXCEPTION2,EXCEPTION3,EXCEPTION4,EXCEPTION5});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new LineNumberFilter(),new AbsoluteDateAndTimeFilter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(Compare.compare(FILTERED,"witness/patternLayout.4"));
}
