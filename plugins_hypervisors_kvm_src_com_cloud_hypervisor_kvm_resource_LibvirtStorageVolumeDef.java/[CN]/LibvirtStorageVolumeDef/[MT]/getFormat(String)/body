{
  if (format == null) {
    return null;
  }
  if (format.equalsIgnoreCase("raw")) {
    return RAW;
  }
 else   if (format.equalsIgnoreCase("qcow2")) {
    return QCOW2;
  }
 else   if (format.equalsIgnoreCase("dir")) {
    return DIR;
  }
 else   if (format.equalsIgnoreCase("tar")) {
    return TAR;
  }
  return null;
}
