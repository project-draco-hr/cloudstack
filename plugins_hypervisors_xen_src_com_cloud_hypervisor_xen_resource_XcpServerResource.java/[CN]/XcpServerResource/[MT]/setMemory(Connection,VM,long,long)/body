{
  if (s_logger.isDebugEnabled()) {
    s_logger.debug("Memory Limits for VM [" + vm.getNameLabel(conn) + "[staticMin:"+ mem_32m+ ", staticMax:"+ maxMemsize+ ", dynamicMin: "+ minMemsize+ ", dynamicMax:"+ maxMemsize+ "]]");
  }
  vm.setMemoryLimits(conn,mem_32m,maxMemsize,minMemsize,maxMemsize);
}
