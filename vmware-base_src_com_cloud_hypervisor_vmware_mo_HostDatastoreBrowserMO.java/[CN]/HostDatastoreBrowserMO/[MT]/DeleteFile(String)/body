{
  if (s_logger.isTraceEnabled())   s_logger.trace("vCenter API trace - deleteFile(). target mor: " + _mor.get_value() + ", file datastore path: "+ datastoreFullPath);
  _context.getService().deleteFile(_mor,datastoreFullPath);
  if (s_logger.isTraceEnabled())   s_logger.trace("vCenter API trace - deleteFile() done");
}
