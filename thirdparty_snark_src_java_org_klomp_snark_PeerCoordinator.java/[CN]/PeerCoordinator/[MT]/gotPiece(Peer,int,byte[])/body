{
  if (halted) {
    return true;
  }
synchronized (wantedPieces) {
    Integer p=new Integer(piece);
    if (!wantedPieces.contains(p)) {
      log.log(Level.FINER,peer + " piece " + piece+ " no longer needed");
      return true;
    }
    try {
      if (storage.putPiece(piece,bs)) {
        log.log(Level.FINER,"Recv p" + piece + " "+ peer);
      }
 else {
        downloaded-=metainfo.getPieceLength(piece);
        log.log(Level.INFO,"Got BAD piece " + piece + " from "+ peer);
        return false;
      }
    }
 catch (    IOException ioe) {
      Snark.abort("Error writing storage",ioe);
    }
    wantedPieces.remove(p);
  }
synchronized (peers) {
    Iterator it=peers.iterator();
    while (it.hasNext()) {
      Peer p=(Peer)it.next();
      if (p.isConnected()) {
        p.have(piece);
      }
    }
  }
  if (completed()) {
    client.interrupt();
  }
  return true;
}
