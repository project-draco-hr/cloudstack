{
  if (halted) {
    peer.disconnect(false);
    return;
  }
synchronized (peers) {
    if (peerIDInList(peer.getPeerID(),peers)) {
      log.log(Level.FINER,"Already connected to: " + peer);
      peer.disconnect(false);
    }
 else {
      log.log(Level.FINER,"New connection to peer: " + peer);
      peers.add(0,peer);
      unchokePeer();
      if (listener != null) {
        listener.peerChange(this,peer);
      }
    }
  }
}
