{
  if (interest) {
synchronized (peers) {
      if (uploaders < MAX_UPLOADERS) {
        if (peer.isChoking()) {
          uploaders++;
          peer.setChoking(false);
          log.log(Level.FINER,"Unchoke: " + peer);
        }
      }
    }
  }
  if (listener != null) {
    listener.peerChange(this,peer);
  }
}
