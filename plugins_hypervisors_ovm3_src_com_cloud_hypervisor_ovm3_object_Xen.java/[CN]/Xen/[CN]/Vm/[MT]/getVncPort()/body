{
  if (_getVnc("port") != null) {
    return Integer.parseInt(_getVnc("port"));
  }
  String vnc=getVncLocation();
  if (vnc.contains(":")) {
    final String[] res=vnc.split(":");
    vmVnc.put("vnclisten",res[0]);
    vmVnc.put("port",res[1]);
    return Integer.parseInt(res[1]);
  }
  return null;
}
