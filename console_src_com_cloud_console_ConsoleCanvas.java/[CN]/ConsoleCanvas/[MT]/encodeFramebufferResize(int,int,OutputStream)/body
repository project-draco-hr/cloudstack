{
  DataOutputStream dos;
  if (os instanceof DataOutputStream)   dos=(DataOutputStream)os;
 else   dos=new DataOutputStream(os);
  dos.writeByte(RfbProto.FramebufferUpdate);
  dos.writeByte(0);
  dos.writeShort(1);
  dos.writeShort(0);
  dos.writeShort(0);
  dos.writeShort(w);
  dos.writeShort(h);
  dos.writeInt(RfbProto.EncodingNewFBSize);
}
