{
  try {
    timestamp=0;
    while (true) {
      processNormalProtocol2();
    }
  }
 catch (  IOException e) {
    viewer.onProtocolProcessException(e);
    throw e;
  }
catch (  OutOfMemoryError e) {
    s_logger.error("Unrecoverable OutOfMemory Error, exit and let it be re-launched");
    System.exit(1);
  }
}
