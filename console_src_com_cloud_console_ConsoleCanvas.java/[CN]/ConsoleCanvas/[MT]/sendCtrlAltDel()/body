{
  KeyEvent evt=new KeyEvent(this,KeyEvent.KEY_PRESSED,System.currentTimeMillis(),InputEvent.CTRL_MASK | InputEvent.ALT_MASK,127);
  boolean bCloseRfb=false;
synchronized (rfb) {
    try {
      rfb.writeKeyEvent(evt);
    }
 catch (    IOException e) {
      bCloseRfb=true;
    }
    rfb.notify();
  }
  if (bCloseRfb) {
synchronized (viewer) {
      if (rfb != null)       rfb.close();
    }
  }
}
