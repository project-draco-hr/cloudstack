{
  FileInputStream strm=null;
  byte[] b=new byte[8];
  try {
    strm=new FileInputStream(file);
    strm.skip(24);
    strm.read(b);
  }
 catch (  Exception e) {
    s_logger.warn("Unable to read qcow2 file " + file,e);
    return null;
  }
 finally {
    if (strm != null) {
      try {
        strm.close();
      }
 catch (      IOException e) {
      }
    }
  }
  long templateSize=NumbersUtil.bytesToLong(b);
  return templateSize;
}
