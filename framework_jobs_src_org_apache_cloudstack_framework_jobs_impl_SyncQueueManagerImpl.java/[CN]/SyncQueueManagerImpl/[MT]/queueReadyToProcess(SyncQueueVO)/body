{
  int nActiveItems=_syncQueueItemDao.getActiveQueueItemCount(queueVO.getId());
  if (nActiveItems < queueVO.getQueueSizeLimit())   return true;
  if (logger.isDebugEnabled())   logger.debug("Queue (queue id, sync type, sync id) - (" + queueVO.getId() + ","+ queueVO.getSyncObjType()+ ", "+ queueVO.getSyncObjId()+ ") is reaching concurrency limit "+ queueVO.getQueueSizeLimit());
  return false;
}
