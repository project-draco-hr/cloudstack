{
  Account caller=CallContext.current().getCallingAccount();
  Long sourceIpAddressId=rule.getSourceIpAddressId();
  return createFirewallRule(sourceIpAddressId,caller,rule.getXid(),rule.getSourcePortStart(),rule.getSourcePortEnd(),rule.getProtocol(),rule.getSourceCidrList(),rule.getIcmpCode(),rule.getIcmpType(),null,rule.getType(),rule.getNetworkId(),rule.getTrafficType());
}
