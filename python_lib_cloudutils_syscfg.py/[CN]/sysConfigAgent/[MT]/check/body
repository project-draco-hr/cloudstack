def check(self):
    if self.env.debug:
        return True
    if (os.geteuid() != 0):
        raise CloudInternalException('Need to execute with root permission')
    kvmEnabled = self.svo.isKVMEnabled()
    if (not kvmEnabled):
        raise CloudInternalException('Checking KVM...[Failed]\nPlease enable KVM on this machine\n')
    return True
