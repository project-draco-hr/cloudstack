def _connect(self):
    if self.region:
        region = self._get_region(self.region)
        self.connection = region.connect(aws_access_key_id=self.access_key, aws_secret_access_key=self.secret_key)
    else:
        self.connection = boto.ec2.connection.EC2Connection(host=self.endpoint, aws_access_key_id=self.access_key, aws_secret_access_key=self.secret_key)
