{
  DataCenter zone=_configMgr.getZone(config.getDataCenterId());
  if (config.getGuestType() != Network.GuestType.Isolated || config.getTrafficType() != TrafficType.Guest) {
    s_logger.trace("Not handling network with Type  " + config.getGuestType() + " and traffic type "+ config.getTrafficType());
    return false;
  }
  return (_networkManager.networkIsConfiguredForExternalNetworking(zone.getId(),config.getNetworkOfferingId()) && _ntwkOfferingSrvcDao.isProviderSupported(config.getNetworkOfferingId(),Service.Lb,Network.Provider.Netscaler));
}
