{
  VolumeVO volume=_volumeDao.findById(volumeTo.getId());
  Long diskOfferingId=volume.getDiskOfferingId();
  Long offeringId=null;
  if (diskOfferingId != null) {
    DiskOfferingVO offering=_diskOfferingDao.findById(diskOfferingId);
    if (offering != null && (offering.getType() == DiskOfferingVO.Type.Disk)) {
      offeringId=offering.getId();
    }
  }
  UsageEventUtils.publishUsageEvent(type,vmSnapshot.getAccountId(),userVm.getDataCenterId(),userVm.getId(),vmSnapshot.getName(),offeringId,volume.getId(),volumeTo.getChainSize(),VMSnapshot.class.getName(),vmSnapshot.getUuid());
}
