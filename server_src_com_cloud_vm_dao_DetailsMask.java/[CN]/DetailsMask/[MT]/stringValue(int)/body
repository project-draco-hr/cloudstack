{
  StringBuffer buf=new StringBuffer();
  for (int i=0; i < values.length; i++) {
    if (mvId == values[i]) {
      return strings[i];
    }
 else     if ((mvId & values[i]) == values[i]) {
      buf.append(strings[i]).append("|");
    }
  }
  if (buf.length() == 0) {
    return "";
  }
  return buf.deleteCharAt(buf.length() - 1).toString();
}
