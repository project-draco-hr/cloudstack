{
  if (s_logger.isDebugEnabled()) {
    s_logger.debug("Stopping vm=" + vmId);
  }
  UserVmVO vm=_vmDao.findById(vmId);
  if (vm == null || vm.getRemoved() != null) {
    if (s_logger.isDebugEnabled()) {
      s_logger.debug("VM is either removed or deleted.");
    }
    return true;
  }
  return stop(userId,vm,0);
}
