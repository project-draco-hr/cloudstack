{
  if (!serviceOffering.isDynamic()) {
    UsageEventUtils.publishUsageEvent(eventType,vm.getAccountId(),vm.getDataCenterId(),vm.getId(),vm.getHostName(),serviceOffering.getId(),vm.getTemplateId(),vm.getHypervisorType().toString(),VirtualMachine.class.getName(),vm.getUuid());
  }
 else {
    UsageEventUtils.publishUsageEvent(eventType,vm.getAccountId(),vm.getDataCenterId(),vm.getId(),vm.getHostName(),serviceOffering.getId(),vm.getTemplateId(),vm.getHypervisorType().toString(),VirtualMachine.class.getName(),vm.getUuid(),customParameters);
  }
}
