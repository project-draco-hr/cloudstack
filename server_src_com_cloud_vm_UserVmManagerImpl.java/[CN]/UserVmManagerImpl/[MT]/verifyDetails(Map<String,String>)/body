{
  if (details != null) {
    String minIops=(String)details.get("minIops");
    String maxIops=(String)details.get("maxIops");
    verifyMinAndMaxIops(minIops,maxIops);
    minIops=(String)details.get("minIopsDo");
    maxIops=(String)details.get("maxIopsDo");
    verifyMinAndMaxIops(minIops,maxIops);
  }
}
