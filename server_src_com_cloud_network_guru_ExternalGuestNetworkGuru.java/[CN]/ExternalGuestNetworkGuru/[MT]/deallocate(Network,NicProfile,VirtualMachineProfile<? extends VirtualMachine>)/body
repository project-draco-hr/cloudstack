{
  super.deallocate(config,nic,vm);
  if (_ovsNetworkMgr.isOvsNetworkEnabled() || _tunnelMgr.isOvsTunnelEnabled()) {
    return;
  }
  if (config.isSpecifiedCidr()) {
    return;
  }
  nic.setIp4Address(null);
  nic.setGateway(null);
  nic.setNetmask(null);
  nic.setBroadcastUri(null);
  nic.setIsolationUri(null);
}
