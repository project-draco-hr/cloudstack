{
  File permKey=new File("/root/.ssh/id_rsa.cloud");
  boolean result=true;
  try {
    SshHelper.scpTo(routerIp,3922,"root",permKey,null,path,content.getBytes(),filename,null);
  }
 catch (  Exception e) {
    s_logger.warn("Fail to create file " + path + filename+ " in VR "+ routerIp,e);
    result=false;
  }
  return result;
}
