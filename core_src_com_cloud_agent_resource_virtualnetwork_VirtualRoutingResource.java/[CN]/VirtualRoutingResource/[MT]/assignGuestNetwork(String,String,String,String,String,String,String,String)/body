{
  String args=" -C";
  args+=" -d " + dev;
  args+=" -i " + routerGIP;
  args+=" -g " + gateway;
  args+=" -m " + cidr;
  args+=" -n " + netmask;
  if (dns != null && !dns.isEmpty()) {
    args+=" -s " + dns;
  }
  if (domainName != null && !domainName.isEmpty()) {
    args+=" -e " + domainName;
  }
  return routerProxy("vpc_guestnw.sh",routerIP,args);
}
