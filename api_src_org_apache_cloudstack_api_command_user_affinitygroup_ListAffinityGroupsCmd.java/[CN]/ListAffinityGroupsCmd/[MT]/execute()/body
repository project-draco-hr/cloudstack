{
  ListResponse<AffinityGroupResponse> response=_queryService.listAffinityGroups(id,affinityGroupName,affinityGroupType,virtualMachineId,getAccountName(),getDomainId(),isRecursive(),listAll(),getStartIndex(),getPageSizeVal(),getKeyword());
  response.setResponseName(getCommandName());
  setResponseObject(response);
}
