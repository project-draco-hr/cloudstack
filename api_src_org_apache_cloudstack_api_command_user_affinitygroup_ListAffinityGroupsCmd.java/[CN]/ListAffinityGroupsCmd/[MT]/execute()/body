{
  ListResponse<AffinityGroupResponse> response=_queryService.listAffinityGroups(id,affinityGroupName,affinityGroupType,virtualMachineId,this.getAccountName(),this.getDomainId(),this.isRecursive(),this.listAll(),this.getStartIndex(),this.getPageSizeVal());
  response.setResponseName(getCommandName());
  this.setResponseObject(response);
}
