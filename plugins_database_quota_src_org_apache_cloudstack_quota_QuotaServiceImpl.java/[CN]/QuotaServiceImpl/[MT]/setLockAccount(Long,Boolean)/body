{
  QuotaAccountVO acc=_quotaAcc.findByIdQuotaAccount(accountId);
  if (acc == null) {
    acc=new QuotaAccountVO(accountId);
    acc.setQuotaEnforce(enforce ? 1 : 0);
    _quotaAcc.persistQuotaAccount(acc);
  }
 else {
    acc.setQuotaEnforce(enforce ? 1 : 0);
    _quotaAcc.updateQuotaAccount(accountId,acc);
  }
}
