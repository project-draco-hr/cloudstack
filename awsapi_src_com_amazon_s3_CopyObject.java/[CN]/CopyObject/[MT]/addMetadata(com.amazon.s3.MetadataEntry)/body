{
  if (localMetadata == null) {
    localMetadata=new com.amazon.s3.MetadataEntry[]{};
  }
  localMetadataTracker=true;
  java.util.List list=org.apache.axis2.databinding.utils.ConverterUtil.toList(localMetadata);
  list.add(param);
