{
  ComponentInfo<GenericDao<?,?>> info=_daoMap.get(name);
  if (info != null) {
    return info;
  }
  if (_parentLocator != null) {
    info=_parentLocator.getDao(name);
  }
  if (info == null) {
    throw new CloudRuntimeException("Unable to find DAO " + name);
  }
  _daoMap.put(name,info);
  return info;
}
