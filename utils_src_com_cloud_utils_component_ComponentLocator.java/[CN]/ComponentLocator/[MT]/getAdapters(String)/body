{
  Adapters<? extends Adapter> adapters=_adapterMap.get(key);
  if (adapters != null) {
    return adapters;
  }
  if (_parentLocator != null) {
    adapters=_parentLocator.getAdapters(key);
    if (adapters != null) {
      return adapters;
    }
  }
  return new Adapters<Adapter>(key,new ArrayList<ComponentInfo<Adapter>>());
}
