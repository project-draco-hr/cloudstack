def name(sxpr):
    "Get the element name of an sxpr.\n    If the sxpr is not an element (i.e. it's an atomic value) its name\n    is None.\n\n    sxpr\n\n    returns name (None if not an element).\n    "
    val = None
    if isinstance(sxpr, types.StringType):
        val = sxpr
    elif (isinstance(sxpr, (types.ListType, types.TupleType)) and len(sxpr)):
        val = sxpr[0]
    return val
