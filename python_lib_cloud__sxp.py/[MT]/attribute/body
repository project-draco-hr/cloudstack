def attribute(sxpr, key, val=None):
    'Get an attribute of an sxpr.\n\n    sxpr sxpr\n    key  attribute key\n    val  default value (default None)\n\n    returns attribute value\n    '
    for x in attributes(sxpr):
        if (x[0] == key):
            val = x[1]
            break
    return val
