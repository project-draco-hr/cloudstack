def parse(io):
    "Completely parse all input from 'io'.\n\n    io\tinput file object\n    returns list of values, None if incomplete\n    raises ParseError on parse error\n    "
    pin = Parser()
    while 1:
        buf = io.readline()
        pin.input(buf)
        if (len(buf) == 0):
            break
    if pin.ready():
        val = pin.get_all()
    else:
        val = None
    return val
