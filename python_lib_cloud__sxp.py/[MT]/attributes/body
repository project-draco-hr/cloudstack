def attributes(sxpr):
    'Get the attribute list of an sxpr.\n\n    sxpr\n\n    returns attribute list\n    '
    val = []
    if (isinstance(sxpr, (types.ListType, types.TupleType)) and (len(sxpr) > 1)):
        attr = sxpr[1]
        if elementp(attr, k_attr_open):
            val = attr[1:]
    return val
