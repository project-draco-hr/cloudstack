def get_api_module(self, api_name, api_class_strs=[]):
    try:
        api_mod = __import__(('marvin.cloudstackAPI.%s' % api_name), globals(), locals(), api_class_strs, (-1))
    except ImportError as e:
        self.print_shell(('Error: API %s not found!' % e))
        return None
    return api_mod
