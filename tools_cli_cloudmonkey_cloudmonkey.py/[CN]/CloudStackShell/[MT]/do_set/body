def do_set(self, args):
    '\n        Set config for CloudStack CLI. Available options are:\n        host, port, apiKey, secretKey, log_file, history_file\n        '
    args = args.split(' ')
    if (len(args) == 2):
        (key, value) = args
        setattr(self, key, value)
        self.write_config()
    else:
        self.print_shell('Please use the syntax: set valid-key value')
