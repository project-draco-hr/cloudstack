def make_request(self, command, args={}, isasync=False):
    (response, error) = monkeyrequest(command, args, isasync, self.asyncblock, logger, self.host, self.port, self.apikey, self.secretkey, self.timeout, self.protocol, self.path)
    if (error is not None):
        self.monkeyprint(error)
    return response
