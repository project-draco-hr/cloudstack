def do_shell(self, args):
    '\n        Execute shell commands using shell <command> or !<command>\n\n        Example:\n        !ls\n        shell ls\n        !for((i=0; i<10; i++)); do cloudmonkey create user account=admin             email=test@test.tt firstname=user$i lastname=user$i             password=password username=user$i; done\n        '
    os.system(args)
