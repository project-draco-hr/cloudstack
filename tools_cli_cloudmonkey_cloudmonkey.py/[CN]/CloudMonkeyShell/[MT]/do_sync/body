def do_sync(self, args):
    '\n        Asks cloudmonkey to discovery and sync apis available on user specified\n        CloudStack host server which has the API discovery plugin, on failure\n        it rollbacks last datastore or api precached datastore.\n        '
    response = self.make_request('listApis')
    self.apicache = monkeycache(response)
    savecache(self.apicache, self.cache_file)
    self.loadcache()
