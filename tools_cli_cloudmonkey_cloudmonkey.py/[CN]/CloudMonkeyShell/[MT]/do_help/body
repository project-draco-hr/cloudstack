def do_help(self, args):
    '\n        Show help docs for various topics\n\n        Example:\n        help list\n        help list users\n        ?list\n        ?list users\n        '
    fields = args.partition(' ')
    if (fields[2] == ''):
        cmd.Cmd.do_help(self, args)
    else:
        verb = fields[0]
        subject = fields[2].partition(' ')[0]
        if (subject in self.cache_verbs[verb]):
            self.monkeyprint(self.cache_verbs[verb][subject][2])
        else:
            self.monkeyprint(('Error: no such api (%s) on %s' % (subject, verb)))
