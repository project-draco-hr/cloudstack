def print_shell(self, *args):
    output = ''
    try:
        for arg in args:
            arg = str(arg)
            if isinstance(type(args), types.NoneType):
                continue
            output += arg
        if (self.color == 'true'):
            monkeyprint(output)
        else:
            print output
    except Exception as e:
        self.print_shell(('Error: ' + e))
