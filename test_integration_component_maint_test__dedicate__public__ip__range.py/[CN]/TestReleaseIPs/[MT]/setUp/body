def setUp(self):
    self.apiclient = self.testClient.getApiClient()
    self.dbclient = self.testClient.getDbConnection()
    self.cleanup = []
    self.testdata['publiciprange']['vlan'] = get_free_vlan(self.apiclient, self.zone.id)[1]
    shared_network_subnet_number = random.randrange(1, 254)
    self.testdata['publiciprange']['gateway'] = (('172.16.' + str(shared_network_subnet_number)) + '.1')
    self.testdata['publiciprange']['startip'] = (('172.16.' + str(shared_network_subnet_number)) + '.2')
    self.testdata['publiciprange']['endip'] = (('172.16.' + str(shared_network_subnet_number)) + '.3')
    self.testdata['publiciprange']['netmask'] = '255.255.255.0'
    return
