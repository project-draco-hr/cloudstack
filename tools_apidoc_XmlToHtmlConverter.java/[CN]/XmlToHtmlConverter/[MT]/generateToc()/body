{
  try {
    TransformerFactory tFactory=TransformerFactory.newInstance();
    Transformer transformer1=tFactory.newTransformer(new javax.xml.transform.stream.StreamSource("generatetoc.xsl"));
    transformer1.transform(new javax.xml.transform.stream.StreamSource("apis/apiSummarySorted.xml"),new javax.xml.transform.stream.StreamResult(new FileOutputStream("html/index.html")));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
