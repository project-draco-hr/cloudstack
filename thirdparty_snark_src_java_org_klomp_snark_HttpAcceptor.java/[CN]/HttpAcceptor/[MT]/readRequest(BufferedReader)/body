{
  String request=br.readLine();
  if (request != null && request.startsWith("GET ")) {
    String resource;
    int index=request.indexOf(' ',4);
    if (index == -1) {
      resource=request.substring(4);
    }
 else {
      resource=request.substring(4,index);
    }
    return resource;
  }
 else {
    return null;
  }
}
