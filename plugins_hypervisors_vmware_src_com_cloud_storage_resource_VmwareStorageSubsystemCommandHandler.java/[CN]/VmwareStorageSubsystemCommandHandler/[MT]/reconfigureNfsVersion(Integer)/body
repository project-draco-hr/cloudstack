{
  try {
    VmwareStorageProcessor processor=(VmwareStorageProcessor)this.processor;
    processor.setNfsVersion(nfsVersion);
    this._nfsVersion=nfsVersion;
    return true;
  }
 catch (  Exception e) {
    s_logger.error("Error while reconfiguring NFS version " + nfsVersion);
    return false;
  }
}
