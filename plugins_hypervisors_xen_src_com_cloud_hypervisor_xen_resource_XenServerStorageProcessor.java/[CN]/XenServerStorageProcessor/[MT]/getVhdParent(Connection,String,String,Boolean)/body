{
  String parentUuid=hypervisorResource.callHostPlugin(conn,"vmopsSnapshot","getVhdParent","primaryStorageSRUuid",primaryStorageSRUuid,"snapshotUuid",snapshotUuid,"isISCSI",isISCSI.toString());
  if (parentUuid == null || parentUuid.isEmpty() || parentUuid.equalsIgnoreCase("None")) {
    s_logger.debug("Unable to get parent of VHD " + snapshotUuid + " in SR "+ primaryStorageSRUuid);
    return null;
  }
  return parentUuid;
}
