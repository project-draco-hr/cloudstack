{
  String result=null;
  try {
    result=hypervisorResource.callHostPluginAsync(conn,"swiftxen","swift",wait,"op","upload","url",swift.getUrl(),"account",swift.getAccount(),"username",swift.getUserName(),"key",swift.getKey(),"container",container,"ldir",ldir,"lfilename",lfilename,"isISCSI",isISCSI.toString());
    if (result != null && result.equals("true")) {
      return true;
    }
  }
 catch (  Exception e) {
    s_logger.warn("swift upload failed due to " + e.toString(),e);
  }
  return false;
}
