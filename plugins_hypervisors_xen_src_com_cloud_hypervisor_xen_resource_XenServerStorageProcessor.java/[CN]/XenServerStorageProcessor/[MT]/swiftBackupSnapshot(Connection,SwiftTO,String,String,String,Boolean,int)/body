{
  String lfilename;
  String ldir;
  if (isISCSI) {
    ldir="/dev/VG_XenStorage-" + srUuid;
    lfilename="VHD-" + snapshotUuid;
  }
 else {
    ldir="/var/run/sr-mount/" + srUuid;
    lfilename=snapshotUuid + ".vhd";
  }
  swiftUpload(conn,swift,container,ldir,lfilename,isISCSI,wait);
  return lfilename;
}
