{
  String status="<?xml version=\"1.0\"?>" + "<nc:rpc message-id=\"1\" xmlns:nc=\"urn:ietf:params:xml:ns:netconf:base:1.0" + "\"xmlns=\"http://www.cisco.com/nxos:1.0:xml\">"+ "  <nc:get>"+ "    <nc:filter type=\"subtree\">"+ "      <show>"+ "        <xml>"+ "          <server>"+ "            <status/>"+ "          </server>"+ "        </xml>"+ "      </show>"+ "    </nc:filter>"+ "  </nc:get>"+ "</nc:rpc>"+ SSH_NETCONF_TERMINATOR;
  send(status);
  parseOkReply(receive());
}
