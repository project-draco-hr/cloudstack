{
  if (buf == null)   return;
  if (verbose)   System.out.println("[" + this + "] INFO: Data received: "+ buf+ ".");
  if (buf.length + 4 > 65535)   throw new RuntimeException("Packet is too long for TPKT (max length 65535-4): " + buf + ".");
  ByteBuffer data=new ByteBuffer(4);
  data.writeByte(3);
  data.writeByte(0);
  data.writeShort(buf.length + 4);
  buf.prepend(data);
  data.unref();
  pushDataToPad(STDOUT,buf);
}
