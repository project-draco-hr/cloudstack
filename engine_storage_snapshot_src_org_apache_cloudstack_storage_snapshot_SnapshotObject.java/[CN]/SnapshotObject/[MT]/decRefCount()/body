{
  if (this.store == null) {
    return;
  }
  if (this.store.getRole() == DataStoreRole.Image || this.store.getRole() == DataStoreRole.ImageCache) {
    SnapshotDataStoreVO store=snapshotStoreDao.findByStoreSnapshot(this.store.getRole(),this.store.getId(),this.getId());
    store.decrRefCnt();
    store.setLastUpdated(new Date());
    snapshotStoreDao.update(store.getId(),store);
  }
}
