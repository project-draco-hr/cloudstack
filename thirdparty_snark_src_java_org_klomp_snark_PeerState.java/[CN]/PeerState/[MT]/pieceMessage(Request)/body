{
  int size=req.len;
  downloaded+=size;
  listener.downloaded(peer,size);
  if (getFirstOutstandingRequest(req.piece) == -1) {
    if (listener.gotPiece(peer,req.piece,req.bs)) {
      log.log(Level.FINEST,"Got " + req.piece + ": "+ peer);
    }
 else {
      log.log(Level.FINEST,"Got BAD " + req.piece + " from "+ peer);
      downloaded=0;
    }
  }
}
