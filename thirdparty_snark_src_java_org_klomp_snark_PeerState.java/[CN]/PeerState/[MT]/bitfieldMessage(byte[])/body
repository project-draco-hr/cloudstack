{
synchronized (this) {
    log.log(Level.FINEST,peer + " rcv bitfield");
    if (bitfield != null) {
      log.log(Level.FINER,"Got unexpected bitfield message from " + peer);
      return;
    }
    bitfield=new BitField(bitmap,metainfo.getPieces());
  }
  setInteresting(listener.gotBitField(peer,bitfield));
}
