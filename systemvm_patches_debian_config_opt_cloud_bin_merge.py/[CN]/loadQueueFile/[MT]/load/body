def load(self, data):
    if (data is not None):
        self.data = data
        self.type = self.data['type']
        proc = updateDataBag(self)
        return
    fn = ((self.dpath + '/') + self.fileName)
    try:
        handle = open(fn)
    except IOError:
        logging.error('Could not open %s', fn)
    else:
        self.data = json.load(handle)
        self.type = self.data['type']
        handle.close()
        proc = updateDataBag(self)
