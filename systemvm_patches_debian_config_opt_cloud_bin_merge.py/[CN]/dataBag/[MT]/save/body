def save(self, dbag):
    try:
        handle = open(self.fpath, 'w')
    except IOError:
        logging.error('Could not write data bag %s', self.key)
    else:
        logging.debug('Writing data bag type %s', self.key)
    jsono = json.dumps(dbag, indent=4, sort_keys=True)
    handle.write(jsono)
