{
  RouterDeploymentDefinition deployment1=this.builder.create().setGuestNetwork(this.mockNw).setDeployDestination(this.mockDestination).makeRedundant().build();
  assertTrue("The builder ignored \".makeRedundant()\"",deployment1.isRedundant());
  RouterDeploymentDefinition deployment2=this.builder.create().setGuestNetwork(this.mockNw).setDeployDestination(this.mockDestination).setRedundant(true).build();
  assertTrue("The builder ignored \".setRedundant(true)\"",deployment2.isRedundant());
}
