{
  when(this.mockDataCenter.getNetworkType()).thenReturn(NetworkType.Advanced);
  List<DeployDestination> destinations=this.deployment.findDestinations();
  Assert.assertEquals(ONLY_THE_PROVIDED_AS_DEFAULT_DESTINATION_WAS_EXPECTED,1,destinations.size());
  Assert.assertEquals(ONLY_THE_PROVIDED_AS_DEFAULT_DESTINATION_WAS_EXPECTED,this.mockDestination,destinations.get(0));
}
