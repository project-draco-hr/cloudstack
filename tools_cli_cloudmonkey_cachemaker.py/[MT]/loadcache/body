def loadcache(json_file):
    '\n    Loads json file as dictionary, feeds it to monkeycache and spits result\n    '
    f = open(json_file, 'r')
    data = f.read()
    f.close()
    try:
        apicache = json.loads(data)
    except ValueError as e:
        print 'Error processing json:', json_file, e
        return {}
    return apicache
