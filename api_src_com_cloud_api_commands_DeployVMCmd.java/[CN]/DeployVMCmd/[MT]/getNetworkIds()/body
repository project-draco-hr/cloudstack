{
  if (ipToNetworkList != null) {
    if (networkIds != null) {
      throw new InvalidParameterValueException("NetworkIds can't be specified along with ipToNetworkMap");
    }
 else {
      List<Long> networks=new ArrayList<Long>();
      networks.addAll(getIpToNetworkMap().keySet());
      return networks;
    }
  }
  return networkIds;
}
