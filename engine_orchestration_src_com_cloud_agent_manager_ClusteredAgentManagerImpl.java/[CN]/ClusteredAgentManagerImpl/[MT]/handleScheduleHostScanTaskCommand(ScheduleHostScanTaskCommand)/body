{
  if (logger.isDebugEnabled()) {
    logger.debug("Intercepting resource manager command: " + _gson.toJson(cmd));
  }
  try {
    scheduleHostScanTask();
  }
 catch (  Exception e) {
    logger.warn("Exception happened while trying to schedule host scan task on mgmt server " + _clusterMgr.getSelfPeerName() + ", ignoring as regular host scan happens at fixed interval anyways",e);
    return null;
  }
  Answer[] answers=new Answer[1];
  answers[0]=new Answer(cmd,true,null);
  return _gson.toJson(answers);
}
