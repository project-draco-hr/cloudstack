{
  if (_peers != null) {
    for (    SocketChannel ch : _peers.values()) {
      try {
        s_logger.info("Closing: " + ch.toString());
        ch.close();
      }
 catch (      IOException e) {
        s_logger.info("[ignored] error on closing channel: " + ch.toString(),e);
      }
    }
  }
  _timer.cancel();
  s_transferExecutor.shutdownNow();
  cleanupTransferMap(_nodeId);
  return super.stop();
}
