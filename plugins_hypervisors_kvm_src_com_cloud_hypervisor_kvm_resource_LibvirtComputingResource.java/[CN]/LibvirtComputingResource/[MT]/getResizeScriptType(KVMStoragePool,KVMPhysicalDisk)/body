{
  StoragePoolType poolType=pool.getType();
  PhysicalDiskFormat volFormat=vol.getFormat();
  if (pool.getType() == StoragePoolType.CLVM && volFormat == KVMPhysicalDisk.PhysicalDiskFormat.RAW) {
    return "CLVM";
  }
 else   if ((poolType == StoragePoolType.NetworkFilesystem || poolType == StoragePoolType.SharedMountPoint || poolType == StoragePoolType.Filesystem) && volFormat == KVMPhysicalDisk.PhysicalDiskFormat.QCOW2) {
    return "QCOW2";
  }
  return null;
}
