{
  String path=disk.getDiskPath();
  String poolUuid=null;
  if (path != null) {
    String[] token=path.split("/");
    if (token.length > 3) {
      poolUuid=token[2];
    }
  }
  if (poolUuid == null) {
    return true;
  }
  try {
    KVMStoragePool pool=_storagePoolMgr.getStoragePool(StoragePoolType.Filesystem,poolUuid);
    if (pool != null) {
      _storagePoolMgr.deleteStoragePool(pool.getType(),pool.getUuid());
    }
    return true;
  }
 catch (  CloudRuntimeException e) {
    return false;
  }
}
