{
  if (uuid == null) {
    uuid=UUID.randomUUID().toString();
  }
 else {
    try {
      UUID uuid2=UUID.fromString(uuid);
      String uuid3=uuid2.toString();
      if (!uuid3.equals(uuid)) {
        uuid=UUID.randomUUID().toString();
      }
    }
 catch (    IllegalArgumentException e) {
      uuid=UUID.randomUUID().toString();
    }
  }
  return uuid;
}
