{
  s_logger.debug("Checking if network " + networkName + " exists as openvswitch bridge");
  if (networkName == null) {
    return true;
  }
  Script command=new Script("/bin/sh",_timeout);
  command.add("-c");
  command.add("ovs-vsctl br-exists " + networkName);
  String result=command.execute(null);
  if ("Ok".equals(result)) {
    return true;
  }
 else {
    return false;
  }
}
