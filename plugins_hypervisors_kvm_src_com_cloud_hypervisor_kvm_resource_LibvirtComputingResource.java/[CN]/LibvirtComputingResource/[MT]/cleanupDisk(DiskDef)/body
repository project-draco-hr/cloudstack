{
  String path=disk.getDiskPath();
  if (path == null) {
    s_logger.debug("Unable to clean up disk with null path (perhaps empty cdrom drive):" + disk);
    return false;
  }
  if (path.endsWith("systemvm.iso")) {
    return true;
  }
  return _storagePoolMgr.disconnectPhysicalDiskByPath(path);
}
