{
  try {
    Domain dm=null;
    try {
      dm=conn.domainLookupByName(vmName);
      if (dm != null && dm.isPersistent() == 1) {
        dm.undefine();
      }
    }
 catch (    LibvirtException e) {
    }
 finally {
      if (dm != null) {
        dm.free();
      }
    }
    conn.domainCreateXML(domainXML,0);
  }
 catch (  final LibvirtException e) {
    throw e;
  }
  return null;
}
