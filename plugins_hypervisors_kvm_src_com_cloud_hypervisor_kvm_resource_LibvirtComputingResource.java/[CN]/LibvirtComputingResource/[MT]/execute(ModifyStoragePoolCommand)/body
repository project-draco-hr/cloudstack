{
  String poolType=cmd.getPool().getType().toString();
  KVMStoragePool storagepool=_storagePoolMgr.createStoragePool(cmd.getPool().getUuid(),cmd.getPool().getHost(),cmd.getPool().getPort(),cmd.getPool().getPath(),cmd.getPool().getUserInfo(),cmd.getPool().getType());
  if (storagepool == null) {
    return new Answer(cmd,false," Failed to create storage pool");
  }
  Map<String,TemplateInfo> tInfo=new HashMap<String,TemplateInfo>();
  ModifyStoragePoolAnswer answer=new ModifyStoragePoolAnswer(cmd,storagepool.getCapacity(),storagepool.getUsed(),tInfo);
  return answer;
}
