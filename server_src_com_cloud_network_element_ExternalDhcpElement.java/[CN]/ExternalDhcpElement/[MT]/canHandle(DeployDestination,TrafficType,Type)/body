{
  DataCenter dc=dest.getDataCenter();
  Pod pod=dest.getPod();
  if ((pod != null && pod.getExternalDhcp()) && dc.getNetworkType() == NetworkType.Basic && trafficType == TrafficType.Guest && networkType == Network.Type.Shared) {
    s_logger.debug("External DHCP can handle");
    return true;
  }
  return false;
}
