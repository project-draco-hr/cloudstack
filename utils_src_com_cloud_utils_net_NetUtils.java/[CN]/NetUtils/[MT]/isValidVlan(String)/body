{
  if (null == vlan || "".equals(vlan))   return false;
  if (vlan.startsWith(VLAN_PREFIX))   vlan=vlan.substring(VLAN_PREFIX_LENGTH);
  try {
    int vnet=Integer.parseInt(vlan);
    if (vnet <= 0 || vnet >= 4095) {
      return false;
    }
    return true;
  }
 catch (  NumberFormatException e) {
    return false;
  }
}
