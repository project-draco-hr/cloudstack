{
  if (!isValidIp(netmask)) {
    return false;
  }
  long ip=ip2Long(netmask);
  int count=0;
  boolean finished=false;
  for (int i=31; i >= 0; i--) {
    if (((ip >> i) & 0x1) == 0) {
      finished=true;
    }
 else {
      if (finished) {
        return false;
      }
      count+=1;
    }
  }
  if (count == 0) {
    return false;
  }
  return true;
}
