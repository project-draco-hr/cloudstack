def deletevolume(self, volumeid):
    'Delete a Volume attached to a VM\n        '
    cmd = deleteVolume.deleteVolumeCmd()
    cmd.id = volumeid
    self.apiclient.deleteVolume(cmd)
    list_volume_response = Volume.list(self.apiclient, id=volumeid, type='DATADISK')
    self.debug(list_volume_response)
    self.assertEqual(isinstance(list_volume_response, list), True, 'Check VOLUME is deleted from list Volumes')
    self.assertEqual(list_volume_response, None, 'Check VOLUME is deleted from list Volumes')
    return
