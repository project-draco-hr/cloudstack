{
  if (null == cloudState)   return "terminated";
  if (cloudState.equalsIgnoreCase("Destroyed"))   return "terminated";
 else   if (cloudState.equalsIgnoreCase("Stopped"))   return "stopped";
 else   if (cloudState.equalsIgnoreCase("Running"))   return "running";
 else   if (cloudState.equalsIgnoreCase("Starting"))   return "pending";
 else   if (cloudState.equalsIgnoreCase("Stopping"))   return "stopping";
 else   if (cloudState.equalsIgnoreCase("Error"))   return "error";
 else   if (cloudState.equalsIgnoreCase("Expunging"))   return "terminated";
 else   return "running";
}
