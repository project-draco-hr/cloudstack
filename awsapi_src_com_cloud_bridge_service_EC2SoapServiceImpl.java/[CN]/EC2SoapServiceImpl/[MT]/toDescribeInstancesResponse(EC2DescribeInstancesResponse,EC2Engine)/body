    if (null == groups || 0 == groups.length) {
      GroupItemType param5=new GroupItemType();
      param5.setGroupId("");
      param4.addItem(param5);
    }
 else {
      for (      EC2SecurityGroup group : groups) {
        GroupItemType param5=new GroupItemType();
