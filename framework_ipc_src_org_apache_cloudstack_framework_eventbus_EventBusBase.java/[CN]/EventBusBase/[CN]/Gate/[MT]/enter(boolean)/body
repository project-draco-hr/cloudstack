{
  while (true) {
synchronized (this) {
      if (_reentranceCount == 0) {
        assert(_gateOwner == null);
        _reentranceCount++;
        _gateOwner=Thread.currentThread();
        return true;
      }
 else {
        if (wait) {
          try {
            wait();
          }
 catch (          InterruptedException e) {
          }
        }
 else {
          break;
        }
      }
    }
  }
  return false;
}
