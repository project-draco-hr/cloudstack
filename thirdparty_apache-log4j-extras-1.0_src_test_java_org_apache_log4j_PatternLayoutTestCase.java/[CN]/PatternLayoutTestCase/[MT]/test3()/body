{
  configure("input/pattern/patternLayout3.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT1,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new LineNumberFilter(),new ISO8601Filter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/patternLayout.3"));
}
