{
  configure("input/pattern/patternLayout12.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT12,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new LineNumberFilter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/patternLayout.12"));
}
