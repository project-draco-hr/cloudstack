def __verify_values(self, expected_vals, actual_vals):
    '  \n        @summary: Function to verify expected and actual values\n        Step1: Initializing return flag to True\n        Step1: Verifying length of expected and actual dictionaries is matching.\n               If not matching returning false\n        Step2: Listing all the keys from expected dictionary\n        Step3: Looping through each key from step2 and verifying expected and actual dictionaries have same value\n               If not making return flag to False\n        Step4: returning the return flag after all the values are verified\n        '
    return_flag = True
    if (len(expected_vals) != len(actual_vals)):
        return False
    keys = expected_vals.keys()
    for i in range(0, len(expected_vals)):
        exp_val = expected_vals[keys[i]]
        act_val = actual_vals[keys[i]]
        if (exp_val == act_val):
            return_flag = (return_flag and True)
        else:
            return_flag = (return_flag and False)
            self.debug(('expected Value: %s, is not matching with actual value: %s' % (exp_val, act_val)))
    return return_flag
