{
  LoggingEvent event=new LoggingEvent("org.apache.log4j.Logger",Logger.getLogger(StringMatchFilterTest.class),System.currentTimeMillis(),Level.INFO,"Hello, World",null);
  StringMatchFilter filter=new StringMatchFilter();
  filter.setStringToMatch("World");
  filter.activateOptions();
  assertEquals(Filter.ACCEPT,filter.decide(event));
}
