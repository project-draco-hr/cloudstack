def isAdminContext(self):
    '\n        @Name : isAdminContext\n        @Desc:A user is a regular user if he fails to listDomains;\n        if he is a domain-admin, he can list only domains that are non-ROOT;\n        if he is an admin, he can list the ROOT domain successfully\n        '
    try:
        listdom = listDomains.listDomainsCmd()
        listdom.name = 'ROOT'
        listdomres = self.__apiClient.listDomains(listdom)
        if (listdomres != FAILED):
            rootdom = listdomres[0].name
            if (rootdom == 'ROOT'):
                return ADMIN
            else:
                return DOMAIN_ADMIN
        return USER
    except:
        return USER
