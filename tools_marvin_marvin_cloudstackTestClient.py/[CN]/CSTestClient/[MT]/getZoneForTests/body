def getZoneForTests(self):
    '\n        @Name : getZoneForTests\n        @Desc : Provides the Zone against which Tests are to run\n                If zone name provided to marvin plugin is none\n                it will get it from Test Data Config File\n                Even, if  it is not available, return None\n        @Output : Returns the Zone Name\n        '
    if (self.__zone is None):
        if self.__parsedTestDataConfig:
            ret = self.__parsedTestDataConfig.get('zone')
            if (ret != 'NA'):
                self.__zone = ret
    return self.__zone
