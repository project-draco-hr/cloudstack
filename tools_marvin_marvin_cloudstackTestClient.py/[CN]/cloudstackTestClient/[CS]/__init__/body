def __init__(self, mgmtDetails, dbSvrDetails, asyncTimeout=3600, defaultWorkerThreads=10, logging=None):
    self.mgmtDetails = mgmtDetails
    self.connection = cloudstackConnection.cloudConnection(self.mgmtDetails, asyncTimeout, logging)
    self.apiClient = cloudstackAPIClient.CloudStackAPIClient(self.connection)
    self.dbConnection = None
    if (dbSvrDetails is not None):
        self.createDbConnection(dbSvrDetails.dbSvr, dbSvrDetails.port, dbSvrDetails.user, dbSvrDetails.passwd, dbSvrDetails.db)
    '\n        Provides the Configuration Object to users through getConfigParser\n        The purpose of this object is to parse the config\n        and provide dictionary of the config so users can\n        use that configuration.Users can later call getConfig\n        on this object and it will return the default parsed\n        config dictionary from default configuration file,\n        they can overwrite it with providing their own\n        configuration file as well.\n        '
    self.configObj = ConfigManager()
    self.asyncJobMgr = None
    self.id = None
    self.defaultWorkerThreads = defaultWorkerThreads
