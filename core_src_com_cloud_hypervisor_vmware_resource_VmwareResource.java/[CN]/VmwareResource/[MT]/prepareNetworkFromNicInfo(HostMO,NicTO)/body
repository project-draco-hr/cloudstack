{
  String switchName=getTargetSwitch(nicTo);
  String namePrefix=getNetworkNamePrefix(nicTo);
  s_logger.info("Prepare network on vSwitch: " + switchName + " with name prefix: "+ namePrefix);
  return HypervisorHostHelper.prepareNetwork(switchName,namePrefix,hostMo,getVlanInfo(nicTo),nicTo.getNetworkRateMbps(),nicTo.getNetworkRateMulticastMbps(),_ops_timeout,!namePrefix.startsWith("cloud.private"));
}
