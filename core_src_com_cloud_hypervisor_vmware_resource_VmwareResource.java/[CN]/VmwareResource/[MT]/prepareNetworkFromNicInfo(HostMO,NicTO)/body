{
  Pair<String,String> switchName=getTargetSwitch(nicTo);
  String namePrefix=getNetworkNamePrefix(nicTo);
  s_logger.info("Prepare network on vSwitch: " + switchName.first() + " with name prefix: "+ namePrefix);
  return HypervisorHostHelper.prepareNetwork(switchName.first(),namePrefix,hostMo,getVlanInfo(nicTo,switchName.second()),nicTo.getNetworkRateMbps(),nicTo.getNetworkRateMulticastMbps(),_ops_timeout,!namePrefix.startsWith("cloud.private"));
}
