{
  if (cursor + 4 > length)   throw new ArrayIndexOutOfBoundsException("Cannot read 4 bytes from this buffer: " + this + ".");
  long result=(((long)(data[offset + cursor + 0] & 0xff) << 24) + ((long)(data[offset + cursor + 1] & 0xff) << 16) + ((long)(data[offset + cursor + 2] & 0xff) << 8)+ (data[offset + cursor + 3] & 0xff));
  cursor+=4;
  return result;
}
