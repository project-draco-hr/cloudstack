{
  _todos=new ArrayList<ChangeRequest>();
  _thread=new Thread(this,_name + "-Selector");
  _isRunning=true;
  _thread.start();
synchronized (_thread) {
    try {
      _thread.wait();
    }
 catch (    InterruptedException e) {
      if (s_logger.isTraceEnabled()) {
        s_logger.info("Interrupted start thread ",e);
      }
    }
  }
}
