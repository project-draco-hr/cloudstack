{
  if (key != null) {
    SocketChannel channel=(SocketChannel)key.channel();
    key.cancel();
    try {
      if (channel != null) {
        if (s_logger.isTraceEnabled()) {
          s_logger.trace("Closing socket " + channel.socket());
        }
        channel.close();
      }
    }
 catch (    IOException ignore) {
    }
  }
}
