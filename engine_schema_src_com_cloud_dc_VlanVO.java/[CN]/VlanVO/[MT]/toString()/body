{
  if (toString == null) {
    toString=new StringBuilder("Vlan[").append(vlanTag).append("|").append(vlanGateway).append("|").append(vlanNetmask).append("|").append(ip6Gateway).append("|").append(ip6Cidr).append("|").append(ipRange).append("|").append("|").append(ip6Range).append(networkId).append("]").toString();
  }
  return toString;
}
