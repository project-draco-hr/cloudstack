@attr(tags=['advanced', 'selfservice'])
def test_delete_portable_ip_range_non_root_admin(self):
    'Test delete ip range - non admin root\n        '
    self.account = Account.create(self.apiclient, self.services['account'], domainid=self.domain.id)
    self.cleanup.append(self.account)
    self.api_client_user = self.testClient.getUserApiClient(UserName=self.account.name, DomainName=self.account.domain)
    with self.assertRaises(Exception):
        self.portable_ip_range.delete(self.api_client_user)
    self.portable_ip_range.delete(self.apiclient)
    return
