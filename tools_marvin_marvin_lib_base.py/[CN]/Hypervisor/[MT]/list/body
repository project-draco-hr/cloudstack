@classmethod
def list(cls, apiclient, **kwargs):
    'Lists hypervisors'
    cmd = listHypervisors.listHypervisorsCmd()
    [setattr(cmd, k, v) for (k, v) in kwargs.items()]
    if (('account' in kwargs.keys()) and ('domainid' in kwargs.keys())):
        cmd.listall = True
    return apiclient.listHypervisors(cmd)
