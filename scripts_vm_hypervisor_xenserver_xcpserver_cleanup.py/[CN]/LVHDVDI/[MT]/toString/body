def toString(self):
    strType = 'VHD'
    if self.raw:
        strType = 'RAW'
    strHidden = ''
    if self.hidden:
        strHidden = '*'
    strSizeVHD = ''
    if (self._sizeVHD > 0):
        strSizeVHD = Util.num2str(self._sizeVHD)
    strActive = 'n'
    if self.lvActive:
        strActive = 'a'
    return ('%s%s[%s](%s/%s/%s|%s)' % (strHidden, self.uuid[0:8], strType, Util.num2str(self.sizeVirt), strSizeVHD, Util.num2str(self.sizeLV), strActive))
