def __init__(self, sr, uuid):
    self.sr = sr
    self.scanError = True
    self.uuid = uuid
    self.parentUuid = ''
    self.sizeVirt = (-1)
    self._sizeVHD = (-1)
    self.hidden = False
    self.parent = None
    self.children = []
    self._vdiRef = None
    self._config = {}
    self._configDirty = {}
    self._clearRef()
