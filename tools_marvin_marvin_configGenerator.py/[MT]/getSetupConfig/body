def getSetupConfig(file):
    config = cloudstackConfiguration()
    configLines = []
    with open(file, 'r') as fp:
        for line in fp:
            ws = line.strip()
            if (not ws.startswith('#')):
                configLines.append(ws)
    config = json.loads('\n'.join(configLines))
    return jsonHelper.jsonLoader(config)
