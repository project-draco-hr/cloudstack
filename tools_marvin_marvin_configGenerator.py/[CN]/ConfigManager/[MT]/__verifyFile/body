def __verifyFile(self, file):
    '\n        @Name : __parseConfig\n        @Description: Parses the Input configuration Json file\n                  and returns a dictionary from the file.\n        @Input      : file NA\n        @Output     : True or False based upon file input validity\n                      and availability\n        '
    if ((file is None) or (file == '')):
        return False
    if (os.path.exists(file) is False):
        return False
    return True
