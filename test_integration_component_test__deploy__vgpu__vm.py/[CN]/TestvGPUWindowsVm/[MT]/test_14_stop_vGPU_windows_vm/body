@attr(tags=['advanced', 'basic', 'vgpu'], required_hardware='true')
def test_14_stop_vGPU_windows_vm(self):
    ' Test stop virtual machine\n        '
    if self.vm_k1_card:
        self.vm_k1_card.stop(self.apiclient)
    if self.vm_k2_card:
        self.vm_k2_card.stop(self.apiclient)
    return
