def test_19_destroy_vGPU_windows_vm_after_recover(self):
    'Test destroy Virtual Machine\n        '
    if self.vm_k1_card:
        self.vm_k1_card.delete(self.apiclient)
        self.vm_k1_card.getState(self.apiclient, 'Destroyed')
    if self.vm_k2_card:
        self.vm_k2_card.delete(self.apiclient)
        self.vm_k2_card.getState(self.apiclient, 'Destroyed')
    return
