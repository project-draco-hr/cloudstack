{
  _ver=ver;
  _cmds=cmds;
  _stopOnError=stopOnError;
  if (inSequence != null) {
    _inSequence=inSequence;
  }
 else {
    for (    final Command cmd : cmds) {
      if (cmd.executeInSequence()) {
        _inSequence=true;
        break;
      }
    }
  }
  _seq=seq;
  _agentId=agentId;
  _mgmtId=mgmtId;
  _fromServer=fromServer;
}
