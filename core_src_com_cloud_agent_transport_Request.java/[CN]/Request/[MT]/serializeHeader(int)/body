{
  final ByteBuffer buffer=ByteBuffer.allocate(40);
  buffer.put(getVersionInByte());
  buffer.put((byte)0);
  buffer.putShort(getFlags());
  buffer.putLong(_seq);
  buffer.putInt(contentSize);
  buffer.putLong(_mgmtId);
  buffer.putLong(_via);
  buffer.putLong(_agentId);
  buffer.flip();
  return buffer;
}
