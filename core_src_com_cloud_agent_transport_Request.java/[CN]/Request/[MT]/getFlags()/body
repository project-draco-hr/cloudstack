{
  short flags=0;
  if (!(this instanceof Response)) {
    flags=FLAG_REQUEST;
  }
 else {
    flags=FLAG_RESPONSE;
  }
  if (_inSequence) {
    flags=(short)(flags | FLAG_IN_SEQUENCE);
  }
  if (_stopOnError) {
    flags=(short)(flags | FLAG_STOP_ON_ERROR);
  }
  if (_fromServer) {
    flags=(short)(flags | FLAG_FROM_SERVER);
  }
  if (_control) {
    flags=(short)(flags | FLAG_CONTROL);
  }
  return flags;
}
