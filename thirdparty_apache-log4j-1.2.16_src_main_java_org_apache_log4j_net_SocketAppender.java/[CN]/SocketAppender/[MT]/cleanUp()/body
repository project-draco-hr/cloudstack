{
  if (oos != null) {
    try {
      oos.close();
    }
 catch (    IOException e) {
      if (e instanceof InterruptedIOException) {
        Thread.currentThread().interrupt();
      }
      LogLog.error("Could not close oos.",e);
    }
    oos=null;
  }
  if (connector != null) {
    connector.interrupted=true;
    connector=null;
  }
}
