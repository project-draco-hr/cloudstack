def __init__(self):
    'the ID of the dedicated resource'
    self.id = None
    'the Account Id to which the Pod is dedicated'
    self.accountid = None
    'the Dedication Affinity Group ID of the pod'
    self.affinitygroupid = None
    'the domain ID to which the Pod is dedicated'
    self.domainid = None
    'the ID of the Pod'
    self.podid = None
    'the Name of the Pod'
    self.podname = None
