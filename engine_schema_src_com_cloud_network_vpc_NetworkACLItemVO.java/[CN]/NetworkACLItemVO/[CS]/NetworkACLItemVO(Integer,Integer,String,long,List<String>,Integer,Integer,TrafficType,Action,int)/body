{
  sourcePortStart=portStart;
  sourcePortEnd=portEnd;
  this.protocol=protocol;
  this.aclId=aclId;
  state=State.Staged;
  this.icmpCode=icmpCode;
  this.icmpType=icmpType;
  setSourceCidrList(sourceCidrs);
