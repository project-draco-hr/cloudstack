{
  return new Configuration().name(configuration.get(NAME)).guid(configuration.get(GUID)).zoneId(configuration.get(ZONE_ID)).hostName(configuration.get(HOST_NAME)).cmsUser(configuration.get(CMS_USER)).cmsUserPassword(configuration.get(CMS_USER_PASSWORD)).port(configuration.get(PORT)).apiVersion(configuration.get(API_VERSION)).apiRelativePath(configuration.get(API_RELATIVE_PATH)).retryCount(configuration.get(RETRY_COUNT)).retryInterval(configuration.get(RETRY_INTERVAL)).nuageVspCmsId(configuration.get(NUAGE_VSP_CMS_ID));
}
