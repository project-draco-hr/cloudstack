{
  try {
    isNuageVspGuruLoaded();
    _nuageVspGuruClient.deallocate(cmd.getNetworkUuid(),cmd.getNicFrmDdUuid(),cmd.getNicMacAddress(),cmd.getNicIp4Address(),cmd.isL3Network(),cmd.getVpcUuid(),cmd.getNetworksDomainUuid(),cmd.getVmInstanceName(),cmd.getVmUuid());
    return new DeallocateVmVspAnswer(cmd,true,"Deallocated VM from Nuage VSP.");
  }
 catch (  Exception e) {
    return new DeallocateVmVspAnswer(cmd,e);
  }
}
