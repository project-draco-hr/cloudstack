{
  try {
    isNuageVspGuruLoaded();
    _nuageVspGuruClient.deallocate(cmd.getNetworkUuid(),cmd.getNicFromDdUuid(),cmd.getNicMacAddress(),cmd.getNicIp4Address(),cmd.isL3Network(),cmd.isSharedNetwork(),cmd.getVpcUuid(),cmd.getNetworksDomainUuid(),cmd.getVmInstanceName(),cmd.getVmUuid(),cmd.isExpungingState());
    return new Answer(cmd,true,"Deallocated VM " + cmd.getVmInstanceName() + " on Nuage VSD "+ _hostName);
  }
 catch (  ExecutionException|ConfigurationException e) {
    s_logger.error("Failure during " + cmd + " on Nuage VSD "+ _hostName,e);
    return new Answer(cmd,e);
  }
}
