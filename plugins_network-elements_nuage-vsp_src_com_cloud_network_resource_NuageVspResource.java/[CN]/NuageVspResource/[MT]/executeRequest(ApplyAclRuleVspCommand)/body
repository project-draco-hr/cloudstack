{
  try {
    isNuageVspElementLoaded();
    _nuageVspElementClient.applyAclRules(cmd.isNetworkAcl(),cmd.getNetworkUuid(),cmd.getNetworkDomainUuid(),cmd.getVpcOrSubnetUuid(),cmd.getNetworkName(),cmd.isL2Network(),cmd.getAclRules(),cmd.getNetworkId(),cmd.isEgressDefaultPolicy(),cmd.getAcsIngressAcl(),cmd.isNetworkReset(),cmd.getDomainTemplateName());
    return new Answer(cmd,true,"Applied ACL Rule to network mapping " + cmd.getVpcOrSubnetUuid() + " on Nuage VSD "+ _hostName);
  }
 catch (  ExecutionException|ConfigurationException e) {
    s_logger.error("Failure during " + cmd + " on Nuage VSD "+ _hostName,e);
    return new Answer(cmd,e);
  }
}
