{
  try {
    isNuageVspElementLoaded();
    _nuageVspElementClient.applyAclRules(cmd.getNetworkUuid(),cmd.getNetworkDomainUuid(),cmd.getVpcOrSubnetUuid(),cmd.isL3Network(),cmd.getAclRules(),cmd.isVpc(),cmd.getNetworkId());
    return new ApplyAclRuleVspAnswer(cmd,true,"Applied ACL Rule to VSP network mapping to " + cmd.getVpcOrSubnetUuid());
  }
 catch (  Exception e) {
    return new ApplyAclRuleVspAnswer(cmd,e);
  }
}
