{
  Map<VirtualMachineProfile.Param,Object> params=new HashMap<VirtualMachineProfile.Param,Object>(1);
  params.put(VirtualMachineProfile.Param.ReProgramGuestNetworks,true);
  RouterDeploymentDefinition routerDeploymentDefinition=new RouterDeploymentDefinition(vpc,dest,_accountMgr.getAccount(vpc.getAccountId()),params,false);
  _vpcRouterMgr.deployVirtualRouter(routerDeploymentDefinition);
  return true;
}
