{
  assert(context.getPool() == this);
  assert(context.getPoolKey() != null);
synchronized (this) {
    List<VmwareContext> l=_pool.get(context.getPoolKey());
    if (l == null) {
      l=new ArrayList<VmwareContext>();
      _pool.put(context.getPoolKey(),l);
    }
    if (l.size() < _maxIdleQueueLength) {
      context.clearStockObjects();
      l.add(context);
      if (s_logger.isInfoEnabled())       s_logger.info("Recycle VmwareContext into idle pool: " + context.getPoolKey() + ", current idle pool size: "+ l.size()+ ", outstanding count: "+ VmwareContext.getOutstandingContextCount());
    }
 else {
      if (s_logger.isInfoEnabled())       s_logger.info("VmwareContextPool queue exceeds limits, queue size: " + l.size());
      context.close();
    }
  }
}
