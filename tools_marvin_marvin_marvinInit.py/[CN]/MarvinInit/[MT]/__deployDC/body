def __deployDC(self):
    try:
        '\n            Deploy the DataCenter and retrieves test client.\n            '
        deploy_obj = deployDataCenters(self.__parsedConfig, self.__tcRunLogger)
        if self.__loadFlag:
            deploy_obj.loadCfg()
        else:
            deploy_obj.deploy()
        self.__testClient = deploy_obj.testClient
        return SUCCESS
    except Exception as e:
        print ('\n Exception Occurred Under __deployDC : %s' % str(e))
        return None
