def getResultFile(self):
    '\n        @Name : getDebugFile\n        @Desc : Creates the result file at a given path.\n        @Output : Returns the Result file to be used for writing\n                test outputs\n        '
    if (self.__logFolderPath is not None):
        self.__tcResultFile = open((self.__logFolderPath + '/results.txt'), 'w')
        return self.__tcResultFile
