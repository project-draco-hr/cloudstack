def init(self):
    '\n        @Desc :Initializes the marvin by\n               1. Parsing the configuration and creating a parsed config\n                  structure\n               2. Creates a timestamped log folder and provides all logs\n                  to be dumped there\n               3. Creates the DataCenter based upon configuration provided\n        '
    try:
        if ((self.__parseConfig() is not None) and (self.__initLogging() is not None) and (self.__deployDC() is not None)):
            return SUCCESS
        else:
            return FAILED
    except Exception as e:
        print ('\n Exception Occurred Under init %s' % str(e))
        return FAILED
