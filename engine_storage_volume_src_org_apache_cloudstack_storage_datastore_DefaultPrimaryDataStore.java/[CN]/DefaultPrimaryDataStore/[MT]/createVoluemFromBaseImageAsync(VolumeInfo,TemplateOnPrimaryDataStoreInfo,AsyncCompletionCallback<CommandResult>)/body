{
  VolumeObject vo=(VolumeObject)volume;
  vo.setVolumeDiskType(templateStore.getTemplate().getDiskType());
  AsyncCallbackDispatcher caller=new AsyncCallbackDispatcher(this).setParentCallback(callback).setOperationName("primarydatastore.createvolumefrombaseImage");
  this.driver.createVolumeFromBaseImageAsync(vo,templateStore,caller);
}
