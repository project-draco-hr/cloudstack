{
  LinkedHashMap<Network,List<? extends NicProfile>> networks=createVpcRouterNetworks(routerDeploymentDefinition,new Pair<Boolean,PublicIp>(true,sourceNatIp),routerDeploymentDefinition.getVpc().getId());
  DomainRouterVO router=nwHelper.deployRouter(routerDeploymentDefinition,vrProvider,svcOffId,networks,true,vpcMgr.getSupportedVpcHypervisors());
  return router;
}
