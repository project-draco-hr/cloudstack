{
  IPAddressVO addr=_ipAddressDao.findById(publicIp);
  List<UserVmVO> vms=_userVmDao.listVmsUsingGuestIpAddress(addr.getDataCenterId(),guestIp);
  if (vms != null) {
    for (    UserVmVO vm : vms) {
      if (vm.getAccountId() == addr.getAccountId()) {
        return vm;
      }
    }
  }
  return null;
}
