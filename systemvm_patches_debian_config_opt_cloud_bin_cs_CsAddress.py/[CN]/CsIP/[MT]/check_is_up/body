def check_is_up(self):
    ' Ensure device is up '
    cmd = ("ip link show %s | grep 'state DOWN'" % self.getDevice())
    for i in CsHelper.execute(cmd):
        if (' DOWN ' in i):
            cmd2 = ('ip link set %s up' % self.getDevice())
            if (self.cl.is_master() or (not self.needs_vrrp())):
                CsHelper.execute(cmd2)
