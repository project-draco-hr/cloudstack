{
  IpAddress ip=_entityMgr.findById(IpAddress.class,getPublicIpId());
  Long ntwkId=null;
  if (ip.getAssociatedWithNetworkId() != null) {
    ntwkId=ip.getAssociatedWithNetworkId();
  }
  if (ntwkId == null) {
    throw new InvalidParameterValueException("Unable to create remote access vpn for the ipAddress id=" + getPublicIpId() + " as ip is not associated with any network and no networkId is passed in");
  }
  return ntwkId;
}
