{
  String swiftCli=getSwiftCLIPath();
  StringBuilder cms=new StringBuilder();
  for (  Map.Entry<String,String> entry : metas.entrySet()) {
    cms.append(" -m ");
    cms.append(entry.getKey());
    cms.append(":");
    cms.append(entry.getValue());
    cms.append(" ");
  }
  Script command=new Script("/bin/bash",logger);
  command.add("-c");
  command.add("/usr/bin/python " + swiftCli + " -A "+ cfg.getEndPoint()+ " -U "+ cfg.getAccount()+ ":"+ cfg.getUserName()+ " -K "+ cfg.getKey()+ " post "+ container+ " "+ object+ " "+ cms.toString());
  OutputInterpreter.OneLineParser parser=new OutputInterpreter.OneLineParser();
  String result=command.execute(parser);
  if (result != null) {
    throw new CloudRuntimeException("Failed to post meta" + result);
  }
  return true;
}
