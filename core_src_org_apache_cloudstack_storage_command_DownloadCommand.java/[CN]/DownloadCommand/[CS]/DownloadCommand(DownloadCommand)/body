{
  super(that);
  hvm=that.hvm;
  checksum=that.checksum;
  id=that.id;
  description=that.description;
  auth=that.getAuth();
  setSecUrl(that.getSecUrl());
  maxDownloadSizeInBytes=that.getMaxDownloadSizeInBytes();
  resourceType=that.resourceType;
  installPath=that.installPath;
  _store=that._store;
}
