{
  if (dc.getNetworkType() == NetworkType.Advanced && isMyTrafficType(offering.getTrafficType()) && offering.getGuestType() == Network.GuestType.Isolated && offering.isSystemOnly()) {
    return true;
  }
 else {
    logger.trace("We only take care of system Guest networks of type   " + GuestType.Isolated + " in zone of type "+ NetworkType.Advanced);
    return false;
  }
}
