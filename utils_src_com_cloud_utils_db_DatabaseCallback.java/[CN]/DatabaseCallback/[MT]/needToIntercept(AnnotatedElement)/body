{
  DB db=element.getAnnotation(DB.class);
  if (db != null) {
    return db.txn();
  }
  Class<?> clazz=element.getClass().getDeclaringClass();
  do {
    db=clazz.getAnnotation(DB.class);
    if (db != null) {
      return db.txn();
    }
    clazz=clazz.getSuperclass();
  }
 while (clazz != Object.class && clazz != null);
  return false;
}
