def create_network_acl_rule(self, rule, traffic_type='Ingress', network=None, acl_list=None):
    self.debug(('Adding NetworkACL rule: %s' % rule))
    return NetworkACL.create(self.api_client, networkid=(network.id if network else None), services=rule, traffictype=traffic_type, aclid=(acl_list.id if acl_list else None))
