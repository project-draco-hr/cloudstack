{
  SearchCriteria<PodVlanVO> sc=VlanPodSearch.create();
  sc.setParameters("vlan",vlan);
  sc.setParameters("podId",podId);
  sc.setParameters("account",accountId);
  PodVlanVO vo=findOneBy(sc);
  if (vo == null) {
    return;
  }
  vo.setTakenAt(null);
  vo.setAccountId(null);
  update(vo.getId(),vo);
}
