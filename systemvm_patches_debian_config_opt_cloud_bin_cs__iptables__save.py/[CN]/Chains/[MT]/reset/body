def reset(self):
    '\n        name is one of filter, nat, raw, mangle,\n        tables is a list of tables in that table-class\n        '
    self.poli = {}
    self.length = 0
    self.policy = '-'
    for tabular in self.tables:
        self.data[tabular] = []
        self.poli[tabular] = 'ACCEPT'
