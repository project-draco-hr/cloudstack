def put_into_tables(self, line):
    'put line into matching Chains-object'
    liste = line.split()
    liste.pop(0)
    rest = ''
    for elem in liste:
        if ('>' not in elem):
            rest = ((rest + elem) + ' ')
    action = liste.pop(0)
    fam = 'filter'
    if ('-t nat' in line):
        fam = 'nat'
    elif ('-t mangle' in line):
        fam = 'mangle'
    elif ('-t raw' in line):
        fam = 'raw'
    fam_dict = self.data[fam]
    fam_dict.put_into_fgr(rest)
