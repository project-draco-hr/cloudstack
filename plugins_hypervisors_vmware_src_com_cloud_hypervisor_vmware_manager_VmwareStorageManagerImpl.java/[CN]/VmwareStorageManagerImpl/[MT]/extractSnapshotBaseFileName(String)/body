{
  if (input == null)   return null;
  String result=input;
  if (result.endsWith(".vmdk")) {
    result=result.substring(0,result.length() - (".vmdk").length());
  }
  if (result.split("-").length == 1)   return result;
  if (result.split("-").length > 2)   return result.split("-")[0] + "-" + result.split("-")[1];
  if (result.split("-").length == 2)   return result.split("-")[0];
 else   return result;
}
