{
  if (input == null) {
    return null;
  }
  String result=input;
  final String fileType=".vmdk";
  if (result.endsWith(fileType)) {
    result=result.substring(0,result.length() - (fileType).length());
  }
  String[] str=result.split("-");
  int length=str.length;
  if (length == 1 || length == 2) {
    return result;
  }
  if (length > 2) {
    return str[0] + "-" + str[1];
  }
  return result;
}
