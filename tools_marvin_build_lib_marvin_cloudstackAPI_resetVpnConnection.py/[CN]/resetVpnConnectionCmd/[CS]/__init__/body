def __init__(self):
    self.isAsync = 'true'
    'id of vpn connection'
    'Required'
    self.id = None
    'an optional account for connection. Must be used with domainId.'
    self.account = None
    'an optional domainId for connection. If the account parameter is used, domainId must also be used.'
    self.domainid = None
    self.required = ['id']
