{
  NetworkOfferingVO voToReturn=vo;
  Class<?> c=voToReturn.getClass();
  try {
    Field f=c.getDeclaredField("id");
    f.setAccessible(true);
    f.setLong(voToReturn,id);
  }
 catch (  NoSuchFieldException ex) {
    logger.warn(ex);
    return null;
  }
catch (  IllegalAccessException ex) {
    logger.warn(ex);
    return null;
  }
  return voToReturn;
}
