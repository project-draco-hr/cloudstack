{
  CreateBaseImageContext<CreateCmdResult> context=new CreateBaseImageContext<CreateCmdResult>(null,volume,dataStore,template,future);
  AsyncCallbackDispatcher<VolumeServiceImpl,CreateCmdResult> caller=AsyncCallbackDispatcher.create(this);
  caller.setCallback(caller.getTarget().copyBaseImageCallback(null,null)).setContext(context);
  DataObject templateOnPrimaryStoreObj=dataObjectMgr.createInternalStateOnly(template,dataStore);
  dataObjectMgr.copyAsync(context.srcTemplate,templateOnPrimaryStoreObj,caller);
  return;
}
