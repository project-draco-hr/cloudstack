{
  VolumeObject vo=(VolumeObject)volume;
  try {
    vo.stateTransit(Volume.Event.CreateRequested);
  }
 catch (  Exception e) {
    throw new CloudRuntimeException(e.toString());
  }
  AsyncCallbackDispatcher caller=new AsyncCallbackDispatcher(this).setParentCallback(callback).setOperationName("volumeservice.createVolumeFromBaseImageCallback").setContextParam("volume",vo);
  pd.createVoluemFromBaseImageAsync(volume,templateOnPrimaryStore,caller);
}
