def equals(self, rule):
    if (rule.get_table() != self.get_table()):
        return False
    if (rule.get_chain() != self.get_chain()):
        return False
    for r in rule.get_rule():
        if (not (r in self.get_rule().keys())):
            return False
        if (rule.get_rule()[r] != self.get_rule()[r]):
            return False
    return True
