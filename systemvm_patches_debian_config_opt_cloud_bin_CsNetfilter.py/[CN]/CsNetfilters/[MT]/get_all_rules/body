def get_all_rules(self):
    for i in CsHelper.execute('iptables-save'):
        if i.startswith('*'):
            self.table.add(i[1:])
        if i.startswith(':'):
            self.chain.add(self.table.last(), i[1:].split(' ')[0])
        if i.startswith('-A'):
            rule = CsNetfilter()
            rule.parse(i)
            rule.set_table(self.table.last())
            self.save(rule)
