{
  if (acl.getSourceCidrList() == null && (acl.getPurpose() == Purpose.Firewall || acl.getPurpose() == Purpose.NetworkACL)) {
    _firewallDao.loadSourceCidrs((FirewallRuleVO)acl);
  }
  return createNetworkACL(UserContext.current().getCaller(),acl.getXid(),acl.getSourcePortStart(),acl.getSourcePortEnd(),acl.getProtocol(),acl.getSourceCidrList(),acl.getIcmpCode(),acl.getIcmpType(),null,acl.getType(),acl.getNetworkId(),acl.getTrafficType());
}
