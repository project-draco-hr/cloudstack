def __init__(self, obj):
    for k in obj:
        v = obj[k]
        if isinstance(v, dict):
            setattr(self, k, jsonLoader(v))
        elif isinstance(v, (list, tuple)):
            if isinstance(v[0], dict):
                setattr(self, k, [jsonLoader(elem) for elem in v])
            else:
                setattr(self, k, v)
        else:
            setattr(self, k, v)
