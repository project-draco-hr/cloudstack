{
  try {
    String[] feature=new String[1];
    feature[0]="LB";
    nsService.enable_features(feature);
  }
 catch (  nitro_exception e) {
    throw new ExecutionException("Enabling netscaler load balancing feature failed due to " + e.getMessage());
  }
catch (  Exception e) {
    throw new ExecutionException("Enabling netscaler load balancing feature failed due to " + e.getMessage());
  }
}
