{
  Script s=new Script("/bin/bash",timeout);
  s.add("-c");
  s.add(command);
  String result=s.execute(null);
  if (result == null || result.trim().isEmpty())   return -1;
 else {
    try {
      return Integer.valueOf(result.trim());
    }
 catch (    NumberFormatException e) {
      return -1;
    }
  }
}
