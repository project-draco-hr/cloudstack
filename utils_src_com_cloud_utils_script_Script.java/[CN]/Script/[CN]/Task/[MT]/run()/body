{
  done=false;
  try {
    result=interpreter.interpret(reader);
  }
 catch (  IOException ex) {
    result=stackTraceAsString(ex);
  }
catch (  Exception ex) {
    result=stackTraceAsString(ex);
  }
 finally {
synchronized (this) {
      done=true;
      notifyAll();
    }
    IOUtils.closeQuietly(reader);
  }
}
