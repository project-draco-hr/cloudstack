{
  assertTrue("Not really a VpcRouterDeploymentDefinition what the builder created",this.deployment instanceof VpcRouterDeploymentDefinition);
  assertTrue("A VpcRouterDeploymentDefinition should declare it is",this.deployment.isVpcRouter());
  assertEquals("A VpcRouterDeploymentDefinition should have a Vpc",this.mockVpc,this.deployment.getVpc());
}
