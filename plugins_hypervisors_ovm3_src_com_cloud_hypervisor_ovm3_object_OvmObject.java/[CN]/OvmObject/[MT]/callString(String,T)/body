{
  Object result=callWrapper(call,args);
  if (result == null) {
    return null;
  }
  if (result instanceof String)   return result.toString();
  if (result instanceof Integer)   return result.toString();
  if (result instanceof Long)   return result.toString();
  if (result instanceof HashMap)   return result.toString();
  Object[] results=(Object[])result;
  if (results.length == 0)   return null;
  if (results.length == 1)   return results[0].toString();
  return null;
}
