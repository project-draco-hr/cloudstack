def check_vag(sf_volume, sf_vag_id, obj_assert):
    obj_assert.assertEqual(len(sf_volume['volumeAccessGroups']), 1, 'The volume should only be in one VAG.')
    obj_assert.assertEqual(sf_volume['volumeAccessGroups'][0], sf_vag_id, (('The volume is not in the VAG with the following ID: ' + str(sf_vag_id)) + '.'))
