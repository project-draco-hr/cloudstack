def load(self):
    self.new_config = []
    self.config = []
    try:
        for line in open(self.filename):
            self.new_config.append(line)
    except IOError:
        logging.debug(('File %s does not exist' % self.filename))
        return
    else:
        logging.debug(('Reading file %s' % self.filename))
        self.config = copy.deepcopy(self.new_config)
