def commit(self):
    if (not self.is_changed()):
        logging.info(('Nothing to commit. The %s file did not change' % self.filename))
        return
    handle = open(self.filename, 'w+')
    for line in self.new_config:
        handle.write(line)
    handle.close()
    logging.info(('Wrote edited file %s' % self.filename))
