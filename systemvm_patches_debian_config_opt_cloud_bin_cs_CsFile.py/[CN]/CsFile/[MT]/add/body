def add(self, string, where=(-1)):
    for (index, line) in enumerate(self.new_config):
        if (line.strip() == string):
            return False
    if (where == (-1)):
        self.new_config.append(('%s\n' % string))
    else:
        self.new_config.insert(where, ('%s\n' % string))
    return True
