{
  final String vmName=cmd.getVmName();
  String result=null;
  State state=State.Stopped;
synchronized (_vms) {
    _vms.put(vmName,State.Starting);
  }
  try {
    result=startVM(cmd);
    if (result != null) {
      return new StartAnswer(cmd,result);
    }
    state=State.Running;
    return new StartAnswer(cmd);
  }
  finally {
synchronized (_vms) {
      _vms.put(vmName,state);
    }
  }
}
