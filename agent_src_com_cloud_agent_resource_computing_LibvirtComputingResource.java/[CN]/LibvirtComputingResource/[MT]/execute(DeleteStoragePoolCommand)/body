{
  try {
    Connect conn=LibvirtConnection.getConnection();
    StoragePool pool=getStoragePool(conn,cmd.getPool().getUuid());
synchronized (getStoragePool(pool.getUUIDString())) {
      pool.destroy();
      pool.undefine();
    }
    KVMHABase.NfsStoragePool sp=new KVMHABase.NfsStoragePool(cmd.getPool().getUuid(),cmd.getPool().getHostAddress(),cmd.getPool().getPath(),_mountPoint + File.separator + cmd.getPool().getUuid(),PoolType.PrimaryStorage);
    _monitor.removeStoragePool(sp);
    rmStoragePool(cmd.getPool().getUuid());
    return new Answer(cmd);
  }
 catch (  LibvirtException e) {
    return new Answer(cmd,false,e.toString());
  }
}
