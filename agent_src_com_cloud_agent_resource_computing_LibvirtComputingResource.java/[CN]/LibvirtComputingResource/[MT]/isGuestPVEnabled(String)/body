{
  if (guestOS == null)   return false;
  if (guestOS.startsWith("Ubuntu 10.04") || guestOS.startsWith("Ubuntu 9") || guestOS.startsWith("Ubuntu 8.10")|| guestOS.startsWith("Fedora 13")|| guestOS.startsWith("Fedora 12")|| guestOS.startsWith("Fedora 11")|| guestOS.startsWith("Fedora 10")|| guestOS.startsWith("Fedora 9")|| guestOS.startsWith("CentOS 5.3")|| guestOS.startsWith("CentOS 5.4")|| guestOS.startsWith("CentOS 5.5")|| guestOS.startsWith("Red Hat Enterprise Linux 5.3")|| guestOS.startsWith("Red Hat Enterprise Linux 5.4")|| guestOS.startsWith("Red Hat Enterprise Linux 5.5")|| guestOS.startsWith("Red Hat Enterprise Linux 6")|| guestOS.startsWith("Debain Lenney")|| guestOS.startsWith("Debain Squeeze"))   return true;
 else   return false;
}
