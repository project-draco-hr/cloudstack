{
  if (sp != null) {
    try {
      String uuid=sp.getUUIDString();
synchronized (getStoragePool(uuid)) {
        sp.destroy();
        sp.undefine();
        sp.free();
      }
      rmStoragePool(uuid);
    }
 catch (    LibvirtException e) {
      s_logger.debug("Failed to destroy storage pool: " + e.toString());
    }
  }
}
