{
  String msg=stopVM(vmName,defineOps.DEFINE_VM);
  if (msg == null) {
    Domain dm=null;
    try {
      dm=_conn.domainLookupByUUID(UUID.nameUUIDFromBytes(vmName.getBytes()));
      dm.create();
      return null;
    }
 catch (    LibvirtException e) {
      s_logger.warn("Failed to create vm",e);
      msg=e.getMessage();
    }
catch (    Exception e) {
      s_logger.warn("Failed to create vm",e);
      msg=e.getMessage();
    }
 finally {
      try {
        if (dm != null)         dm.free();
      }
 catch (      LibvirtException e) {
      }
    }
  }
  return msg;
}
