{
  final ArrayList<Integer> ports=new ArrayList<Integer>(names.size());
  for (  final String name : names) {
    Integer port=null;
    try {
      port=getVncPort(name);
    }
 catch (    Exception e) {
      s_logger.debug(e.toString());
    }
    if (port != null) {
      ports.add(port);
    }
  }
  final int[] results=new int[ports.size()];
  int i=0;
  for (  final Integer port : ports) {
    results[i++]=port;
  }
  return results;
}
