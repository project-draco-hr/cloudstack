{
  Account caller=CallContext.current().getCallingAccount();
  AclGroup group=_aclGroupDao.findById(groupId);
  if (group == null) {
    throw new InvalidParameterValueException("Unable to find acl group: " + groupId + "; failed to remove accounts from acl group.");
  }
  _accountMgr.checkAccess(caller,null,true,group);
  Transaction txn=Transaction.currentTxn();
  txn.start();
  for (  Long acctId : acctIds) {
    Account account=_accountDao.findById(acctId);
    if (account == null) {
      throw new InvalidParameterValueException("Unable to find account: " + acctId + "; failed to add account to acl group.");
    }
    _accountMgr.checkAccess(caller,null,true,account);
    AclGroupAccountMapVO grMap=_aclGroupAccountMapDao.findByGroupAndAccount(groupId,acctId);
    if (grMap != null) {
      _aclGroupAccountMapDao.remove(grMap.getId());
    }
  }
  txn.commit();
  return group;
}
