def __init__(self, logger_name):
    '\n        @Name: __init__\n        @Input: logger_name for logger\n        '
    self.__loggerName = logger_name
    '\n        Logger for Logging Info\n        '
    self.__logger = None
    '\n        Log Folder Directory\n        '
    self.__logFolderDir = None
    self.__setLogger()
