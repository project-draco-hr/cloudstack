{
  Pair<Boolean,String> result;
  try {
    s_logger.debug("Executing command in VR:  /opt/cloud/bin/router_proxy.sh " + script + " "+ routerIP+ " "+ args);
    result=SshHelper.sshExecute(_host.ip,22,_username,null,_password.peek(),"/opt/cloud/bin/router_proxy.sh " + script + " "+ routerIP+ " "+ args,60000,60000,timeout * 1000);
  }
 catch (  Exception e) {
    return new ExecutionResult(false,e.getMessage());
  }
  return new ExecutionResult(result.first(),result.second());
}
