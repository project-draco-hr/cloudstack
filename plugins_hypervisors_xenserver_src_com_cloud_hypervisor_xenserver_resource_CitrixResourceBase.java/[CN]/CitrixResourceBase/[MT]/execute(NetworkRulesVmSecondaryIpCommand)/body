{
  boolean success=true;
  final Connection conn=getConnection();
  final String result=callHostPlugin(conn,"vmops","network_rules_vmSecondaryIp","vmName",cmd.getVmName(),"vmMac",cmd.getVmMac(),"vmSecIp",cmd.getVmSecIp(),"action",cmd.getAction());
  if (result == null || result.isEmpty() || !Boolean.parseBoolean(result)) {
    success=false;
  }
  return new Answer(cmd,success,"");
}
