{
  String result=callHostPluginPremium(conn,"heartbeat","host",_host.uuid,"timeout",Integer.toString(_heartbeatTimeout),"interval",Integer.toString(_heartbeatInterval));
  if (result == null || !result.contains("> DONE <")) {
    s_logger.warn("Unable to launch the heartbeat process on " + _host.ip);
    return false;
  }
  return true;
}
