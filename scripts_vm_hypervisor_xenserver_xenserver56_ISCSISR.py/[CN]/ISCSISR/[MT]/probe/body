def probe(self):
    SRs = self.session.xenapi.SR.get_all_records()
    Recs = {}
    for sr in SRs:
        record = SRs[sr]
        sm_config = record['sm_config']
        if (sm_config.has_key('targetIQN') and (sm_config['targetIQN'] == self.targetIQN)):
            Recs[record['uuid']] = sm_config
    return self.srlist_toxml(Recs)
