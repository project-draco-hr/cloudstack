{
  return new TimerTask(){
    @Override public void run(){
      try {
        mhost.setLastHeartbeatTime(DateHelper.currentGMTTime());
        mhostDao.updateHeartBeat(mhost);
      }
 catch (      Throwable e) {
        logger.error("Unexpected exception " + e.getMessage(),e);
      }
 finally {
      }
    }
  }
;
}
