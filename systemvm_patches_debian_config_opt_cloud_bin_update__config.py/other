import sys
from merge import loadQueueFile
import logging
logging.basicConfig(filename='/var/log/cloud.log', level=logging.DEBUG, format='%(asctime)s %(message)s')
if (len(sys.argv) != 2):
    print 'Invalid usage'
    sys.exit(1)
if sys.argv[1].startswith('ip'):
    qf = loadQueueFile()
    qf.setType('ips')
    qf.setFile(sys.argv[1])
    qf.load()
