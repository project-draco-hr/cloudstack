{
  try {
    prepare(network,nic,vm,dest,context);
  }
 catch (  ConcurrentOperationException e) {
    logger.error("prepareForMigration failed.",e);
    return false;
  }
catch (  ResourceUnavailableException e) {
    logger.error("prepareForMigration failed.",e);
    return false;
  }
catch (  InsufficientCapacityException e) {
    logger.error("prepareForMigration failed.",e);
    return false;
  }
  return true;
}
