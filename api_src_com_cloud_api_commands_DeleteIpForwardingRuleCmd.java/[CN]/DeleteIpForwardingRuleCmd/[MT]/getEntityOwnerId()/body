{
  if (ownerId == null) {
    PortForwardingRule rule=_entityMgr.findById(PortForwardingRule.class,id);
    if (rule == null) {
      throw new InvalidParameterValueException("Unable to find firewall rule by id: " + id);
    }
 else {
      ownerId=rule.getAccountId();
    }
  }
  return ownerId;
}
