def _verifyVag(self, host_iscsi_iqns, sf_vag_initiators):
    self.assertEqual(isinstance(host_iscsi_iqns, list), True, "'host_iscsi_iqns' is not a list.")
    self.assertEqual(isinstance(sf_vag_initiators, list), True, "'sf_vag_initiators' is not a list.")
    self.assertEqual(len(host_iscsi_iqns), len(sf_vag_initiators), 'Lists are not the same size.')
    for host_iscsi_iqn in host_iscsi_iqns:
        sf_vag_initiators.index(host_iscsi_iqn)
