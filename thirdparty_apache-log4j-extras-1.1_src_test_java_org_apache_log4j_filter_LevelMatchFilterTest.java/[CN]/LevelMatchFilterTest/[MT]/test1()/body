{
  LoggingEvent event=new LoggingEvent("org.apache.log4j.Logger",Logger.getLogger(LevelMatchFilterTest.class),System.currentTimeMillis(),Level.INFO,"Hello, World",null);
  LevelMatchFilter filter=new LevelMatchFilter();
  filter.setLevelToMatch("info");
  filter.activateOptions();
  assertEquals(Filter.ACCEPT,filter.decide(event));
}
