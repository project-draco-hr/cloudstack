{
  this.name=name;
  this.id=poolId;
  this.uuid=uuid;
  this.poolType=type;
  this.dataCenterId=dataCenterId;
  this.usedBytes=availableBytes;
  this.capacityBytes=capacityBytes;
  this.hostAddress=hostAddress;
  this.port=port;
  this.podId=podId;
  this.setStatus(StoragePoolStatus.Initial);
  this.setPath(hostPath);
}
