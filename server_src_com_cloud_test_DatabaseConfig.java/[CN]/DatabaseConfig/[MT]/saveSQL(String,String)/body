{
  TransactionLegacy txn=TransactionLegacy.open("saveSQL");
  try {
    PreparedStatement stmt=txn.prepareAutoCloseStatement(sql);
    stmt.executeUpdate();
  }
 catch (  SQLException ex) {
    System.out.println("SQL Exception: " + ex.getMessage());
    printError(errorMsg);
  }
 finally {
    txn.close();
  }
}
