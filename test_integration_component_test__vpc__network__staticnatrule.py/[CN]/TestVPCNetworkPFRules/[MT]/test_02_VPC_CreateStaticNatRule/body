@attr(tags=['advanced', 'intervlan'])
def test_02_VPC_CreateStaticNatRule(self):
    ' Test case no 229 : Create Static NAT Rule for a single virtual network of \n            a VPC using a new Public IP Address available with the VPC when the Virtual Router is in Running State\n        '
    network_1 = self.create_Network(self.services['network_offering'])
    self.create_ingress_rule(network_1)
    vm_1 = self.create_VM_in_Network(network_1)
    public_ip_1 = self.acquire_Public_IP(network_1)
    self.create_StaticNatRule_For_VM(vm_1, public_ip_1, network_1)
    self.check_ssh_into_vm(vm_1, public_ip_1, testnegative=False)
    return
