@try_except_decor
def button_cancel(self):
    eles = self.browser.find_elements_by_xpath("//button[@type='button' and @role='button']")
    for e in eles:
        if (e.text == 'Cancel'):
            e.click()
            break
    Shared.wait_for_element(self.browser, 'class_name', 'fixed-header')
