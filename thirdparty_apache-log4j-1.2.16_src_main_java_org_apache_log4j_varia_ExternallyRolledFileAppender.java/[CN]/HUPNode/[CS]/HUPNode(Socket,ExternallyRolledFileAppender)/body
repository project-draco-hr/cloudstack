{
  this.socket=socket;
  this.er=er;
  try {
    dis=new DataInputStream(socket.getInputStream());
    dos=new DataOutputStream(socket.getOutputStream());
  }
 catch (  InterruptedIOException e) {
    Thread.currentThread().interrupt();
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
catch (  RuntimeException e) {
    e.printStackTrace();
  }
}
