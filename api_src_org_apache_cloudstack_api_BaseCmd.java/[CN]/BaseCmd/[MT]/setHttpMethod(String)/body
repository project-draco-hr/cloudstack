{
  if (method != null) {
    if (method.equalsIgnoreCase("GET"))     httpMethod=HTTPMethod.GET;
 else     if (method.equalsIgnoreCase("PUT"))     httpMethod=HTTPMethod.PUT;
 else     if (method.equalsIgnoreCase("POST"))     httpMethod=HTTPMethod.POST;
 else     if (method.equalsIgnoreCase("DELETE"))     httpMethod=HTTPMethod.DELETE;
  }
 else {
    httpMethod=HTTPMethod.GET;
  }
}
