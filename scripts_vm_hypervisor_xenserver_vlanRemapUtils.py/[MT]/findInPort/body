def findInPort():
    listIface = [vsctlPath, 'list interface']
    res = doCmd(listIface, True)
    inport = []
    port = ''
    for i in res:
        if ('ofport' in i):
            (x, port) = i.split(':')
            port = port.lstrip().rstrip()
        if ('type' in i):
            (x, type) = i.split(':')
            type = type.lstrip().rstrip()
            if (type == 'gre'):
                inport.append(port)
    return inport
