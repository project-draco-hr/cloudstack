def doCreateFlow(bridge, vifName, mac, remap):
    if (createFlow(bridge, vifName, mac, remap) < 0):
        log(('Create flow failed(bridge=%s, vifName=%s, mac=%s,remap=%s' % (bridge, vifName, mac, remap)))
    else:
        log(('Create flow success(bridge=%s, vifName=%s, mac=%s,remap=%s' % (bridge, vifName, mac, remap)))
