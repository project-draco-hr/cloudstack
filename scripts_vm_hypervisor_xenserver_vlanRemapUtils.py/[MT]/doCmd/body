def doCmd(cmds, lines=False):
    cmd = ''
    for i in cmds:
        cmd += ' '
        cmd += i
    log(("do command '%s'" % cmd))
    f = os.popen(cmd)
    if (lines == True):
        res = f.readlines()
    else:
        res = f.readline()
        res = res[:(-1)]
    f.close()
    if (lines == False):
        log(("command output '%s'" % res))
    return res
