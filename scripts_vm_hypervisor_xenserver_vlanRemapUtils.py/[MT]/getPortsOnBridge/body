def getPortsOnBridge(bridge):
    listBr = [vsctlPath, 'list br', bridge]
    res = doCmd(listBr, True)
    for i in res:
        if ('ports' in i):
            (x, str) = i.split(':')
            str = str.lstrip().rstrip()
            str = str[1:]
            str = str[:(-1)]
            return str.split(',')
    return None
