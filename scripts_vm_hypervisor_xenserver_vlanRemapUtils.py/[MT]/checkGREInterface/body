def checkGREInterface(bridge, remoteIP, greKey):
    listIfaces = [vsctlPath, 'list interface']
    res = doCmd(listIfaces, True)
    start = False
    num = 0
    keyStr = ('key=%s' % greKey)
    uuid = ''
    for i in res:
        if ('_uuid' in i):
            (x, uuid) = i.split(':')
            uuid = strip(uuid)
        if (('options' in i) and (remoteIP in i) and (keyStr in i)):
            log(('WARNING: GRE tunnel for remote_ip=%s key=%s already here' % (remoteIP, greKey)))
            return (-1)
    return 0
