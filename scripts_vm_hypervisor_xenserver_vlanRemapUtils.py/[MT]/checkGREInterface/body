def checkGREInterface(bridge, remoteIP, greKey):
    ports = getPortsOnBridge(bridge)
    if (ports == None):
        return 0
    for i in ports:
        ifaces = getInterfacesOnPort(i)
        if (ifaces == None):
            continue
        for j in ifaces:
            if (j == '[]'):
                continue
            options = getFieldOfInterface(j, 'options')
            if ((remoteIP in options) and (greKey in options)):
                log(('WARNING: GRE tunnel for remote_ip=%s key=%s already here, interface(%s)' % (remoteIP, greKey, j)))
                return (-1)
    return 0
