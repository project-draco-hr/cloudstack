{
  Transaction txn=Transaction.currentTxn();
  try {
    txn.start();
    ManagementServerHostVO msHost=findById(id);
    msHost.setState(ManagementServerNode.State.Down);
    super.remove(id);
    txn.commit();
    return true;
  }
 catch (  Exception e) {
    s_logger.warn("Unexpected exception, ",e);
    txn.rollback();
  }
  return false;
}
