{
  try {
    GlobalLock scanLock=GlobalLock.getInternLock("uploadmonitor.storageGC");
    try {
      if (scanLock.lock(3)) {
        try {
          cleanupStorage();
        }
  finally {
          scanLock.unlock();
        }
      }
    }
  finally {
      scanLock.releaseRef();
    }
  }
 catch (  Exception e) {
    logger.error("Caught the following Exception",e);
  }
}
