{
  CheckSshAnswer answer=(CheckSshAnswer)cmds.getAnswer("checkSsh");
  if (answer == null || !answer.getResult()) {
    if (answer != null)     s_logger.warn("Unable to ssh to the VM: " + answer.getDetails());
 else     s_logger.warn("Unable to ssh to the VM: null answer");
    return false;
  }
  return true;
}
