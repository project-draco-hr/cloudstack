{
  final HashMap<String,HostVmStateReportEntry> vmStates=new HashMap<String,HostVmStateReportEntry>();
  Map<String,State> vms=sync();
  for (  final Map.Entry<String,State> vm : vms.entrySet()) {
    s_logger.debug("VM " + vm.getKey() + " state: "+ vm.getValue()+ ":"+ convertStateToPower(vm.getValue()));
    vmStates.put(vm.getKey(),new HostVmStateReportEntry(convertStateToPower(vm.getValue()),c.getIp()));
  }
  return vmStates;
}
