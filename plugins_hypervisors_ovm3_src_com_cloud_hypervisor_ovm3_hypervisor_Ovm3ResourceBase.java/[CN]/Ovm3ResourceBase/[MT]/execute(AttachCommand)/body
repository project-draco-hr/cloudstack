{
  String vmName=cmd.getVmName();
  DiskTO disk=cmd.getDisk();
  Boolean success=false;
  String answer="";
  if (cmd.getDisk().getType() == Volume.Type.ISO) {
    answer=isoAttachDetach(vmName,disk,true);
  }
  if (answer == null) {
    success=true;
  }
  return new Answer(cmd,success,answer);
}
