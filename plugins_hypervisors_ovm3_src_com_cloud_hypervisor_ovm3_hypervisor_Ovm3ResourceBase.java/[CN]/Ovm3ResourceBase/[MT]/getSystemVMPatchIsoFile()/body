{
  String svmName=getSystemVMIsoFileNameOnDatastore();
  URL url=this.getClass().getClassLoader().getResource("vms/" + svmName);
  s_logger.debug(url);
  File isoFile=null;
  if (url != null) {
    isoFile=new File(url.getPath());
  }
  if (isoFile == null || !isoFile.exists()) {
    isoFile=new File("/usr/share/cloudstack-common/vms/" + svmName);
  }
  assert(isoFile != null);
  if (!isoFile.exists()) {
    s_logger.error("Unable to locate " + svmName + " in your setup at "+ isoFile.toString());
  }
  return isoFile;
}
