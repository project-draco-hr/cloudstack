def __init__(self):
    self.isAsync = 'true'
    'UUID of storage network ip range'
    'Required'
    self.id = None
    'the ending IP address'
    self.endip = None
    'the netmask for storage network'
    self.netmask = None
    'the beginning IP address'
    self.startip = None
    'Optional. the vlan the ip range sits on'
    self.vlan = None
    self.required = ['id']
