def __init__(self):
    self.isAsync = 'true'
    'Map of (key/value pairs)'
    'Required'
    self.details = []
    'resource id to create the details for'
    'Required'
    self.resourceid = None
    'type of the resource'
    'Required'
    self.resourcetype = None
    self.required = ['details', 'resourceid', 'resourcetype']
