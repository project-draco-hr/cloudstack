@attr(tags=['advanced'])
@log_test_exceptions
def test_01_egress_fr1(self):
    'Test By-default the communication from guest n/w to public n/w is allowed.\n        '
    self.create_vm()
    self.exec_script_on_user_vm('ping -c 1 www.google.com', "| grep -oP '\\d+(?=% packet loss)'", "['0']", negative_test=False)
