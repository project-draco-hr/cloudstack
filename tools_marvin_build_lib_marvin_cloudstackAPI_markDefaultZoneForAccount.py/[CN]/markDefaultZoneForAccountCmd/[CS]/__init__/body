def __init__(self):
    self.isAsync = 'true'
    'Name of the account that is to be marked.'
    'Required'
    self.account = None
    'Marks the account that belongs to the specified domain.'
    'Required'
    self.domainid = None
    'The Zone ID with which the account is to be marked.'
    'Required'
    self.zoneid = None
    self.required = ['account', 'domainid', 'zoneid']
