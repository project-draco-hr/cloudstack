{
  rollingStyle=style;
switch (rollingStyle) {
case BY_SIZE:
    rollDate=false;
  rollSize=true;
break;
case BY_DATE:
rollDate=true;
rollSize=false;
break;
case BY_COMPOSITE:
rollDate=true;
rollSize=true;
break;
default :
errorHandler.error("Invalid rolling Style, use 1 (by size only), 2 (by date only) or 3 (both)");
}
}
