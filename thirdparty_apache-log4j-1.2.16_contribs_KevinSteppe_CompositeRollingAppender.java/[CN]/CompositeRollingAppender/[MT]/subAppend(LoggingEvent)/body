{
  if (rollDate) {
    long n=System.currentTimeMillis();
    if (n >= nextCheck) {
      now.setTime(n);
      nextCheck=rc.getNextCheckMillis(now);
      rollOverTime();
    }
  }
  if (rollSize) {
    if ((fileName != null) && ((CountingQuietWriter)qw).getCount() >= maxFileSize) {
      rollOverSize();
    }
  }
  super.subAppend(event);
}
