{
  return new Runnable(){
    @Override public void run(){
      Transaction txn=Transaction.open(Transaction.CLOUD_DB);
      try {
        reallyRun();
      }
 catch (      Throwable e) {
        s_logger.warn("Unexpected exception " + e.getMessage(),e);
      }
 finally {
        txn.close();
      }
    }
    private void reallyRun(){
      loadScan();
    }
  }
;
}
