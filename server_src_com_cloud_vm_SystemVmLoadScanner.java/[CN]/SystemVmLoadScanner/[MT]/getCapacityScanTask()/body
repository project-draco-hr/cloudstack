{
  return new Runnable(){
    @Override public void run(){
      ServerContexts.registerSystemContext();
      try {
        reallyRun();
      }
 catch (      Throwable e) {
        s_logger.warn("Unexpected exception " + e.getMessage(),e);
      }
 finally {
        ServerContexts.unregisterSystemContext();
      }
    }
    private void reallyRun(){
      loadScan();
    }
  }
;
}
