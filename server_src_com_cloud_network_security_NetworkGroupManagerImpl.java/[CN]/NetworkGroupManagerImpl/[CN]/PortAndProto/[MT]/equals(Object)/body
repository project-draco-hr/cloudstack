{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  PortAndProto other=(PortAndProto)obj;
  if (endPort != other.endPort)   return false;
  if (proto == null) {
    if (other.proto != null)     return false;
  }
 else   if (!proto.equals(other.proto))   return false;
  if (startPort != other.startPort)   return false;
  return true;
}
