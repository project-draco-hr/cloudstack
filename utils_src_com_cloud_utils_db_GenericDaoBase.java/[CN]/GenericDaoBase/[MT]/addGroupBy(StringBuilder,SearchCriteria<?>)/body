{
  List<Attribute> groupBys=sc.getGroupBy();
  if (groupBys != null) {
    sql.append(" GROUP BY ");
    for (int i=0; i < groupBys.size(); i++) {
      Attribute attr=groupBys.get(i);
      sql.append(attr.table).append(".").append(attr.columnName);
      if (i < groupBys.size() - 1)       sql.append(", ");
    }
  }
}
