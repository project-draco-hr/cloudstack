{
  final T entity=(T)_factory.newInstance(new Callback[]{NoOp.INSTANCE,new UpdateBuilder(_allAttributes)});
  if (id != null) {
    try {
      _idField.set(entity,id);
    }
 catch (    final IllegalArgumentException e) {
    }
catch (    final IllegalAccessException e) {
    }
  }
  return entity;
}
