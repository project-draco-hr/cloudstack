def getVifs(vmName):
    vifs = []
    xmlfile = virshdumpxml(vmName)
    if (xmlfile == None):
        return vifs
    dom = xml.dom.minidom.parseString(xmlfile)
    for network in dom.getElementsByTagName('interface'):
        target = network.getElementsByTagName('target')[0]
        nicdev = target.getAttribute('dev').strip()
        vifs.append(nicdev)
    return vifs
