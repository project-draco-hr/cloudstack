{
  configure("input/pattern/enhancedPatternLayout.mdc.1.properties");
  clearMDC();
  MDC.put("key1","va11");
  MDC.put("key2","va12");
  logger.debug("Hello World");
  MDC.remove("key1");
  MDC.remove("key2");
  Transformer.transform(TEMP,FILTERED,new Filter[]{new EnhancedLineNumberFilter(),new SunReflectFilter(),new EnhancedJunitTestRunnerFilter(),new MDCOrderFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.mdc.1"));
}
