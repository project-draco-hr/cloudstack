{
  configure("input/pattern/enhancedPatternLayout3.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT1,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new EnhancedLineNumberFilter(),new ISO8601Filter(),new SunReflectFilter(),new EnhancedJunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.3"));
}
