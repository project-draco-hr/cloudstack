{
  configure("input/pattern/enhancedPatternLayout5.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT2,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new EnhancedLineNumberFilter(),new AbsoluteDateAndTimeFilter(),new SunReflectFilter(),new EnhancedJunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.5"));
}
