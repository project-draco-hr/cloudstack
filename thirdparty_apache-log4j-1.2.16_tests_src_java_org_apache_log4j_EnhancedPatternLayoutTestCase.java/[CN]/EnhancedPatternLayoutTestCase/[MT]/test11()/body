{
  configure("input/pattern/enhancedPatternLayout11.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT11a,PAT11b,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new EnhancedLineNumberFilter(),new SunReflectFilter(),new EnhancedJunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.11"));
}
