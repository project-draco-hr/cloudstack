{
  configure("input/pattern/enhancedPatternLayout12.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT12,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new EnhancedLineNumberFilter(),new SunReflectFilter(),new EnhancedJunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.12"));
}
