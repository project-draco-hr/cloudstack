{
  HttpURLConnection conn=getHTTPConnection(urlString);
  InputStream in=conn.getInputStream();
  OutputStream out=new FileOutputStream(new File(localFileFullName));
  byte[] buf=new byte[102400];
  int len=0;
  while ((len=in.read(buf)) > 0) {
    out.write(buf,0,len);
  }
  in.close();
  out.close();
}
