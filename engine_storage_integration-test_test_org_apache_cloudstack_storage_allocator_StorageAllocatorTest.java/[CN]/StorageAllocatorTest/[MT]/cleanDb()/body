{
  if (volumeId != null) {
    volumeDao.remove(volumeId);
    volumeId=null;
  }
  if (diskOfferingId != null) {
    diskOfferingDao.remove(diskOfferingId);
    diskOfferingId=null;
  }
  if (storagePoolId != null) {
    storagePoolDao.remove(storagePoolId);
    storagePoolId=null;
  }
  if (clusterId != null) {
    clusterDao.remove(clusterId);
    clusterId=null;
  }
  if (podId != null) {
    podDao.remove(podId);
    podId=null;
  }
  if (dcId != null) {
    dcDao.remove(dcId);
    dcId=null;
  }
}
