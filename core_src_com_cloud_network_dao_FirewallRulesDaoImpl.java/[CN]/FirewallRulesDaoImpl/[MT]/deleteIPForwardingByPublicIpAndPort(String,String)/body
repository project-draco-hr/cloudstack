{
  Transaction txn=Transaction.currentTxn();
  PreparedStatement pstmt=null;
  try {
    pstmt=txn.prepareAutoCloseStatement(DELETE_IP_FORWARDING_BY_IP_PORT_SQL);
    pstmt.setString(1,ipAddress);
    pstmt.setString(2,port);
    pstmt.executeUpdate();
  }
 catch (  Exception e) {
    s_logger.warn(e);
  }
}
