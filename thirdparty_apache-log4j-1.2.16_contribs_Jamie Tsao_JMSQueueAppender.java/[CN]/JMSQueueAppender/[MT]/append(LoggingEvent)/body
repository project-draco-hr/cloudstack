{
  if (!checkEntryConditions()) {
    return;
  }
  try {
    ObjectMessage msg=queueSession.createObjectMessage();
    msg.setObject(event);
    queueSender.send(msg);
  }
 catch (  Exception e) {
    errorHandler.error("Could not send message in JMSQueueAppender [" + name + "].",e,ErrorCode.GENERIC_FAILURE);
  }
}
