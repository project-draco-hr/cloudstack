{
  String fail=null;
  if (this.queueConnection == null) {
    fail="No QueueConnection";
  }
 else   if (this.queueSession == null) {
    fail="No QueueSession";
  }
 else   if (this.queueSender == null) {
    fail="No QueueSender";
  }
  if (fail != null) {
    errorHandler.error(fail + " for JMSQueueAppender named [" + name+ "].");
    return false;
  }
 else {
    return true;
  }
}
