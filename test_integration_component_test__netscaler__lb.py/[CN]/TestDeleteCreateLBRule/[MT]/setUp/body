def setUp(self):
    self.apiclient = self.testClient.getApiClient()
    self.dbclient = self.testClient.getDbConnection()
    self.lb_rule = LoadBalancerRule.create(self.apiclient, self.services['lbrule'], ipaddressid=self.public_ip.ipaddress.id, accountid=self.account.name, networkid=self.network.id)
    self.cleanup = []
    return
