{
  for (int i=0; i < 10; i++) {
    try {
      return dsMo.fileExists(volumeDatastorePath);
    }
 catch (    Exception e) {
      if (!e.getMessage().contains("is not accessible")) {
        break;
      }
    }
    try {
      Thread.sleep(5000);
    }
 catch (    Exception e) {
    }
  }
  return false;
}
