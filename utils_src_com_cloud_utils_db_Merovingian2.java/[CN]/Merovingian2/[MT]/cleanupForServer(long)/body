{
  Connection conn=null;
  try {
    conn=Transaction.getStandaloneConnectionWithException();
    conn.setAutoCommit(true);
    cleanup(conn,msId);
  }
 catch (  SQLException e) {
    throw new CloudRuntimeException("Unable to clear the locks",e);
  }
 finally {
    try {
      if (conn != null) {
        conn.close();
      }
    }
 catch (    SQLException e) {
    }
  }
}
