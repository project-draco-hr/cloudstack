{
  Connection conn=null;
  try {
    conn=Transaction.getStandaloneConnectionWithException();
    return owns(conn,key);
  }
 catch (  SQLException e) {
    throw new CloudRuntimeException("Unable to retrieve locks ",e);
  }
 finally {
    try {
      if (conn != null) {
        conn.close();
      }
    }
 catch (    SQLException e) {
    }
  }
}
