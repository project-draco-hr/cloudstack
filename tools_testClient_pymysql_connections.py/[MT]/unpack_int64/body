def unpack_int64(n):
    return (((((((struct.unpack('B', n[0])[0] + (struct.unpack('B', n[1])[0] << 8)) + (struct.unpack('B', n[2])[0] << 16)) + (struct.unpack('B', n[3])[0] << 24)) + (struct.unpack('B', n[4])[0] << 32)) + (struct.unpack('B', n[5])[0] << 40)) + (struct.unpack('B', n[6])[0] << 48)) + (struct.unpack('B', n[7])[0] << 56))
