{
  if (dc.getNetworkType() == NetworkType.Advanced && isMyTrafficType(offering.getTrafficType()) && offering.getGuestType() == GuestType.Shared && !_ntwkOfferingSrvcDao.isProviderForNetworkOffering(offering.getId(),Network.Provider.NuageVsp) && !_ntwkOfferingSrvcDao.isProviderForNetworkOffering(offering.getId(),Network.Provider.NiciraNvp)) {
    return true;
  }
 else {
    s_logger.trace("We only take care of Guest networks of type " + GuestType.Shared);
    return false;
  }
}
