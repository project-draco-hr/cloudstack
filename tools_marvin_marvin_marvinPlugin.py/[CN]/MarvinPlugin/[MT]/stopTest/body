def stopTest(self, test):
    '\n        Currently used to record end time for tests\n        '
    endTime = time.time()
    if (self.startTime is not None):
        totTime = int((endTime - self.startTime))
        self.logger.debug(('TestCaseName: %s; Time Taken: %s Seconds;                 StartTime: %s; EndTime: %s' % (self.testName, str(totTime), str(time.ctime(self.startTime)), str(time.ctime(endTime)))))
