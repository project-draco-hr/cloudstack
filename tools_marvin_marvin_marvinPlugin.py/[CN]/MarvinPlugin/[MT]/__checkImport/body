def __checkImport(self, filename):
    '\n        @Name : __checkImport\n        @Desc : Verifies to Import the test Module before running and check\n                whether if it is importable.\n                This will check for test modules which has some issues to be\n                getting imported.\n                Returns False or True based upon the result.\n        '
    try:
        if os.path.isfile(filename):
            ret = os.path.splitext(filename)
            if (ret[1] == '.py'):
                os.system(('python ' + filename))
                return True
        return False
    except ImportError as e:
        print ('FileName :%s : Error : %s' % (filename, GetDetailExceptionInfo(e)))
        return False
