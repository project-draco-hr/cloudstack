def startMarvin(self):
    '\n        @Name : startMarvin\n        @Desc : Initializes the Marvin\n                creates the test Client\n                creates the runlogger for logging\n                Parses the config and creates a parsedconfig\n                Creates a debugstream for tc debug log\n        '
    try:
        obj_marvininit = MarvinInit(self.__configFile, self.__deployDcFlag, None, self.__zoneForTests)
        if (obj_marvininit and (obj_marvininit.init() == SUCCESS)):
            self.__testClient = temp_obj.getTestClient()
            self.__tcRunLogger = temp_obj.getLogger()
            self.__parsedConfig = temp_obj.getParsedConfig()
            self.__resultStream = temp_obj.getResultFile()
            self.__logFolderPath = temp_obj.getLogFolderPath()
            self.__testRunner = nose.core.TextTestRunner(stream=self.__resultStream, descriptions=True, verbosity=2)
            return SUCCESS
        return FAILED
    except Exception as e:
        print ('Exception Occurred under startMarvin: %s' % GetDetailExceptionInfo(e))
        return FAILED
