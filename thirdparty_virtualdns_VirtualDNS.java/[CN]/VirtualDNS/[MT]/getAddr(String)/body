{
  byte[] result=new byte[4];
  String[] fields=hostname.split("-",10);
  if (fields.length != 4) {
    return null;
  }
  for (int i=0; i < 4; i++) {
    try {
      int n=Integer.parseInt(fields[i]);
      if (n > 255) {
        return null;
      }
      result[i]=(byte)n;
    }
 catch (    NumberFormatException e) {
      return null;
    }
  }
  return result;
}
