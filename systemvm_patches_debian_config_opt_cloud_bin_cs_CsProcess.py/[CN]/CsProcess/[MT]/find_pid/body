def find_pid(self):
    self.pid = []
    for i in CsHelper.execute('ps aux'):
        items = len(self.search)
        proc = re.split('\\s+', i)[(items * (-1)):]
        matches = len([m for m in proc if (m in self.search)])
        if (matches == items):
            self.pid.append(re.split('\\s+', i)[1])
    return self.pid
