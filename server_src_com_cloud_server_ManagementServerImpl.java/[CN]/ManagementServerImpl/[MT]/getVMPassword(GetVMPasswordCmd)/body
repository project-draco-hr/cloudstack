{
  Account account=UserContext.current().getCaller();
  UserVmVO vm=_userVmDao.findById(cmd.getId());
  UserVmDetailVO password=_userVmDetailsDao.findDetail(cmd.getId(),"Encrypted.Password");
  if (vm == null || password == null || password.getValue() == null || password.getValue().equals("") || vm.getAccountId() != account.getAccountId())   throw new InvalidParameterValueException("No password for VM with id '" + getId() + "' found.");
  return password.getValue();
}
