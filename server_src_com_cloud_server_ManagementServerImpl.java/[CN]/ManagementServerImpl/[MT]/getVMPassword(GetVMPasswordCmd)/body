{
  Account account=UserContext.current().getCaller();
  UserVmVO vm=_userVmDao.findById(cmd.getId());
  if (vm == null || vm.getEncryptedPassword() == null || vm.getEncryptedPassword().equals("") || vm.getAccountId() != account.getAccountId())   throw new InvalidParameterValueException("No password for VM with id '" + getId() + "' found.");
  return vm.getEncryptedPassword();
}
