{
  boolean success=false;
  try {
    success=_tmpltMgr.copy(userId,templateId,sourceZoneId,destZoneId,startEventId);
  }
 catch (  Exception e) {
    s_logger.warn("Unable to copy template " + templateId + " from zone "+ sourceZoneId+ " to "+ destZoneId,e);
    success=false;
  }
  return success;
}
