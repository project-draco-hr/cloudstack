{
  Account account=UserContext.current().getCaller();
  if (cmd.getName() != null && cmd.getName().length() > 0)   return _sshKeyPairDao.listKeyPairsByName(account.getAccountId(),account.getDomainId(),cmd.getName());
  if (cmd.getFingerprint() != null && cmd.getFingerprint().length() > 0)   return _sshKeyPairDao.listKeyPairsByFingerprint(account.getAccountId(),account.getDomainId(),cmd.getFingerprint());
  return _sshKeyPairDao.listKeyPairs(account.getAccountId(),account.getDomainId());
}
