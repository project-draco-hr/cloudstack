{
  if (s_logger.isDebugEnabled()) {
    s_logger.debug("Retrieiving user with id: " + userId);
  }
  UserVO user=_userDao.getUser(userId);
  if (user == null) {
    if (s_logger.isDebugEnabled()) {
      s_logger.debug("Unable to find user with id " + userId);
    }
    return null;
  }
  return user;
}
