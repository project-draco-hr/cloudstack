{
  if (accountDomainId == serviceOfferingDomainId)   return true;
  DomainVO domainRecord=_domainDao.findById(accountDomainId);
  if (domainRecord != null) {
    while (true) {
      if (domainRecord.getId() == serviceOfferingDomainId)       return true;
      if (domainRecord.getParent() != null)       domainRecord=_domainDao.findById(domainRecord.getParent());
 else       break;
    }
  }
  return false;
}
