{
  Map<String,SystemIntegrityChecker> checkers=ComponentContext.getApplicationContext().getBeansOfType(SystemIntegrityChecker.class);
  for (  SystemIntegrityChecker checker : checkers.values()) {
    try {
      checker.check();
    }
 catch (    Exception e) {
      s_logger.error("Problems with running checker:" + ComponentContext.getTargetClass(checker).getName(),e);
      System.exit(1);
    }
  }
}
