{
  UserVmVO vm=_userVmDao.findById(vmId);
  VMTemplateVO iso=_tmpltDao.findById(isoId);
  boolean success=attachISOToVM(vmId,isoId,attach);
  if (success && attach) {
    vm.setIsoId(iso.getId());
    _userVmDao.update(vmId,vm);
  }
  if (success && !attach) {
    vm.setIsoId(null);
    _userVmDao.update(vmId,vm);
  }
  return success;
}
