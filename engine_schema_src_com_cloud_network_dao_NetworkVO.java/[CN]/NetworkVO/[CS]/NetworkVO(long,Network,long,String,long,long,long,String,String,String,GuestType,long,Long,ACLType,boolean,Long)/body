{
  this(id,that.getTrafficType(),that.getMode(),that.getBroadcastDomainType(),offeringId,domainId,accountId,related,name,displayText,networkDomain,guestType,dcId,physicalNetworkId,aclType,specifyIpRanges,vpcId);
  gateway=that.getGateway();
  cidr=that.getCidr();
  networkCidr=that.getNetworkCidr();
  broadcastUri=that.getBroadcastUri();
  broadcastDomainType=that.getBroadcastDomainType();
  this.guruName=guruName;
  state=that.getState();
  if (state == null) {
    state=State.Allocated;
  }
  uuid=UUID.randomUUID().toString();
  ip6Gateway=that.getIp6Gateway();
  ip6Cidr=that.getIp6Cidr();
}
