def pprint(self, l):
    if isinstance(l, types.ListType):
        self.begin(all=1)
        self.write('(')
        i = 0
        for x in l:
            if i:
                self.addbreak()
            self.pprint(x)
            i += 1
        self.addbreak(width=0, indent=0)
        self.write(')')
        self.end()
    else:
        self.write(self.pstring(l))
