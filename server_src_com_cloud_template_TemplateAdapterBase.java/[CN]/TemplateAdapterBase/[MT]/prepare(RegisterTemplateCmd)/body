{
  Account caller=UserContext.current().getCaller();
  Account owner=_accountMgr.getAccount(cmd.getEntityOwnerId());
  _accountMgr.checkAccess(caller,null,owner);
  return prepare(false,UserContext.current().getCallerUserId(),cmd.getTemplateName(),cmd.getDisplayText(),cmd.getBits(),cmd.isPasswordEnabled(),cmd.getRequiresHvm(),cmd.getUrl(),cmd.isPublic(),cmd.isFeatured(),cmd.isExtractable(),cmd.getFormat(),cmd.getOsTypeId(),cmd.getZoneId(),HypervisorType.getType(cmd.getHypervisor()),cmd.getChecksum(),true,cmd.getTemplateTag(),owner);
}
