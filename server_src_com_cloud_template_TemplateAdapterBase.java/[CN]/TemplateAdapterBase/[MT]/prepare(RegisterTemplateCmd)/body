{
  return prepare(false,UserContext.current().getCallerUserId(),cmd.getTemplateName(),cmd.getDisplayText(),cmd.getBits(),cmd.isPasswordEnabled(),cmd.getRequiresHvm(),cmd.getUrl(),cmd.isPublic(),cmd.isFeatured(),cmd.isExtractable(),cmd.getFormat(),cmd.getOsTypeId(),cmd.getZoneId(),HypervisorType.getType(cmd.getHypervisor()),cmd.getAccountName(),cmd.getDomainId(),cmd.getChecksum(),true);
}
