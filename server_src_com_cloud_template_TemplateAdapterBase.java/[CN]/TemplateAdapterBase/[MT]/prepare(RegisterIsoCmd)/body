{
  return prepare(true,UserContext.current().getCallerUserId(),cmd.getIsoName(),cmd.getDisplayText(),64,false,true,cmd.getUrl(),cmd.isPublic(),cmd.isFeatured(),cmd.isExtractable(),ImageFormat.ISO.toString(),cmd.getOsTypeId(),cmd.getZoneId(),HypervisorType.None,cmd.getAccountName(),cmd.getDomainId(),cmd.getChecksum(),cmd.isBootable());
}
