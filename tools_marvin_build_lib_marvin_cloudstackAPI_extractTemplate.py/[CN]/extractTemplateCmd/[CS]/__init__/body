def __init__(self):
    self.isAsync = 'true'
    'the ID of the template'
    'Required'
    self.id = None
    'the mode of extraction - HTTP_DOWNLOAD or FTP_UPLOAD'
    'Required'
    self.mode = None
    'the url to which the ISO would be extracted'
    self.url = None
    'the ID of the zone where the ISO is originally located'
    self.zoneid = None
    self.required = ['id', 'mode']
