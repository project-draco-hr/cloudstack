{
  try {
    if (!ipmiPing()) {
      Thread.sleep(1000);
      if (!ipmiPing()) {
        logger.warn("Cannot ping ipmi nic " + _ip);
        return null;
      }
    }
  }
 catch (  Exception e) {
    logger.debug("Cannot ping ipmi nic " + _ip,e);
    return null;
  }
  return new PingRoutingCommand(getType(),id,null);
}
