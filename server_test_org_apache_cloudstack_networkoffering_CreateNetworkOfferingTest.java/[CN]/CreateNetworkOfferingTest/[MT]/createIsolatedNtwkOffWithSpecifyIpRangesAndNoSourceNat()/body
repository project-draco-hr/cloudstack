{
  Map<Service,Set<Provider>> serviceProviderMap=new HashMap<Network.Service,Set<Network.Provider>>();
  Set<Network.Provider> vrProvider=new HashSet<Network.Provider>();
  NetworkOfferingVO off=configMgr.createNetworkOffering("isolated","isolated",TrafficType.Guest,null,false,Availability.Optional,200,serviceProviderMap,false,Network.GuestType.Isolated,false,null,false,null,true,false);
  assertNotNull("Isolated network offering with specifyIpRanges=true and with no sourceNatService, failed to create",off);
}
