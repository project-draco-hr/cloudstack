{
  NetworkOfferingVO off=configMgr.createNetworkOffering("shared","shared",TrafficType.Guest,null,true,Availability.Optional,200,null,false,Network.GuestType.Shared,false,null,false,null,true,false,null,false);
  assertNotNull("Shared network offering with specifyIpRanges=true failed to create ",off);
