{
  Connection conn=getConnection();
  String[] results=new String[cmd.getIpAddresses().length];
  int i=0;
  String routerName=cmd.getAccessDetail(NetworkElementCommand.ROUTER_NAME);
  String routerIp=cmd.getAccessDetail(NetworkElementCommand.ROUTER_IP);
  try {
    IpAddressTO[] ips=cmd.getIpAddresses();
    for (    IpAddressTO ip : ips) {
      assignPublicIpAddress(conn,routerName,routerIp,ip.getPublicIp(),ip.isAdd(),ip.isFirstIP(),ip.isSourceNat(),ip.getVlanId(),ip.getVlanGateway(),ip.getVlanNetmask(),ip.getVifMacAddress(),ip.getGuestIp(),ip.getNetworkRate(),ip.getTrafficType(),ip.getNetworkTags());
      results[i++]=ip.getPublicIp() + " - success";
    }
  }
 catch (  InternalErrorException e) {
    s_logger.error("Ip Assoc failure on applying one ip due to exception:  ",e);
    results[i++]=IpAssocAnswer.errorResult;
  }
  return new IpAssocAnswer(cmd,results);
}
