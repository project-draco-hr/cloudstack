{
  Set<VM> vms=host.getResidentVMs(conn);
  for (  VM vm : vms) {
    VM.Record rec=null;
    String name=null;
    if (getRecord) {
      rec=vm.getRecord(conn);
      name=rec.nameLabel;
    }
 else {
      name=vm.getNameLabel(conn);
    }
    if (name.equals(nameLabel)) {
      return new Pair<VM,VM.Record>(vm,rec);
    }
  }
  return null;
}
