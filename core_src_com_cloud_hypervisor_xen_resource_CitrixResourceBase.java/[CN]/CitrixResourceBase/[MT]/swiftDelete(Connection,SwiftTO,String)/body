{
  String result=null;
  try {
    result=callHostPlugin(conn,"swift","swift","op","delete","hostname",swift.getHostName(),"account",swift.getAccount(),"username",swift.getUserName(),"token",swift.getToken(),"rfilename",rfilename);
    if (result != null && result.equals("true")) {
      return true;
    }
  }
 catch (  Exception e) {
    s_logger.warn("swift download failed due to " + e.toString());
  }
  return false;
}
