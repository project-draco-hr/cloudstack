{
  String result=null;
  try {
    result=callHostPluginAsync(conn,"swift","swift",60 * 60 * 1000,"op","upload","hostname",swift.getHostName(),"account",swift.getAccount(),"username",swift.getUserName(),"token",swift.getToken(),"rfilename",rfilename,"lfilename",lfilename);
    if (result != null && result.equals("true")) {
      return true;
    }
  }
 catch (  Exception e) {
    s_logger.warn("swift download failed due to ",e);
  }
  return false;
}
