{
  Connection conn=getConnection();
  try {
    String nwName=Networks.BroadcastScheme.VSwitch.toString();
    Network nw=getNetworkByName(conn,nwName);
    if (nw == null) {
      nw=setupvSwitchNetwork(conn);
    }
    String result=callHostPlugin(conn,"vmops","vlanRemapUtils","op","createGRE","bridge",nw.getBridge(conn),"remoteIP",cmd.getRemoteIp(),"greKey",cmd.getKey());
    if (result.equalsIgnoreCase("SUCCESS")) {
      return new Answer(cmd,true,"create gre tunnel to " + cmd.getRemoteIp() + " success");
    }
 else {
      return new Answer(cmd,false,"create gre tunnel to " + cmd.getRemoteIp() + " failed");
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return new Answer(cmd,false,"create gre tunnel to " + cmd.getRemoteIp() + " failed");
}
