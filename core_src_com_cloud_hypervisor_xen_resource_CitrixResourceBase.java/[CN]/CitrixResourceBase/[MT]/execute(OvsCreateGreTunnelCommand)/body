{
  _isOvs=true;
  Connection conn=getConnection();
  String bridge="unkonwn";
  try {
    Network nw=setupvSwitchNetwork(conn);
    bridge=nw.getBridge(conn);
    String result=callHostPlugin(conn,"vmops","vlanRemapUtils","op","createGRE","bridge",bridge,"remoteIP",cmd.getRemoteIp(),"greKey",cmd.getKey());
    if (result.equalsIgnoreCase("SUCCESS") || result.equalsIgnoreCase("TUNNEL_EXISTED")) {
      return new OvsCreateGreTunnelAnswer(cmd,true,result);
    }
 else {
      return new OvsCreateGreTunnelAnswer(cmd,false,result,_host.ip,cmd.getRemoteIp(),bridge,cmd.getKey());
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return new OvsCreateGreTunnelAnswer(cmd,false,"EXCEPTION",_host.ip,cmd.getRemoteIp(),bridge,cmd.getKey());
}
