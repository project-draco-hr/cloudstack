{
  String parentUuid=callHostPlugin("vmopsSnapshot","getVhdParent","primaryStorageSRUuid",primaryStorageSRUuid,"snapshotUuid",snapshotUuid,"isISCSI",isISCSI.toString());
  if (parentUuid == null || parentUuid.isEmpty()) {
    s_logger.debug("Unable to get parent of VHD " + snapshotUuid + " in SR "+ primaryStorageSRUuid);
    return null;
  }
  return parentUuid;
}
