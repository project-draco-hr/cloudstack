{
  String result=null;
  try {
    result=callHostPluginAsync(conn,"swiftxen","swift",60 * 60,"op","download","url",swift.getUrl(),"account",swift.getAccount(),"username",swift.getUserName(),"key",swift.getKey(),"rfilename",rfilename,"lfilename",lfilename);
    if (result != null && result.equals("true")) {
      return true;
    }
  }
 catch (  Exception e) {
    s_logger.warn("swift download failed due to ",e);
  }
  return false;
}
