{
  XenServerConnection conn=(XenServerConnection)getConnection();
  try {
    Host host=Host.getByUuid(conn,_host.uuid);
    host.enable(conn);
    return true;
  }
 catch (  Exception e) {
    String msg="Catch Exception " + e.getClass().getName() + " : Enable "+ _host+ " in pool("+ _host.pool+ ") failed due to "+ e.toString();
    s_logger.warn(msg);
  }
  return false;
}
