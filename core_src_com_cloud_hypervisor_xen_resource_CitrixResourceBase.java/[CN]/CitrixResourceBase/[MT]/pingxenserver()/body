{
  XenServerConnection conn=(XenServerConnection)getConnection();
  try {
    Host host=Host.getByUuid(conn,_host.uuid);
    host.enable(conn);
    return true;
  }
 catch (  Exception e) {
    String msg="Catch Exception " + e.getClass().getName() + " : Enable host("+ _host.uuid+ ") in pool("+ conn.getPoolUuid()+ ") failed due to "+ e.toString();
    s_logger.warn(msg);
  }
  return false;
}
