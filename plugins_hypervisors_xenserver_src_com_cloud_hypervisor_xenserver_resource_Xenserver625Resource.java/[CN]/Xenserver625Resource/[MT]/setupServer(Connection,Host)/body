{
  com.trilead.ssh2.Connection sshConnection=new com.trilead.ssh2.Connection(_host.ip,22);
  try {
    sshConnection.connect(null,60000,60000);
    if (!sshConnection.authenticateWithPassword(_username,_password.peek())) {
      throw new CloudRuntimeException("Unable to authenticate");
    }
    String cmd="rm -f /opt/xensource/sm/hostvmstats.py " + "/opt/xensource/bin/copy_vhd_to_secondarystorage.sh " + "/opt/xensource/bin/copy_vhd_from_secondarystorage.sh "+ "/opt/xensource/bin/create_privatetemplate_from_snapshot.sh "+ "/opt/xensource/bin/vhd-util "+ "/opt/cloud/bin/copy_vhd_to_secondarystorage.sh "+ "/opt/cloud/bin/copy_vhd_from_secondarystorage.sh "+ "/opt/cloud/bin/create_privatetemplate_from_snapshot.sh "+ "/opt/cloud/bin/vhd-util";
    SSHCmdHelper.sshExecuteCmd(sshConnection,cmd);
  }
 catch (  Exception e) {
    s_logger.debug("Catch exception " + e.toString(),e);
  }
 finally {
    sshConnection.close();
  }
  return super.setupServer(conn,host);
}
