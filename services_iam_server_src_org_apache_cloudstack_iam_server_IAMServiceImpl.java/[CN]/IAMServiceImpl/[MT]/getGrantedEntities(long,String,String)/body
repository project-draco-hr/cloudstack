{
  List<IAMPolicy> policies=listIAMPolicies(accountId);
  List<Long> entityIds=new ArrayList<Long>();
  for (  IAMPolicy policy : policies) {
    List<IAMPolicyPermissionVO> pp=_policyPermissionDao.listGrantedByActionAndScope(policy.getId(),action,scope);
    if (pp != null) {
      for (      IAMPolicyPermissionVO p : pp) {
        if (p.getScopeId() != null) {
          entityIds.add(p.getScopeId());
        }
      }
    }
  }
  return entityIds;
}
