{
  Answer[] answers=send(hostId,new Command[]{cmd},true,timeout);
  if (answers != null && !(answers[0] instanceof UnsupportedAnswer)) {
    return answers[0];
  }
  if (answers != null && (answers[0] instanceof UnsupportedAnswer)) {
    s_logger.warn("Unsupported Command: " + answers[0].getDetails());
    return answers[0];
  }
  return null;
}
