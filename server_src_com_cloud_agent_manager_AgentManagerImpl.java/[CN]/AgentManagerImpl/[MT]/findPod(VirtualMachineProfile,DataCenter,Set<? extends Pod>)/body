{
  for (  PodAllocator allocator : _podAllocators) {
    Pod pod=allocator.allocateTo(vm,dc,avoids);
    if (pod != null) {
      s_logger.debug("Pod " + pod.getId() + " is found by "+ allocator.getName());
      return pod;
    }
  }
  s_logger.debug("Unable to find any pod for " + vm);
  return null;
}
