{
  s_logger.debug("Adding link for " + id);
  final AgentAttache attache=new ConnectedAgentAttache(id,link,server.getStatus() == Status.Maintenance || server.getStatus() == Status.ErrorInMaintenance || server.getStatus() == Status.PrepareForMaintenance);
  link.attach(attache);
  AgentAttache old=null;
synchronized (_agents) {
    old=_agents.get(id);
    _agents.put(id,attache);
  }
  return attache;
}
