{
  final HostVO host=_hostDao.findById(hostId);
  if (host == null) {
    if (logger.isDebugEnabled()) {
      logger.debug("Cant not find host " + hostId);
    }
    return false;
  }
 else {
    try {
      return resourceStateTransitTo(host,ResourceState.Event.UnableToMigrate,_nodeId);
    }
 catch (    final NoTransitionException e) {
      logger.debug("No next resource state for host " + host.getId() + " while current state is "+ host.getResourceState()+ " with event "+ ResourceState.Event.UnableToMigrate,e);
      return false;
    }
  }
}
