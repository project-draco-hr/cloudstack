{
  if (host == null) {
    if (cmds != null) {
      StartupCommand firstCmd=cmds[0];
      host=findHostByGuid(firstCmd.getGuid());
      if (host == null) {
        host=findHostByGuid(firstCmd.getGuidWithoutResource());
      }
    }
  }
  if (host != null) {
    _agentMgr.agentStatusTransitTo(host,Status.Event.AgentDisconnected,_nodeId);
  }
}
