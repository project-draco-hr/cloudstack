{
  HostVO host=_hostDao.findById(hostId);
  if (host == null) {
    return null;
  }
 else {
    _hostDao.loadDetails(host);
    DetailVO detail=_hostDetailsDao.findDetail(hostId,"guest.os.category.id");
    if (detail == null) {
      return null;
    }
 else {
      return Long.parseLong(detail.getValue());
    }
  }
}
