{
  DataObject leafData=null;
  DataStore store=cacheMgr.getCacheStorage(snapshot.getDataStore().getScope());
  while (snapshot != null) {
    DataObject cacheData=cacheMgr.createCacheObject(snapshot,store);
    if (leafData == null) {
      leafData=cacheData;
    }
    snapshot=snapshot.getParent();
  }
  return leafData;
}
