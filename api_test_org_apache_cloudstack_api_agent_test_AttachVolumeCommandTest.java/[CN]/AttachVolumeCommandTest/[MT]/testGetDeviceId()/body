{
  Long dId=avc.getDeviceId();
  Long expected=123456789L;
  assertEquals(expected,dId);
  avc.setDeviceId(5L);
  dId=avc.getDeviceId();
  expected=5L;
  assertEquals(expected,dId);
  avc.setDeviceId(0L);
  dId=avc.getDeviceId();
  expected=0L;
  assertEquals(expected,dId);
  avc.setDeviceId(-5L);
  dId=avc.getDeviceId();
  expected=-5L;
  assertEquals(expected,dId);
}
