{
  boolean columnExists=false;
  try (PreparedStatement pstmt=conn.prepareStatement("SELECT " + columnName + " FROM "+ tableName)){
    pstmt.executeQuery();
    columnExists=true;
  }
 catch (  SQLException e) {
    s_logger.warn("Field " + columnName + " doesn't exist in "+ tableName,e);
  }
  return columnExists;
}
