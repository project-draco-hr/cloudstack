@classmethod
def login(cls, apiclient, username, password, domain=None, domainid=None):
    'Logins to the CloudStack'
    cmd = login.loginCmd()
    cmd.username = username
    mdf = hashlib.md5()
    mdf.update(password)
    cmd.password = mdf.hexdigest()
    if domain:
        cmd.domain = domain
    if domainid:
        cmd.domainid = domainid
    return apiclient.login(cmd)
