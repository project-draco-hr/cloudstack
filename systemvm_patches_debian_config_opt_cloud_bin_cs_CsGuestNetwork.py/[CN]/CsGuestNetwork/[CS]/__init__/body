def __init__(self, device, config):
    self.data = {}
    self.guest = True
    db = DataBag()
    db.setKey('guestnetwork')
    db.load()
    dbag = db.getDataBag()
    self.config = config
    if ((device in dbag.keys()) and (len(dbag[device]) != 0)):
        self.data = dbag[device][0]
    else:
        self.guest = False
