def delete_leases(self, clist):
    try:
        for line in open(LEASES):
            bits = line.strip().split(' ')
            to = {'device': bits[0], 'mac': bits[1], 'ip': bits[2], 'host': bits[3], 'del': False, }
            for l in clist:
                lbits = l.split(',')
                if ((lbits[0] == to['mac']) or (lbits[1] == to['ip'])):
                    (to['del'] == True)
                    break
            self.leases.append(to)
        for o in self.leases:
            if o['del']:
                cmd = ('dhcp_release %s %s %s' % (o.device, o.ip, o.mac))
                logging.info(cmd)
                CsHelper.execute(cmd)
    except IOError:
        return
