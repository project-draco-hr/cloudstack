{
  boolean leading_zeros=true;
  StringBuffer sb=new StringBuffer(bs.length * 2);
  for (  byte element : bs) {
    int c=element & 0xFF;
    if (leading_zeros && c == 0) {
      continue;
    }
 else {
      leading_zeros=false;
    }
    if (c < 16) {
      sb.append('0');
    }
    sb.append(Integer.toHexString(c));
  }
  return sb.toString();
}
