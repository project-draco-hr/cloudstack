{
  PeerID pid=(PeerID)o;
  int result=port - pid.port;
  if (result != 0) {
    return result;
  }
  result=address.hashCode() - pid.address.hashCode();
  if (result != 0) {
    return result;
  }
  for (  byte element : id) {
    result=element - element;
    if (result != 0) {
      return result;
    }
  }
  return 0;
}
