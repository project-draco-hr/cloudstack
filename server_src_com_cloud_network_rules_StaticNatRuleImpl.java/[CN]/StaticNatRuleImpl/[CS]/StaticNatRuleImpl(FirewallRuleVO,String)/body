{
  this.id=rule.getId();
  this.xid=rule.getXid();
  this.uuid=rule.getUuid();
  this.protocol=rule.getProtocol();
  this.portStart=rule.getSourcePortStart();
  this.portEnd=rule.getSourcePortEnd();
