def setup(self):
    self.fw.append(['', 'front', ('-A INPUT -i %s -d %s/32 -p tcp -m tcp -m state --state NEW --dport 8080 -j ACCEPT' % (self.dev, self.ip))])
    proc = CsProcess(['/opt/cloud/bin/vpc_passwd_server', self.ip])
    if (not proc.find()):
        proc.start('/usr/bin/nohup', '>/dev/null 2>&1 &')
