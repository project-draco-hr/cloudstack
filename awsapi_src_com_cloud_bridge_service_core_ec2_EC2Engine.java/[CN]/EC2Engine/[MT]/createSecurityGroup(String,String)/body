{
  EC2SecurityGroup sg=new EC2SecurityGroup();
  try {
    CloudStackSecurityGroup grp=getApi().createSecurityGroup(groupName,null,groupDesc,null);
    if (grp != null && grp.getId() != null) {
      sg.setId(grp.getId());
    }
  }
 catch (  Exception e) {
    logger.error("EC2 CreateSecurityGroup - ",e);
    handleException(e);
  }
  return sg;
}
