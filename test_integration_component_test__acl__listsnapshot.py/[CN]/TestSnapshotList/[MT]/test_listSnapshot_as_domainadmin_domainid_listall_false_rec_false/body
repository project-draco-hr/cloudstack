@attr('simulator_only', tags=['advanced'], required_hardware='false')
def test_listSnapshot_as_domainadmin_domainid_listall_false_rec_false(self):
    '\n\tTest listing of Snapshots by passing domainId ,listall="false" and isrecursive="false" parameter as domain admin\n\tValidate that it returns all the Snapshots in the domain passed\n\t'
    self.apiclient.connection.apiKey = self.user_d1_apikey
    self.apiclient.connection.securityKey = self.user_d1_secretkey
    snapshotList = Snapshot.list(self.apiclient, domainid=self.domain_11.id, listall='false', isrecursive='false')
    self.debug(('List as Domain Admin passing domainId  - listall=false,isrecursive=false %s' % snapshotList))
    self.assertEqual((len(snapshotList) == 3), True, 'Number of items in list response check failed!!')
    if (self.checkForExistenceOfValue(snapshotList, self.vm_d11_snapshot.id) and self.checkForExistenceOfValue(snapshotList, self.vm_d11a_snapshot.id) and self.checkForExistenceOfValue(snapshotList, self.vm_d11b_snapshot.id)):
        accountAccess = True
    else:
        accountAccess = False
    self.assertEqual(accountAccess, True, 'Account access check failed!!')
