{
  if (networkType == NetworkType.Advanced && isMyTrafficType(offering.getTrafficType()) && offering.getGuestType() == Network.GuestType.Isolated && isMyIsolationMethod(physicalNetwork) && ntwkOfferingSrvcDao.areServicesSupportedByNetworkOffering(offering.getId(),Service.Connectivity)) {
    return true;
  }
 else {
    logger.trace("We only take care of Guest networks of type   " + GuestType.Isolated + " in zone of type "+ NetworkType.Advanced);
    return false;
  }
}
