{
  if (networkType == NetworkType.Advanced && isMyTrafficType(offering.getTrafficType()) && supportedGuestTypes(offering,Network.GuestType.Isolated,Network.GuestType.Shared) && isMyIsolationMethod(physicalNetwork) && ntwkOfferingSrvcDao.areServicesSupportedByNetworkOffering(offering.getId(),Service.Connectivity)) {
    return true;
  }
 else {
    return false;
  }
}
