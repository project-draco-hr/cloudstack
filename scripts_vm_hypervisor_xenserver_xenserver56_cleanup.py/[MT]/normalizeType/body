def normalizeType(type):
    if (type in LVHDSR.SUBTYPES):
        type = SR.TYPE_LVHD
    if (type in ['lvm', 'lvmoiscsi', 'lvmohba']):
        type = SR.TYPE_LVHD
    if (type in ['ext', 'nfs']):
        type = SR.TYPE_FILE
    if (not (type in SR.TYPES)):
        raise util.SMException(('Unsupported SR type: %s' % type))
    return type
