{
  Frame frame=new Frame(title + " - RDP");
  scroller=new ScrollPane(ScrollPane.SCROLLBARS_AS_NEEDED);
  scroller.add(canvas);
  scroller.setSize(canvas.getWidth(),canvas.getHeight());
  frame.add(scroller);
  frame.pack();
  frame.setVisible(true);
  frame.addWindowListener(windowListener);
  return frame;
}
