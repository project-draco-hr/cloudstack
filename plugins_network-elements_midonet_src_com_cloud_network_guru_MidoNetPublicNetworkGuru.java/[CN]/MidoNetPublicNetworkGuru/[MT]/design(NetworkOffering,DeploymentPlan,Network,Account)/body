{
  s_logger.debug("design called with network: " + network.toString());
  if (!canHandle(offering)) {
    return null;
  }
  if (offering.getTrafficType() == Networks.TrafficType.Public) {
    NetworkVO ntwk=new NetworkVO(offering.getTrafficType(),Networks.Mode.Static,Networks.BroadcastDomainType.Mido,offering.getId(),Network.State.Allocated,plan.getDataCenterId(),plan.getPhysicalNetworkId());
    return ntwk;
  }
 else {
    return null;
  }
}
