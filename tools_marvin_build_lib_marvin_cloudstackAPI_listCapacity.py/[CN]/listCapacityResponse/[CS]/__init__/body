def __init__(self):
    'the total capacity available'
    self.capacitytotal = None
    'the capacity currently in use'
    self.capacityused = None
    'the Cluster ID'
    self.clusterid = None
    'the Cluster name'
    self.clustername = None
    'the percentage of capacity currently in use'
    self.percentused = None
    'the Pod ID'
    self.podid = None
    'the Pod name'
    self.podname = None
    'the capacity type'
    self.type = None
    'the Zone ID'
    self.zoneid = None
    'the Zone name'
    self.zonename = None
