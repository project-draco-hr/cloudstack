{
  Answer answer=null;
  int retries=3;
  int i=0;
  for (  EndPoint ep : endPoints) {
    answer=ep.sendMessage(cmd);
    if (answer == null || answer.getDetails() != null) {
      if (i < retries) {
        s_logger.debug("create volume failed, retrying: " + i);
      }
      i++;
    }
 else {
      break;
    }
  }
  return answer;
}
