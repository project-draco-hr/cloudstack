{
  System.out.print("API Rule: ");
  String skip="";
  int brk=0;
  while (true) {
    int stop=StringUtils.formatForOutput(rule,brk,75 - skip.length(),' ');
    if (stop < 0) {
      break;
    }
    System.out.print(skip);
    skip="          ";
    System.out.println(rule.substring(brk,stop).trim());
    brk=stop;
  }
}
