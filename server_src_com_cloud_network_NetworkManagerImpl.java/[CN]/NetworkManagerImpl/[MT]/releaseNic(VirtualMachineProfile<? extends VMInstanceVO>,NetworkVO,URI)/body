{
  NicVO nic=_nicDao.findByInstanceIdNetworkIdAndBroadcastUri(network.getId(),vmProfile.getId(),broadcastUri.toString());
  releaseNic(vmProfile,nic,network);
  NicProfile profile=new NicProfile(nic,network,nic.getBroadcastUri(),nic.getIsolationUri(),null,isSecurityGroupSupportedInNetwork(network),getNetworkTag(vmProfile.getVirtualMachine().getHypervisorType(),network));
  return profile;
}
