{
  String xenLabel=null;
switch (trafficType) {
case Public:
    xenLabel="cloud-public";
  break;
case Guest:
xenLabel="cloud-guest";
break;
case Storage:
xenLabel="cloud-storage";
break;
case Management:
xenLabel="cloud-private";
break;
case Control:
xenLabel="cloud_link_local_network";
break;
}
return xenLabel;
}
