{
  long dcId=vpc.getZoneId();
  IPAddressVO sourceNatIp=getExistingSourceNat(owner.getId(),null,vpc.getId());
  PrivateIp ipToReturn=null;
  if (sourceNatIp != null) {
    ipToReturn=new PrivateIp(sourceNatIp,_vlanDao.findById(sourceNatIp.getVlanId()),NetUtils.createSequenceBasedMacAddress(sourceNatIp.getMacAddress()));
  }
 else {
    ipToReturn=assignSourceNatIpAddress(owner,null,vpc.getId(),dcId);
  }
  return ipToReturn;
}
