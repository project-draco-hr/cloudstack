{
  for (  PublicIp ip : publicIps) {
    if (ip.isOneToOneNat()) {
      Long activeFwCount=null;
      activeFwCount=_firewallDao.countRulesByIpIdAndState(ip.getId(),FirewallRule.State.Active);
      if (!postApplyRules && !forRevoke) {
        if (activeFwCount > 0) {
          continue;
        }
 else {
          return true;
        }
      }
 else       if (postApplyRules && forRevoke) {
        return true;
      }
    }
 else {
      continue;
    }
  }
  return false;
}
