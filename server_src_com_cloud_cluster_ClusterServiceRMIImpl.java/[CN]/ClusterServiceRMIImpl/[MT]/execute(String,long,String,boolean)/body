{
  if (s_logger.isInfoEnabled())   s_logger.info("Execute command forwarded from peer : " + callingPeer);
  Command[] cmds=null;
  try {
    cmds=gson.fromJson(gsonPackage,Command[].class);
  }
 catch (  Throwable e) {
    assert(false);
  }
  try {
    Answer[] answers=manager.sendToAgent(agentId,cmds,stopOnError);
    if (answers != null)     return gson.toJson(answers);
  }
 catch (  AgentUnavailableException e) {
    s_logger.warn("Agent unavailable",e);
  }
catch (  OperationTimedoutException e) {
    s_logger.warn("Timed Out",e);
  }
  return null;
}
