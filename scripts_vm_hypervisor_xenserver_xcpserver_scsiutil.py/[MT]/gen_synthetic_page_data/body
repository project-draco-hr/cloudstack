def gen_synthetic_page_data(uuid):
    page80 = ''
    page80 += '\x00\x80'
    page80 += '\x00\x12'
    page80 += uuid[0:16]
    page80 += '  '
    page83 = ''
    page83 += '\x00\x83'
    page83 += '\x001'
    page83 += '\x02\x01\x00-'
    page83 += 'XENSRC  '
    page83 += uuid
    page83 += ' '
    return ['', base64.b64encode(page80), base64.b64encode(page83)]
