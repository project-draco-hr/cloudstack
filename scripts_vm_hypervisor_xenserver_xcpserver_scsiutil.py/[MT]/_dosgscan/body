def _dosgscan():
    regex = re.compile('([^:]*):\\s+scsi([0-9]+)\\s+channel=([0-9]+)\\s+id=([0-9]+)\\s+lun=([0-9]+)')
    scan = util.pread2(['/usr/bin/sg_scan']).split('\n')
    sgs = []
    for line in scan:
        m = regex.match(line)
        if m:
            device = m.group(1)
            host = m.group(2)
            channel = m.group(3)
            sid = m.group(4)
            lun = m.group(5)
            sgs.append([device, host, channel, sid, lun])
    return sgs
