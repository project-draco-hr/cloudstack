{
  Class<?> cmdClass=_apiServer.getCmdClass(action);
  AccessType accessType=null;
  if (BaseListCmd.class.isAssignableFrom(cmdClass)) {
    accessType=AccessType.ListEntry;
  }
  return _iamSrv.addAclPermissionToAclPolicy(aclPolicyId,entityType,scope.toString(),scopeId,action,accessType.toString(),perm,recursive);
}
