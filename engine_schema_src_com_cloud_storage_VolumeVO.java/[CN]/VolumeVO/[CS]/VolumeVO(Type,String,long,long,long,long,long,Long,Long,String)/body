{
  volumeType=type;
  this.name=name;
  dataCenterId=dcId;
  this.accountId=accountId;
  this.domainId=domainId;
  this.size=size;
  this.minIops=minIops;
  this.maxIops=maxIops;
  _iScsiName=iScsiName;
  this.diskOfferingId=diskOfferingId;
  state=State.Allocated;
  uuid=UUID.randomUUID().toString();
}
