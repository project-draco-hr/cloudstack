def __init__(self):
    self.isAsync = 'true'
    'The ID of the vm'
    'Required'
    self.virtualmachineid = None
    'The discription of the snapshot'
    self.description = None
    'The display name of the snapshot'
    self.name = None
    'snapshot memory if true'
    self.snapshotmemory = None
    self.required = ['virtualmachineid']
