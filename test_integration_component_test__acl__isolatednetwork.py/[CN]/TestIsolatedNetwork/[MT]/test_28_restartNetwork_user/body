@attr('simulator_only', tags=['advanced'], required_hardware='false')
def test_28_restartNetwork_user(self):
    '\n        #Validate that  Regular should be able to restart network for himslef\n\t'
    self.apiclient.connection.apiKey = self.user_d1a_apikey
    self.apiclient.connection.securityKey = self.user_d1a_secretkey
    restartResponse = self.network_d1a.restart(self.apiclient)
    self.assertEqual(restartResponse.success, True, 'User is not able to restart network he owns')
