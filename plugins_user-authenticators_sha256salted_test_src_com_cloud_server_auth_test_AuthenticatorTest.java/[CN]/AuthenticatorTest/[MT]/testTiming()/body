{
  Map<String,Object[]> dummyMap=new HashMap<String,Object[]>();
  Double threshold=(double)500000;
  Long t1=System.nanoTime();
  authenticator.authenticate("admin","password",0L,dummyMap);
  Long t2=System.nanoTime();
  authenticator.authenticate("admin20Byte","password",0L,dummyMap);
  Long t3=System.nanoTime();
  authenticator.authenticate("fake","fake",0L,dummyMap);
  Long t4=System.nanoTime();
  authenticator.authenticate("admin","fake",0L,dummyMap);
  Long t5=System.nanoTime();
  Long diff1=t2 - t1;
  Long diff2=t3 - t2;
  Long diff3=t4 - t3;
  Long diff4=t5 - t4;
  Assert.assertTrue("All computation times within " + threshold / 1000000 + " milisecond",(diff1 <= threshold) && (diff2 <= threshold) && (diff3 <= threshold)&& (diff4 <= threshold));
}
