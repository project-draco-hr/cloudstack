{
  Comparator<?> comparator=new Comparator<DomainVO>(){
    public int compare(    DomainVO u1,    DomainVO u2){
      if (_manager.isSystemRootDomain(u1)) {
        return -1;
      }
      if (_manager.isSystemRootDomain(u2)) {
        return 1;
      }
      return u1.getUuid().compareTo(u2.getUuid());
    }
  }
;
  return comparator;
}
