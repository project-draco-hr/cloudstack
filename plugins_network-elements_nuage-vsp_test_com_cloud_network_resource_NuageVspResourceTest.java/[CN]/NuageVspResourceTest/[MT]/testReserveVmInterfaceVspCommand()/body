{
  _resource.configure("NuageVspResource",_parameters);
  ReserveVmInterfaceVspCommand rsrvVmInfCmd=new ReserveVmInterfaceVspCommand("nicUuid","nicMacAddress","networkUuid",true,"vpcUuid","networkDomainUuid","networksAccountUuid",false,"domainRouterIp","vmInstanceName","vmUuid","vmUserName","vmUserDomainName");
  doAnswer(genericAnswer).when(_mockNuageVspGuruClient).reserve("nicUuid","nicMacAddress","networkUuid",true,"vpcUuid","networkDomainUuid","networksAccountUuid",false,"domainRouterIp","vmInstanceName","vmUuid");
  ReserveVmInterfaceVspAnswer rsrvVmInfAns=(ReserveVmInterfaceVspAnswer)_resource.executeRequest(rsrvVmInfCmd);
  assertTrue(rsrvVmInfAns.getResult());
}
