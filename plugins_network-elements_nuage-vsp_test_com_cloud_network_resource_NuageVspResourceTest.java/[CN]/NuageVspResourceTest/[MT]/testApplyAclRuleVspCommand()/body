{
  _resource.configure("NuageVspResource",_parameters);
  ApplyAclRuleVspCommand applyAclCmd=new ApplyAclRuleVspCommand("networkUuid","networkDomainUuid","vpcOrSubnetUuid",true,new ArrayList<Map<String,Object>>(),false,100);
  doAnswer(genericAnswer).when(_mockNuageVspElementClient).applyAclRules("networkUuid","networkDomainUuid","vpcOrSubnetUuid",true,new ArrayList<Map<String,Object>>(),false,100);
  ApplyAclRuleVspAnswer applyAclAns=(ApplyAclRuleVspAnswer)_resource.executeRequest(applyAclCmd);
  assertTrue(applyAclAns.getResult());
}
