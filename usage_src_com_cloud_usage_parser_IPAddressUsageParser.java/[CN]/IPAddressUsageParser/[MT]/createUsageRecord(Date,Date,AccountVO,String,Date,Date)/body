{
  if (s_logger.isDebugEnabled()) {
    s_logger.debug("Creating usage record for account: " + account.getId() + ", ip: "+ address+ ", assigned date: "+ assigned+ ", owned until: "+ ownedUntil);
  }
  String usageDesc="usage for ip address '" + address + "' (assigned on "+ assigned+ ", owned until "+ ownedUntil+ ")";
  UsageVO usageRecord=new UsageVO(Long.valueOf(0),account.getId(),account.getDomainId(),usageDesc,"1 Month",UsageTypes.IP_ADDRESS,Double.valueOf(1),null,null,null,null,null,null,startDate,endDate);
  m_usageDao.persist(usageRecord);
}
