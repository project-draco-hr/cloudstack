{
  if (template.getAccountId() != Account.ACCOUNT_ID_SYSTEM) {
    UsageEventUtils.publishUsageEvent(EventTypes.EVENT_TEMPLATE_CREATE,template.getAccountId(),host.getDataCenterId(),template.getId(),template.getName(),null,template.getSourceTemplateId(),0L,template.getClass().getName(),template.getUuid());
  }
}
