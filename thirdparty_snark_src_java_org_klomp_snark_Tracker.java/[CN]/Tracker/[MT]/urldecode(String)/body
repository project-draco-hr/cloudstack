{
  s=s.replace('+',' ');
  char[] cs=s.toCharArray();
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  int i=0;
  while (i < cs.length) {
    if (cs[i] != '%') {
      baos.write((byte)cs[i]);
      i++;
    }
 else     if (i + 2 < cs.length) {
      int val=16 * Character.digit(cs[i + 1],16) + Character.digit(cs[i + 2],16);
      baos.write((byte)val);
      i+=3;
    }
 else {
      i++;
    }
  }
  return baos.toByteArray();
}
