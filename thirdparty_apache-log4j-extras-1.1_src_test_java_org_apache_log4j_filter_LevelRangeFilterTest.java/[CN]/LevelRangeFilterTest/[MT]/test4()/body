{
  LoggingEvent event=new LoggingEvent("org.apache.log4j.Logger",Logger.getLogger(LevelRangeFilterTest.class),System.currentTimeMillis(),Level.INFO,"Hello, World",null);
  LevelRangeFilter filter=new LevelRangeFilter();
  filter.setLevelMax(Level.ERROR);
  filter.setAcceptOnMatch(true);
  filter.activateOptions();
  assertEquals(Filter.ACCEPT,filter.decide(event));
}
