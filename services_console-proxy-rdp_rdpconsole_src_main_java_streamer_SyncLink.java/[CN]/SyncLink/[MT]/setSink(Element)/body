{
  if (sink != null && this.sink != null)   throw new RuntimeException("[" + this + "] ERROR: This link sink element is already set. Link: "+ this+ ", new sink: "+ sink+ ", existing sink: "+ this.sink+ ".");
  if (sink == null && cacheBuffer != null)   throw new RuntimeException("[" + this + "] ERROR: Cannot drop link: cache is not empty. Link: "+ this+ ", cache: "+ cacheBuffer);
  this.sink=sink;
  return sink;
}
