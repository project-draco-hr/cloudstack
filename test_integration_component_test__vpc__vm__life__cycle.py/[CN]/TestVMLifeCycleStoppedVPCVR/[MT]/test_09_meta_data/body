@attr(tags=['advanced', 'intervlan'])
def test_09_meta_data(self):
    ' Test meta data in virtual machines\n        '
    try:
        ssh = self.vm_1.get_ssh_client(ipaddress=self.public_ip_1.ipaddress.ipaddress)
        self.debug('SSH into VM is successfully')
    except Exception as e:
        self.fail('Failed to SSH into instance')
    routers = Router.list(self.apiclient, zoneid=self.zone.id, listall=True)
    self.assertEqual(isinstance(routers, list), True, 'Check list response returns a valid list')
    router = routers[0]
    self.debug('check the metadata with that of present in router')
    try:
        cmds = [('wget http://%s/latest/meta-data' % router.guestipaddress), 'cat user-data']
        for c in cmds:
            result = ssh.execute(c)
            self.debug(('%s: %s' % (c, result)))
    except Exception as e:
        self.fail(('Failed to SSH in Virtual machine: %s' % e))
    res = str(result)
    self.assertNotEqual(res, None, 'Meta data should be returned from router')
    return
