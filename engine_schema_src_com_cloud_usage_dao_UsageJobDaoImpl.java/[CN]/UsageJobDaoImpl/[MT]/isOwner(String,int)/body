{
  Transaction txn=Transaction.open(Transaction.USAGE_DB);
  try {
    if ((hostname == null) || (pid <= 0)) {
      return null;
    }
    UsageJobVO job=getLastJob();
    if (job == null) {
      return null;
    }
    if (hostname.equals(job.getHost()) && (job.getPid() != null) && (pid == job.getPid().intValue())) {
      return job;
    }
  }
  finally {
    txn.close();
  }
  return null;
}
