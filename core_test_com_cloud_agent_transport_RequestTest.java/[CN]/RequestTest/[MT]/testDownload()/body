{
  s_logger.info("Testing Download answer");
  VMTemplateVO template=new VMTemplateVO(1,"templatename",ImageFormat.QCOW2,true,true,true,TemplateType.USER,"url",true,32,1,"chksum","displayText",true,30,true,HypervisorType.KVM);
