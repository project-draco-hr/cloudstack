{
  if (scope.equals(PermissionScope.ACCOUNT)) {
    if (caller.getAccountId() == entity.getAccountId()) {
      return true;
    }
  }
 else   if (scope.equals(PermissionScope.DOMAIN)) {
    if (_domainDao.isChildDomain(caller.getDomainId(),entity.getDomainId())) {
      return true;
    }
  }
  return false;
}
