{
  try {
    this.stateMachine.transitTo(vo,event,null,templateStoreDao);
    vo=templateStoreDao.findById(vo.getId());
  }
 catch (  NoTransitionException e) {
    throw new CloudRuntimeException("Failed change state",e);
  }
catch (  Exception e) {
    throw new CloudRuntimeException("Failed change state",e);
  }
}
