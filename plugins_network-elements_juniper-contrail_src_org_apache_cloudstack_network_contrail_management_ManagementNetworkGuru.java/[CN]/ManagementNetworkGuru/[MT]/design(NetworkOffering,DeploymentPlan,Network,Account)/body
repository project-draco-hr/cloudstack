{
  if (!canHandle(offering)) {
    return null;
  }
  NetworkVO network=new NetworkVO(offering.getTrafficType(),Mode.Dhcp,BroadcastDomainType.Lswitch,offering.getId(),Network.State.Allocated,plan.getDataCenterId(),plan.getPhysicalNetworkId());
  if (_mgmt_cidr != null) {
    network.setCidr(_mgmt_cidr);
    network.setGateway(_mgmt_gateway);
  }
  s_logger.debug("Allocated network " + userSpecified.getName() + (network.getCidr() == null ? "" : " subnet: " + network.getCidr()));
  return network;
}
