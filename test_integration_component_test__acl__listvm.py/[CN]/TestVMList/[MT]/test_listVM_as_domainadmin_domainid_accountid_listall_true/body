@attr(tags=['advanced'], required_hardware='false')
def test_listVM_as_domainadmin_domainid_accountid_listall_true(self):
    '\n\t# Test listing of Vms by passing account ,domainId and listall="true" parameter as domain admin\n\t# Validate that it returns all the Vms owned by the account passed in account parameter\n\t'
    self.apiclient.connection.apiKey = self.user_d1_apikey
    self.apiclient.connection.securityKey = self.user_d1_secretkey
    vmList = VirtualMachine.list(self.apiclient, account=self.account_d11.user[0].username, domainid=self.domain_11.id, listall='true')
    self.debug(('List as Domain Admin passing domainId and accountId - listall=true %s' % vmList))
    self.assertEqual((len(vmList) == 1), True, 'Number of items in list response check failed!!')
    if self.checkForExistenceOfValue(vmList, self.vm_d11.id):
        accountAccess = True
    else:
        accountAccess = False
    self.assertEqual(accountAccess, True, 'Account access check failed!!')
