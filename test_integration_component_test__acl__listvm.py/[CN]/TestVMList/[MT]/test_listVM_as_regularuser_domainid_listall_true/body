@attr('simulator_only', tags=['advanced'], required_hardware='false')
def test_listVM_as_regularuser_domainid_listall_true(self):
    '\n\t# Test listing of Vms by passing domainid,listall="true" parameter as regular user\n\t# Validate that it returns all the Vms of the account the user belongs to \n\t'
    self.apiclient.connection.apiKey = self.user_d1a_apikey
    self.apiclient.connection.securityKey = self.user_d1a_secretkey
    vmList = VirtualMachine.list(self.apiclient, domainid=self.domain_1.id, listall='true')
    self.debug(('List as Regular User passing domainId  - listall=true %s' % vmList))
    self.assertEqual((len(vmList) == 1), True, 'Number of items in list response check failed!!')
    if self.checkForExistenceOfValue(vmList, self.vm_d1a.id):
        accountAccess = True
    else:
        accountAccess = False
    self.assertEqual(accountAccess, True, 'Account access check failed!!')
