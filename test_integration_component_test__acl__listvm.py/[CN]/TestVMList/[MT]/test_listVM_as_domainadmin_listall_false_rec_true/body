@attr(tags=['advanced'], required_hardware='false')
def test_listVM_as_domainadmin_listall_false_rec_true(self):
    '\n\t# Test listing of Vms by passing listall="false" and isrecusrive="true" parameter as domain admin\n\t# Validate that it returns all the Vms that is owned by the domain admin\n\t'
    self.apiclient.connection.apiKey = self.user_d1_apikey
    self.apiclient.connection.securityKey = self.user_d1_secretkey
    vmList = VirtualMachine.list(self.apiclient, listall='false', isrecursive='true')
    self.debug(('List as Domain Admin  - listall=false,isrecursive=true %s' % vmList))
    self.assertEqual((len(vmList) == 1), True, 'Number of items in list response check failed!!')
    if self.checkForExistenceOfValue(vmList, self.vm_d1.id):
        accountAccess = True
    else:
        accountAccess = False
    self.assertEqual(accountAccess, True, 'Account access check failed!!')
