{
  if (events != null && !events.isEmpty()) {
    Transaction txn=Transaction.currentTxn();
    txn.start();
    for (    EventVO event : events) {
      event=lockRow(event.getId(),true);
      event.setArchived(true);
      update(event.getId(),event);
      txn.commit();
    }
    txn.close();
  }
}
