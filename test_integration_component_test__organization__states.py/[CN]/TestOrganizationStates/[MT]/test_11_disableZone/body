@attr('disruptive', 'simulator_only', tags=['advanced'], required_hardware='false')
def test_11_disableZone(self):
    '\n\tDisable Zone\n        Validate that listZones() returns the  allocationstate as "Disabled"\n\t'
    self.debug(('Zone to be disabled: ' + self.zone.id))
    zoneupdResp = self.zone.update(self.apiclient, allocationstate='Disabled')
    self.assertEqual(zoneupdResp.allocationstate, 'Disabled', 'Disabling Zone did not set the alloctionstate to Disabled')
    zonelistResp = Zone.list(self.apiclient, id=self.zone.id)
    self.assertEqual(zonelistResp[0].allocationstate, 'Disabled', 'Disabling Zone did not set the alloctionstate to Disabled')
