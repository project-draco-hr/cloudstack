{
  if (eventType.startsWith("DOMAIN.")) {
    return Domain.class;
  }
 else   if (eventType.startsWith("ACCOUNT.")) {
    return Account.class;
  }
 else   if (eventType.startsWith("USER.")) {
    return User.class;
  }
 else   if (eventType.startsWith("VM.")) {
    return VirtualMachine.class;
  }
  return null;
}
