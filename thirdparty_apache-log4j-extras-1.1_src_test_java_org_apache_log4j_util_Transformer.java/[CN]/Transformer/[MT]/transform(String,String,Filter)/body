{
  String line;
  BufferedReader input=new BufferedReader(new FileReader(in));
  PrintStream output=new PrintStream(new FileOutputStream(out));
  try {
    while ((line=input.readLine()) != null) {
      line=filter.filter(line);
      output.println(line);
    }
  }
  finally {
    input.close();
    output.close();
  }
}
