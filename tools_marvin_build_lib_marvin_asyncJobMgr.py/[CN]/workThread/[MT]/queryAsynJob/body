def queryAsynJob(self, job):
    if (job.jobId is None):
        return job
    try:
        try:
            self.lock.acquire()
            result = self.connection.poll(job.jobId, job.responsecls).jobresult
        except cloudstackException.cloudstackAPIException as e:
            result = str(e)
    finally:
        self.lock.release()
    job.result = result
    return job
