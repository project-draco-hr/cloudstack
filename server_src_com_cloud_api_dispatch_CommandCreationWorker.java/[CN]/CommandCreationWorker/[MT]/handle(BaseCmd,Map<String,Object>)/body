{
  if (cmd instanceof BaseAsyncCreateCmd) {
    try {
      ((BaseAsyncCreateCmd)cmd).create();
    }
 catch (    final ResourceAllocationException e) {
      throw new ServerApiException(ApiErrorCode.RESOURCE_ALLOCATION_ERROR,e.getMessage(),e);
    }
  }
 else {
    throw new ServerApiException(ApiErrorCode.INTERNAL_ERROR,"Trying to invoke creation on a Command that is not  BaseAsyncCreateCmd");
  }
  return true;
}
