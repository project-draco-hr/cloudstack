{
  if (!(obj instanceof SuccessResponse) && !(obj instanceof ExceptionResponse)) {
    sb.append("<").append(obj.getObjectName()).append(">");
  }
  serializeResponseObjFieldsXML(sb,obj);
  if (!(obj instanceof SuccessResponse) && !(obj instanceof ExceptionResponse)) {
    sb.append("</").append(obj.getObjectName()).append(">");
  }
}
