{
synchronized (this) {
    for (Enumeration e=connections.elements(); e.hasMoreElements(); ) {
      try {
        ((Socket)e.nextElement()).close();
      }
 catch (      InterruptedIOException ex) {
        Thread.currentThread().interrupt();
      }
catch (      IOException ex) {
      }
catch (      RuntimeException ex) {
      }
    }
  }
  try {
    serverSocket.close();
  }
 catch (  InterruptedIOException ex) {
    Thread.currentThread().interrupt();
  }
catch (  IOException ex) {
  }
catch (  RuntimeException ex) {
  }
}
