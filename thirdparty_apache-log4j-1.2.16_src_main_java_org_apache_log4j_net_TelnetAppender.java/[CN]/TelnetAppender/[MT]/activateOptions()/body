{
  try {
    sh=new SocketHandler(port);
    sh.start();
  }
 catch (  InterruptedIOException e) {
    Thread.currentThread().interrupt();
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
catch (  RuntimeException e) {
    e.printStackTrace();
  }
  super.activateOptions();
}
