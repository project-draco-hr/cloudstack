{
  if (!allChecked && !checking) {
    MetaInfo meta=storage.getMetaInfo();
    if (meta != null) {
      log.log(Level.INFO,"Checking existing " + meta.getPieces() + " pieces: ");
    }
    checking=true;
  }
  if (checking) {
    if (checked) {
      System.err.print("+");
    }
 else {
      System.err.print("-");
    }
  }
 else {
    log.log(Level.FINE,"Got " + (checked ? "" : "BAD ") + "piece: "+ num);
  }
}
