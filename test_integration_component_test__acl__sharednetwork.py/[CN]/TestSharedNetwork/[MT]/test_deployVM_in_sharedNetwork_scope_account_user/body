@attr(tags=['advanced'], required_hardware='false')
def test_deployVM_in_sharedNetwork_scope_account_user(self):
    '\n\tValidate that regular user in the account is allowed to deploy VM in a shared network created with scope="account" for an account\n\t'
    self.apiclient.connection.apiKey = self.user_d111a_apikey
    self.apiclient.connection.securityKey = self.user_d111a_secretkey
    self.vmdata['name'] = (self.acldata['vmD111A']['name'] + '-shared-scope-domain-withsubdomainaccess')
    self.vmdata['displayname'] = (self.acldata['vmD111A']['displayname'] + '-shared-scope-domain-withsubdomainaccess')
    vm = VirtualMachine.create(self.apiclient, self.vmdata, zoneid=self.zone.id, serviceofferingid=self.service_offering.id, templateid=self.template.id, networkids=self.shared_network_account_d111a.id)
    self.assertEqual((vm.state == 'Running'), True, 'User in the account that has a shared network with scope=account failed to deploy a VM in this shared network')
