{
  Method targetMethod=method.getMethod();
  if (needToIntercept(targetMethod,method.getThis())) {
    Transaction txn=Transaction.open(targetMethod.getName());
    Object ret=null;
    try {
      ret=method.proceed();
    }
  finally {
      txn.close();
    }
    return ret;
  }
  return method.proceed();
}
