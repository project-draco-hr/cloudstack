{
  MethodSignature methodSignature=(MethodSignature)call.getSignature();
  Method targetMethod=methodSignature.getMethod();
  if (needToIntercept(targetMethod)) {
    Transaction txn=Transaction.open(call.getSignature().getName());
    Object ret=null;
    try {
      ret=call.proceed();
    }
  finally {
      txn.close();
    }
    return ret;
  }
  return call.proceed();
}
