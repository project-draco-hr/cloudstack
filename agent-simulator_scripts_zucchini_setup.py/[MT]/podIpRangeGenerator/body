def podIpRangeGenerator():
    x = 1
    y = 2
    while 1:
        if (y == 256):
            x = (x + 1)
            if (x == 256):
                break
            y = 1
        y = (y + 1)
        yield ((((('172.' + str(x)) + '.') + str(y)) + '.129'), (((('172.' + str(x)) + '.') + str(y)) + '.130'), (((('172.' + str(x)) + '.') + str(y)) + '.189'))
