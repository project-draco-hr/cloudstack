def generateCodeFromJSON(self, apiJson):
    '\n        Api Discovery plugin returns the supported APIs of a CloudStack endpoint.\n        @return: The classes in cloudstackAPI/ formed from api discovery json\n        '
    with open(apiJson, 'r') as apiStream:
        cmds = self.loadCmdFromJSON(apiStream)
        for cmd in cmds:
            self.generate(cmd)
        self.finalize()
