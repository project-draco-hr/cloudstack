{
  try {
    lock();
    checkPreconditions();
    final List<DeployDestination> destinations=findDestinations();
    for (    final DeployDestination destination : destinations) {
      dest=destination;
      planDeploymentRouters();
      generateDeploymentPlan();
      executeDeployment();
    }
  }
  finally {
    unlock();
  }
}
