{
  try {
    String xmlFilePath=Script.findScript(scriptsDir,filename);
    if (xmlFilePath == null) {
      throw new Exception("Failed to find Juniper SRX XML file: " + filename);
    }
    try (InputStreamReader fr=new InputStreamReader(new FileInputStream(xmlFilePath),"UTF-8");BufferedReader br=new BufferedReader(fr)){
      String xml="";
      String line;
      while ((line=br.readLine()) != null) {
        xml+=line.trim();
      }
      return xml;
    }
 catch (    Exception e) {
      s_logger.debug(e);
      return null;
    }
  }
 catch (  Exception e) {
    s_logger.debug(e);
    return null;
  }
}
