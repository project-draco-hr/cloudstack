def isCopyRightLine(self, txt):
    words = [c.strip().strip('.').strip('\n').strip(',') for c in txt.split(' ')]
    total = len(words)
    if (total == 0):
        return False
    numKeyWord = 0
    for w in words:
        if (w in self.keywords):
            numKeyWord += 1
    if ((float(numKeyWord) / float(total)) > (float(1) / float(3))):
        return True
    return False
