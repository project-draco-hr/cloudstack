def cleanBlankComment(self):
    newBody = []
    for l in self.fileBody:
        if (self.isCommentLine(l) and (l.strip().strip(self.COMMENT_NOTATION).strip().strip('\n') == '')):
            Logger.debug(('Blank Comment: %s' % l))
            continue
        newBody.append(l)
    self.fileBody = newBody
