{
  Rule rule=(Rule)SerializationTestHelper.serializeClone(LevelInequalityRule.getRule(">=","finer"));
  LoggingEvent event=new LoggingEvent("org.apache.log4j.Logger",Logger.getRootLogger(),System.currentTimeMillis(),UtilLoggingLevel.FINER,"Hello, World",null);
  assertTrue(rule.evaluate(event,null));
}
