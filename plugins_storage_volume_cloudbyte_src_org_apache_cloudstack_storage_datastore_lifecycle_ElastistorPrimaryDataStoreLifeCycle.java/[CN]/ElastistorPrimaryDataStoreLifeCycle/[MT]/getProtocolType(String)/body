{
  StringTokenizer st=new StringTokenizer(url,":");
  while (st.hasMoreElements()) {
    String accessprotocol=st.nextElement().toString();
    if (accessprotocol.contentEquals("nfs")) {
      return "nfs";
    }
 else     if (accessprotocol.contentEquals("iscsi")) {
      return "iscsi";
    }
 else     break;
  }
  return null;
}
