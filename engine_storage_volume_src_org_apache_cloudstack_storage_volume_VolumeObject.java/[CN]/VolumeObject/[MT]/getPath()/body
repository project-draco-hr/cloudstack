{
  if (this.dataStore.getRole() == DataStoreRole.Primary) {
    return this.volumeVO.getPath();
  }
 else {
    DataObjectInStore objInStore=this.objectInStoreMgr.findObject(this,dataStore);
    if (objInStore != null) {
      return objInStore.getInstallPath();
    }
 else {
      return null;
    }
  }
}
