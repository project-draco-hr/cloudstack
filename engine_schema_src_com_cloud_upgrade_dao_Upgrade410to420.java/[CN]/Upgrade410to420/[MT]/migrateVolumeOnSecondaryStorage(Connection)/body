{
  PreparedStatement sql=null;
  try {
    sql=conn.prepareStatement("update `cloud`.`volumes` set state='Uploaded' where state='UploadOp'");
    sql.executeUpdate();
  }
 catch (  SQLException e) {
    throw new CloudRuntimeException("Failed to upgrade volume state: ",e);
  }
 finally {
    if (sql != null) {
      try {
        sql.close();
      }
 catch (      SQLException e) {
      }
    }
  }
}
