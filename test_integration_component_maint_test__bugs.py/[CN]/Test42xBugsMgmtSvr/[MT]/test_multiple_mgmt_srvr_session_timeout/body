@attr(tags=['advanced', 'basic'])
@attr(required_hardware='false')
def test_multiple_mgmt_srvr_session_timeout(self):
    "\n        @Desc: Check whether mgmt server session times out with in 30s\n        @Steps:\n        Step1: run 'telnet localhot 8250' on the management server\n        and see that it times out with in 30seconds\n        "
    mgmt_ssh = SshClient(self.apiClient.connection.mgtSvr, 22, self.apiClient.connection.user, self.apiClient.connection.passwd)
    res = mgmt_ssh.execute('time telnet localhost 8250')
    return
