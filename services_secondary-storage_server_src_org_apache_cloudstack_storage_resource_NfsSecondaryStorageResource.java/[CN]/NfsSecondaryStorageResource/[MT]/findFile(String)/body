{
  File srcFile=_storage.getFile(path);
  if (!srcFile.exists()) {
    srcFile=_storage.getFile(path + ".qcow2");
    if (!srcFile.exists()) {
      srcFile=_storage.getFile(path + ".vhd");
      if (!srcFile.exists()) {
        srcFile=_storage.getFile(path + ".ova");
        if (!srcFile.exists()) {
          srcFile=_storage.getFile(path + ".vmdk");
          if (!srcFile.exists()) {
            return null;
          }
        }
      }
    }
  }
  return srcFile;
}
