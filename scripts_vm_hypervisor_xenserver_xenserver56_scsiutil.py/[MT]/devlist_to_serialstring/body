def devlist_to_serialstring(devlist):
    serial = ''
    for dev in devlist:
        try:
            devserial = ('scsi-%s' % getSCSIid(dev))
            if (not (len(devserial) > 0)):
                continue
            if len(serial):
                serial += ','
            serial += devserial
        except:
            pass
    return serial
