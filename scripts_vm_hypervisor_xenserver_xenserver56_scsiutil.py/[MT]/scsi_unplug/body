def scsi_unplug(vdipath):
    dev = getdev(vdipath)
    util.SMlog(('unplug path %s dev %s' % (vdipath, dev)))
    path = ('/sys/block/%s/device/delete' % dev)
    if os.path.exists(path):
        try:
            f = open(path, 'w')
            f.write('1')
            f.close()
            time.sleep(2)
        except:
            pass
