def getuniqueserial(path):
    dev = getdev(path)
    output = gen_rdmfile()
    try:
        cmd = ['scsi_id', '-g', '-s', ('/block/%s' % dev)]
        text = util.pread2(cmd)
        cmd = ['md5sum']
        txt = util.pread3(cmd, text)
        return txt.split(' ')[0]
    except:
        return ''
