{
  DataStore destStore=destData.getDataStore();
  DataStore srcStore=srcData.getDataStore();
  if (destStore.getRole() == DataStoreRole.Image || destStore.getRole() == DataStoreRole.ImageCache || srcStore.getRole() == DataStoreRole.Image || srcStore.getRole() == DataStoreRole.ImageCache) {
    return true;
  }
  return true;
}
