{
  try {
    if (clientSocket != null) {
      s_logger.info("Closing connection to " + clientSocket.getInetAddress());
      clientSocket.close();
      clientSocket=null;
    }
  }
 catch (  IOException ioe) {
  }
  try {
    if (clientIns != null) {
      clientIns.close();
      clientIns=null;
    }
  }
 catch (  IOException ioe) {
  }
  try {
    if (clientOuts != null) {
      clientOuts.close();
      clientOuts=null;
    }
  }
 catch (  IOException ioe) {
  }
}
