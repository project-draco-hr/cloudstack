{
  LOG.info("Entered the sort method.");
  for (int i=intArray.length - 1; i >= 0; i--) {
    NDC.push("i=" + i);
    OUTER.debug("in outer loop.");
    for (int j=0; j < i; j++) {
      NDC.push("j=" + j);
      INNER.debug("in inner loop.");
      if (intArray[j] > intArray[j + 1])       swap(j,j + 1);
      NDC.pop();
    }
    NDC.pop();
  }
}
