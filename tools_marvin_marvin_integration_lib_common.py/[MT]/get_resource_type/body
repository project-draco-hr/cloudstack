def get_resource_type(resource_id):
    'Returns resource type'
    lookup = {0: 'VM', 1: 'Public IP', 2: 'Volume', 3: 'Snapshot', 4: 'Template', 5: 'Projects', 6: 'Network', 7: 'VPC', 8: 'CPUs', 9: 'RAM', 10: 'Primary (shared) storage (Volumes)', 11: 'Secondary storage (Snapshots, Templates & ISOs)', }
    return lookup[resource_id]
