{
  int pix;
  if (bytesPixel == 1) {
    byte[] buf=new byte[count];
    is.readBytes(buf,0,count);
    for (int i=0; i < count; i++) {
      dst[i]=(int)buf[i] & 0xFF;
    }
  }
 else {
    byte[] buf=new byte[count * 3];
    is.readBytes(buf,0,count * 3);
    for (int i=0; i < count; i++) {
      dst[i]=((buf[i * 3 + 2] & 0xFF) << 16 | (buf[i * 3 + 1] & 0xFF) << 8 | (buf[i * 3] & 0xFF));
    }
  }
}
