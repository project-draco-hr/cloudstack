{
  Object src, dst;
  if (bytesPixel == 1) {
    src=zrleTilePixels8;
    dst=pixels8;
  }
 else {
    src=zrleTilePixels24;
    dst=pixels24;
  }
  int offsetSrc=0;
  int offsetDst=(y * rfb.framebufferWidth + x);
  for (int j=0; j < h; j++) {
    System.arraycopy(src,offsetSrc,dst,offsetDst,w);
    offsetSrc+=w;
    offsetDst+=rfb.framebufferWidth;
  }
  handleUpdatedPixels(x,y,w,h);
}
