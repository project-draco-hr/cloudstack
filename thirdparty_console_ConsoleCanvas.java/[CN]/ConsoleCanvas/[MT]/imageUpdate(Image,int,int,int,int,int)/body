{
  if ((infoflags & (ALLBITS | ABORT)) == 0) {
    return true;
  }
 else {
    if ((infoflags & ALLBITS) != 0) {
      if (jpegRect != null) {
synchronized (jpegRect) {
          memGraphics.drawImage(img,jpegRect.x,jpegRect.y,null);
          scheduleRepaint(jpegRect.x,jpegRect.y,jpegRect.width,jpegRect.height);
          jpegRect.notify();
        }
      }
    }
    return false;
  }
}
