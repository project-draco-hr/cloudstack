{
  final String lPath=determineSnapshotLocalDirectory(secondaryStorageUrl,accountId,volumeId) + File.pathSeparator + (deleteAllFlag ? "*" : "*" + name + "*");
  final String result=deleteLocalFile(lPath);
  if (result != null) {
    return "failed to delete snapshot " + lPath + " , err="+ result;
  }
  return null;
}
