def __init__(self):
    self.isAsync = 'true'
    'Template ID.'
    'Required'
    self.id = None
    'ID of the zone the template is being copied to.'
    'Required'
    self.destzoneid = None
    'ID of the zone the template is currently hosted on.'
    'Required'
    self.sourcezoneid = None
    self.required = ['id', 'destzoneid', 'sourcezoneid']
