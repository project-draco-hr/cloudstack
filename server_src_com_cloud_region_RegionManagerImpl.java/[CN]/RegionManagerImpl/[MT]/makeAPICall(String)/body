{
  try {
    HttpClient client=new HttpClient();
    HttpMethod method=new GetMethod(url);
    if (client.executeMethod(method) == 200) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  HttpException e) {
    s_logger.error(e.getMessage());
    return false;
  }
catch (  IOException e) {
    s_logger.error(e.getMessage());
    return false;
  }
}
