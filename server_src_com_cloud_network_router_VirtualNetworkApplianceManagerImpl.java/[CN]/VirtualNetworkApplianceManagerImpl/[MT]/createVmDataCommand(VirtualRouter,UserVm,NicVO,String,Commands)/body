{
  String serviceOffering=_serviceOfferingDao.findByIdIncludingRemoved(vm.getServiceOfferingId()).getDisplayText();
  String zoneName=_dcDao.findById(router.getDataCenterIdToDeployIn()).getName();
  cmds.addCommand("vmdata",generateVmDataCommand(router,nic.getIp4Address(),vm.getUserData(),serviceOffering,zoneName,nic.getIp4Address(),vm.getHostName(),vm.getUuid(),publicKey,nic.getNetworkId()));
}
