{
  final State newState=transition.getToState();
  final VirtualMachine.Event event=transition.getEvent();
  if (vo.getType() == VirtualMachine.Type.DomainRouter && event == VirtualMachine.Event.FollowAgentPowerOnReport && newState == State.Running && isOutOfBandMigrated(opaque)) {
    logger.debug("Virtual router " + vo.getInstanceName() + " is powered-on out-of-band");
  }
  return true;
}
