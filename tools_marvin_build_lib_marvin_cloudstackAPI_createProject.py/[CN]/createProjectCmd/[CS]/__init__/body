def __init__(self):
    self.isAsync = 'true'
    'display text of the project'
    'Required'
    self.displaytext = None
    'name of the project'
    'Required'
    self.name = None
    'account who will be Admin for the project'
    self.account = None
    'domain ID of the account owning a project'
    self.domainid = None
    self.required = ['displaytext', 'name']
