{
  int b1_8=(b & 0x1) << 7;
  int b2_7=(b & 0x2) << 5;
  int b3_6=(b & 0x4) << 3;
  int b4_5=(b & 0x8) << 1;
  int b5_4=(b & 0x10) >>> 1;
  int b6_3=(b & 0x20) >>> 3;
  int b7_2=(b & 0x40) >>> 5;
  int b8_1=(b & 0x80) >>> 7;
  byte c=(byte)(b1_8 | b2_7 | b3_6| b4_5| b5_4| b6_3| b7_2| b8_1);
  return c;
}
