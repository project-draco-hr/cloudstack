{
  this.setUuid(propUuid);
  this.setSsUuid(propUuid);
  this.setMntUuid(mntUuid);
  this.setFsHost(nfsHost);
  this.setFsSourcePath(nfsHost + ":" + nfsPath);
  this.setFsType("FileSys");
  Map<String,Object> props=(HashMap<String,Object>)callWrapper("storage_plugin_getFileSystemInfo",this.pluginType,this.baseProps,this.ssProps);
  this.ssProps=props;
  if (props == null)   return false;
  return true;
}
