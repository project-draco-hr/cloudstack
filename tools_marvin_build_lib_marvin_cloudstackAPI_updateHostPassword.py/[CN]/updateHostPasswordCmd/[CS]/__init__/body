def __init__(self):
    self.isAsync = 'false'
    'the new password for the host/cluster'
    'Required'
    self.password = None
    'the username for the host/cluster'
    'Required'
    self.username = None
    'the cluster ID'
    self.clusterid = None
    'the host ID'
    self.hostid = None
    self.required = ['password', 'username']
