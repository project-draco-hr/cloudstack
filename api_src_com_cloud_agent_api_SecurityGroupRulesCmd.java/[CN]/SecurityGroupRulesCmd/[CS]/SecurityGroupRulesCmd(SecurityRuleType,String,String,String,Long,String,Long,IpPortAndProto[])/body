{
  super();
  this.guestIp=guestIp;
  this.vmName=vmName;
  this.ruleSet=ruleSet;
  this.guestMac=guestMac;
  this.signature=signature;
  this.seqNum=seqNum;
  this.vmId=vmId;
  this.ruleType=ruleType;
  if (signature == null) {
    String stringified=stringifyRules();
    this.signature=DigestUtils.md5Hex(stringified);
  }
}
