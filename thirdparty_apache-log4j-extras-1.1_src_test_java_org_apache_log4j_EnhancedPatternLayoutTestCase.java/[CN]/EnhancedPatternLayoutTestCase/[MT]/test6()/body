{
  configure("input/pattern/enhancedPatternLayout6.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT3,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new LineNumberFilter(),new AbsoluteTimeFilter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.6"));
}
