{
  configure("input/pattern/enhancedPatternLayout4.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT2,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new LineNumberFilter(),new AbsoluteDateAndTimeFilter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.4"));
}
