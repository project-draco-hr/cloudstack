{
  configure("input/pattern/enhancedPatternLayout8.properties");
  common();
  ControlFilter cf1=new ControlFilter(new String[]{PAT4,EXCEPTION1,EXCEPTION2,EXCEPTION3});
  Transformer.transform(TEMP,FILTERED,new Filter[]{cf1,new LineNumberFilter(),new RelativeTimeFilter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(compare(FILTERED,"witness/pattern/enhancedPatternLayout.8"));
}
