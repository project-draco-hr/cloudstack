{
  closed=true;
  if (sqw != null) {
    try {
      if (layoutHeaderChecked && layout != null && layout.getFooter() != null) {
        sendLayoutMessage(layout.getFooter());
      }
      sqw.close();
      sqw=null;
    }
 catch (    java.io.InterruptedIOException e) {
      Thread.currentThread().interrupt();
      sqw=null;
    }
catch (    IOException e) {
      sqw=null;
    }
  }
}
