{
  try {
    while (connectionAlive) {
      ClientPacket packet=queue.poll(1,TimeUnit.SECONDS);
      if (packet != null) {
        packet.write(os);
        os.flush();
      }
    }
  }
 catch (  Throwable e) {
    if (connectionAlive) {
      closeConnection();
      vncConnection.shutdown();
    }
  }
}
