{
  XSLTLayout xmlLayout=new XSLTLayout();
  xmlLayout.setLocationInfo(true);
  root.addAppender(new FileAppender(xmlLayout,TEMP,false));
  common();
  Transformer.transform(TEMP,FILTERED,new Filter[]{new LineNumberFilter(),new JunitTestRunnerFilter(),new XMLTimestampFilter(),new XMLLineAttributeFilter(),new SunReflectFilter(),new XMLDateFilter()});
  assertTrue(Compare.compare(XSLTLayoutTestCase.class,FILTERED,"witness/xml/xsltLayout.2"));
}
