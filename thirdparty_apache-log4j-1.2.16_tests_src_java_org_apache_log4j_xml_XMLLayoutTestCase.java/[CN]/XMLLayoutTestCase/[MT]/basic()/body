{
  XMLLayout xmlLayout=new XMLLayout();
  root.addAppender(new FileAppender(xmlLayout,TEMP,false));
  common();
  Transformer.transform(TEMP,FILTERED,new Filter[]{new LineNumberFilter(),new XMLTimestampFilter(),new JunitTestRunnerFilter(),new SunReflectFilter()});
  assertTrue(Compare.compare(FILTERED,"witness/xmlLayout.1"));
}
