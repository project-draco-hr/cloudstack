def _check_size_and_iops(self, sf_volume, volume, size):
    self.assertEqual(sf_volume['qos']['minIOPS'], volume.miniops, ('Check QOS - Min IOPS: ' + str(sf_volume['qos']['minIOPS'])))
    self.assertEqual(sf_volume['qos']['maxIOPS'], volume.maxiops, ('Check QOS - Max IOPS: ' + str(sf_volume['qos']['maxIOPS'])))
    self.assertEqual(sf_volume['totalSize'], size, ('Check SF volume size: ' + str(sf_volume['totalSize'])))
