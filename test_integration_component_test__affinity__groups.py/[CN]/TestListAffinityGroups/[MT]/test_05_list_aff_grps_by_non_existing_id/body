def test_05_list_aff_grps_by_non_existing_id(self):
    '\n            List Affinity Groups by non-existing id\n        '
    self.create_aff_grp(aff_grp=self.services['host_anti_affinity_0'])
    list_aff_grps = AffinityGroup.list(self.api_client, id=1234)
    self.assertEqual(list_aff_grps, None, 'Listing Affinity Group by non-existing id succeeded.')
    AffinityGroup.delete(self.api_client, self.aff_grp[0].name)
