{
  Transaction txn=null;
  MultiPartUploadsVO uploadvo=null;
  try {
    txn=Transaction.open(Transaction.AWSAPI_DB);
    uploadvo=findById(new Long(uploadid));
    if (null != uploadvo) {
      if (attribute.equalsIgnoreCase("AccessKey"))       return uploadvo.getAccessKey();
 else       if (attribute.equalsIgnoreCase("x_amz_acl"))       return uploadvo.getAmzAcl();
    }
    return null;
  }
  finally {
    txn.close();
  }
}
