def raisealert(severity, msg, process_name=None):
    if (process_name is not None):
        log = ((((((((('[' + severity) + ']') + ' ') + '[') + process_name) + ']') + ' ') + msg) + '\n')
    else:
        log = ((((('[' + severity) + ']') + ' ') + msg) + '\n')
    msg = ('logger -t monit ' + log)
    pout = Popen(msg, shell=True, stdout=PIPE)
