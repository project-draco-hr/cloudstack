{
  if (!services.contains(Service.SourceNat)) {
    if (services.contains(Service.StaticNat) || services.contains(Service.Firewall) || services.contains(Service.Lb)|| services.contains(Service.PortForwarding)|| services.contains(Service.Vpn)) {
      String servicesList="[";
      for (      Service service : services) {
        servicesList+=service.getName() + " ";
      }
      servicesList+="]";
      s_logger.warn("Virtual router can't enable services " + servicesList + " without source NAT service");
      return false;
    }
  }
  return true;
}
