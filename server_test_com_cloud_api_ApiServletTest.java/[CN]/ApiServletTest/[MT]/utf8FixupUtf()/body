{
  Mockito.when(request.getQueryString()).thenReturn(URLEncoder.encode("??????????????????","UTF-8") + "=" + URLEncoder.encode("??rv??zt??r??t??k??rf??r??","UTF-8"));
  HashMap<String,Object[]> params=new HashMap<String,Object[]>();
  servlet.utf8Fixup(request,params);
  Assert.assertEquals("??rv??zt??r??t??k??rf??r??",params.get("??????????????????")[0]);
}
