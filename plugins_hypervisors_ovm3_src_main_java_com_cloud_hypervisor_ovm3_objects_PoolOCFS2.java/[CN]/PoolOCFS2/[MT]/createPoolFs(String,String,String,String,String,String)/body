{
  if (!hasAPoolFs()) {
    return nullIsTrueCallWrapper("create_pool_filesystem",type,target,clustername,fsid,nfsbaseid,managerid,fsid);
  }
 else   if (hasPoolFs(fsid)) {
    LOGGER.debug("PoolFs already exists on this host: " + fsid);
    return true;
  }
 else {
    throw new Ovm3ResourceException("Unable to add pool filesystem to host, " + "pool filesystem with other id found: " + poolFsId);
  }
}
