{
  this(name,cpu,ramSize,speed,rateMbps,multicastRateMbps,offerHA,limitResourceUse,volatileVm,displayText,useLocalStorage,recreatable,tags,systemUse,vm_type,domainId,hostTag);
  if (deploymentPlanner != null) {
    this.deploymentPlanner=deploymentPlanner;
