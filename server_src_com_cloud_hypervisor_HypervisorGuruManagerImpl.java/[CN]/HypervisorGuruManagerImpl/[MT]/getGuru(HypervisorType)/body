{
  HypervisorGuru result=_hvGurus.get(hypervisorType);
  if (result == null) {
    for (    HypervisorGuru guru : _hvGuruList) {
      if (guru.getHypervisorType() == hypervisorType) {
        _hvGurus.put(hypervisorType,guru);
        result=guru;
        break;
      }
    }
  }
  return result;
}
