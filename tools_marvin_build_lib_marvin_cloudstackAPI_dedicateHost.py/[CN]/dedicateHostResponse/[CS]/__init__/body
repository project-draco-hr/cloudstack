def __init__(self):
    'the ID of the dedicated resource'
    self.id = None
    'the Account ID of the host'
    self.accountid = None
    'the Dedication Affinity Group ID of the host'
    self.affinitygroupid = None
    'the domain ID of the host'
    self.domainid = None
    'the ID of the host'
    self.hostid = None
    'the name of the host'
    self.hostname = None
