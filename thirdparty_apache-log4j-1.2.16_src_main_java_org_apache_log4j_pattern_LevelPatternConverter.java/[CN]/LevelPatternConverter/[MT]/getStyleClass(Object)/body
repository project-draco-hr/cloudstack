{
  if (e instanceof LoggingEvent) {
    int lint=((LoggingEvent)e).getLevel().toInt();
switch (lint) {
case TRACE_INT:
      return "level trace";
case Level.DEBUG_INT:
    return "level debug";
case Level.INFO_INT:
  return "level info";
case Level.WARN_INT:
return "level warn";
case Level.ERROR_INT:
return "level error";
case Level.FATAL_INT:
return "level fatal";
default :
return "level " + ((LoggingEvent)e).getLevel().toString();
}
}
return "level";
}
