@attr(tags=['advanced', 'intervlan'])
def test_02_network_services_VPC_CreatePF(self):
    ' Test case no 190 : Create PF rules for a single virtual network of a VPC using a\n                new Public IP Address available with the VPC when Virtual Router is in Running State\n        '
    network_1 = self.create_Network(self.services['network_offering'])
    vm_1 = self.create_VM_in_Network(network_1)
    public_ip_1 = self.acquire_Public_IP(network_1)
    self.create_NatRule_For_VM(vm_1, public_ip_1, network_1)
    self.check_ssh_into_vm(vm_1, public_ip_1, testnegative=False)
    return
