{
  if (this.ajaxViewer != ajaxViewer) {
    if (this.ajaxViewer) {
      this.ajaxSessionId++;
    }
 else {
      if (clientStream != null) {
        byte[] bs=new byte[2];
        bs[0]=(byte)250;
        bs[1]=1;
        writeToClientStream(bs);
        try {
          clientStream.close();
        }
 catch (        IOException e) {
        }
        clientStream=null;
      }
    }
    this.ajaxViewer=ajaxViewer;
  }
}
