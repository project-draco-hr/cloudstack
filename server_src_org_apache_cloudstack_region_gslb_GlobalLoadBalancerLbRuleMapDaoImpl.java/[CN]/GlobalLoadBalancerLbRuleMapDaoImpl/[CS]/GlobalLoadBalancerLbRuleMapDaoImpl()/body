{
  listByGslbRuleId=createSearchBuilder();
  listByGslbRuleId.and("gslbLoadBalancerId",listByGslbRuleId.entity().getGslbLoadBalancerId(),SearchCriteria.Op.EQ);
  listByGslbRuleId.done();
