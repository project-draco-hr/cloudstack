{
  StringBuilder sb=new StringBuilder();
  if (values != null) {
    boolean first=true;
    for (    String s : values) {
      if (s != null && s.length() > 0) {
        if (first)         first=false;
 else         sb.append(delim);
        sb.append(s);
      }
    }
  }
  return sb.toString();
}
