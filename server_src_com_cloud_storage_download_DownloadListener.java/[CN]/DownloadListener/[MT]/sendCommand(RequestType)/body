{
  if (getJobId() != null) {
    if (s_logger.isTraceEnabled()) {
      log("Sending progress command ",Level.TRACE);
    }
    long sent=downloadMonitor.send(sserver.getId(),new DownloadProgressCommand(getCommand(),getJobId(),reqType),this);
    if (sent == -1) {
      setDisconnected();
    }
  }
}
