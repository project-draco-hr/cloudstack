{
  if (!(cmd instanceof StartupStorageCommand)) {
    return true;
  }
  if (cmd.getGuid().startsWith("iso:")) {
    return true;
  }
  long agentId=agent.getId();
  StartupStorageCommand storage=(StartupStorageCommand)cmd;
  if (storage.getResourceType() == Volume.StorageResourceType.STORAGE_HOST || storage.getResourceType() == Volume.StorageResourceType.SECONDARY_STORAGE) {
    downloadMonitor.handleTemplateSync(agentId,storage.getTemplateInfo());
  }
 else {
  }
  return true;
}
