{
  GlobalLock scanLock=GlobalLock.getInternLock("ExternalDeviceNetworkUsageManagerImpl");
  try {
    if (scanLock.lock(20)) {
      try {
        runExternalDeviceNetworkUsageTask();
      }
  finally {
        scanLock.unlock();
      }
    }
  }
 catch (  Exception e) {
    logger.warn("Problems while getting external device usage",e);
  }
 finally {
    scanLock.releaseRef();
  }
}
