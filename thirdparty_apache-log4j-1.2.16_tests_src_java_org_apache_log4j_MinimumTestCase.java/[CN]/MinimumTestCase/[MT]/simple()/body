{
  Layout layout=new SimpleLayout();
  Appender appender=new FileAppender(layout,"output/simple",false);
  root.addAppender(appender);
  common();
  Transformer.transform("output/simple",FILTERED,new Filter[]{new LineNumberFilter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(Compare.compare(FILTERED,"witness/simple"));
}
