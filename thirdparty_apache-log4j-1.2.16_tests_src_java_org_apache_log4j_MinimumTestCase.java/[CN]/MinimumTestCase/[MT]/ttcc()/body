{
  Layout layout=new TTCCLayout(AbsoluteTimeDateFormat.DATE_AND_TIME_DATE_FORMAT);
  Appender appender=new FileAppender(layout,"output/ttcc",false);
  root.addAppender(appender);
  String oldName=Thread.currentThread().getName();
  Thread.currentThread().setName("main");
  common();
  Thread.currentThread().setName(oldName);
  ControlFilter cf1=new ControlFilter(new String[]{TTCC_PAT,TTCC2_PAT,EXCEPTION1,EXCEPTION2,EXCEPTION3,EXCEPTION4,EXCEPTION5});
  Transformer.transform("output/ttcc",FILTERED,new Filter[]{cf1,new LineNumberFilter(),new AbsoluteDateAndTimeFilter(),new SunReflectFilter(),new JunitTestRunnerFilter()});
  assertTrue(Compare.compare(FILTERED,"witness/ttcc"));
}
