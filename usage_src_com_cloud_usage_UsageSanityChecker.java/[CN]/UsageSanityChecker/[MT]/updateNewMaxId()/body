{
  FileWriter fstream=null;
  try {
    fstream=new FileWriter(lastCheckFile);
    BufferedWriter out=new BufferedWriter(fstream);
    out.write("" + maxId);
    out.close();
  }
 catch (  IOException e) {
  }
 finally {
    if (fstream != null) {
      try {
        fstream.close();
      }
 catch (      IOException e) {
        s_logger.error(e);
      }
    }
  }
}
