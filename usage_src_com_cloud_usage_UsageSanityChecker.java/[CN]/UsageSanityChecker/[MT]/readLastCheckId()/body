{
  BufferedReader reader=null;
  try {
    reader=new BufferedReader(new FileReader(lastCheckFile));
    String lastIdText=null;
    lastId=-1;
    if ((reader != null) && (lastIdText=reader.readLine()) != null) {
      lastId=Integer.parseInt(lastIdText);
    }
  }
 catch (  IOException e) {
    s_logger.error(e);
  }
 finally {
    try {
      reader.close();
    }
 catch (    IOException e) {
      s_logger.error(e);
    }
  }
}
