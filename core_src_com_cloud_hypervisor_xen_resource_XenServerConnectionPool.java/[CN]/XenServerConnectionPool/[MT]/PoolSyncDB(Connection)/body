{
  try {
    Set<Host> hosts=Host.getAll(conn);
    for (    Host host : hosts) {
      try {
        host.enable(conn);
      }
 catch (      Exception e) {
      }
    }
  }
 catch (  Exception e) {
    s_logger.debug("Enbale host failed due to " + e.getMessage() + e.toString());
  }
  try {
    Pool.syncDatabase(conn);
  }
 catch (  Exception e) {
    s_logger.debug("Sync Database failed due to " + e.getMessage() + e.toString());
  }
}
