def create_virtual_machine(self, network_id=None):
    virtual_machine = VirtualMachine.create(self.apiclient, self.services['virtual_machine'], networkids=network_id, serviceofferingid=self.service_offering.id, accountid=self.account.name, domainid=self.domain.id)
    self.debug(('Virtual Machine is created: ' + virtual_machine.id))
    return virtual_machine
