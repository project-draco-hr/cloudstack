{
  URI uri=new URI(url);
  Map<String,String> params=getParameters(uri);
  EncodedDataStore store=new EncodedDataStore(params.get(EncodingType.ROLE.toString()),params.get(EncodingType.STOREUUID.toString()),params.get(EncodingType.PROVIDERNAME.toString()),uri.getScheme(),uri.getScheme() + uri.getHost() + uri.getPath(),uri.getHost(),uri.getPath());
  Long size=null;
  try {
    size=Long.parseLong(params.get(EncodingType.SIZE.toString()));
  }
 catch (  NumberFormatException e) {
  }
  DecodedDataObject obj=new DecodedDataObject(params.get(EncodingType.OBJTYPE.toString()),size,params.get(EncodingType.NAME.toString()),params.get(EncodingType.PATH.toString()),store);
  return obj;
}
