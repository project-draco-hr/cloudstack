{
  while (snapshot != null) {
    SnapshotInfo child=snapshot.getChild();
    SnapshotInfo parent=snapshot.getParent();
    if (child == null) {
      if (parent == null || !parent.getPath().equalsIgnoreCase(snapshot.getPath())) {
        this.snapshotSvr.deleteSnapshot(snapshot);
        snapshot=parent;
        continue;
      }
      break;
    }
 else {
      break;
    }
  }
}
