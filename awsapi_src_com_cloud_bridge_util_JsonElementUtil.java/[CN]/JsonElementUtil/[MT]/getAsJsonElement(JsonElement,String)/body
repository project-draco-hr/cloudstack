{
  JsonElement currentElement=jsonElement;
  for (  final String propertyName : path) {
    if (currentElement == null) {
      return null;
    }
    if (jsonElement.isJsonObject()) {
      currentElement=((JsonObject)currentElement).get(propertyName);
    }
 else {
      return null;
    }
  }
  return currentElement;
}
