{
  Vpc vpc=_vpcService.createVpc(getZoneId(),getVpcOffering(),getEntityOwnerId(),getVpcName(),getDisplayText(),getCidr(),getNetworkDomain());
  if (vpc != null) {
    this.setEntityId(vpc.getId());
    this.setEntityUuid(vpc.getUuid());
  }
 else {
    throw new ServerApiException(BaseCmd.INTERNAL_ERROR,"Failed to create a VPC");
  }
}
