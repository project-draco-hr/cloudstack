def create_guest_shared_network_services(self):
    network_services = {'name': 'nicira_enabled_network_shared', 'displaytext': 'nicira_enabled_network_shared', 'zoneid': self.zone.id, 'networkoffering': self.network_offering_shared.id, 'startip': self.ip_ranges_shared.startip, 'endip': self.ip_ranges_shared.endip, 'netmask': self.ip_ranges_shared.netmask, 'gateway': self.ip_ranges_shared.gateway, }
    network = Network.create(self.api_client, network_services, accountid='admin', domainid=self.domain.id)
    self.test_cleanup.append(network)
    return network
