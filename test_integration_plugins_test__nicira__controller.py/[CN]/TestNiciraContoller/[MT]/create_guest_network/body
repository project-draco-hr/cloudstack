def create_guest_network(self):
    network_services = {'name': 'nicira_enabled_network', 'displaytext': 'nicira_enabled_network', 'zoneid': self.zone.id, 'networkoffering': self.network_offering.id, }
    network = Network.create(self.api_client, network_services, accountid='admin', domainid=self.domain.id)
    self.test_cleanup.append(network)
    return network
