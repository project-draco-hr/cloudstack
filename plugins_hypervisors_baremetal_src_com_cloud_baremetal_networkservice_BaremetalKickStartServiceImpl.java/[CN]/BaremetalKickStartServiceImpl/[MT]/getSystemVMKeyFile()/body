{
  URL url=this.getClass().getClassLoader().getResource("scripts/vm/systemvm/id_rsa.cloud");
  File keyFile=null;
  if (url != null) {
    keyFile=new File(url.getPath());
  }
  if (keyFile == null || !keyFile.canRead()) {
    s_logger.error("Unable to locate id_rsa.cloud");
    return null;
  }
  return keyFile;
}
