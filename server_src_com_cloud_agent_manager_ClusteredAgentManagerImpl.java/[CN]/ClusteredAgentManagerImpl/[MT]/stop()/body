{
  if (_peers != null) {
    for (    SocketChannel ch : _peers.values()) {
      try {
        s_logger.info("Closing: " + ch.toString());
        ch.close();
      }
 catch (      IOException e) {
      }
    }
  }
  _timer.cancel();
  s_transferExecutor.shutdownNow();
  cleanupTransferMap();
  return super.stop();
}
