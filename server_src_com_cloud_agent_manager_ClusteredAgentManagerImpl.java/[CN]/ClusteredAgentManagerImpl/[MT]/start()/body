{
  if (!super.start()) {
    return false;
  }
  _timer.schedule(new DirectAgentScanTimerTask(),STARTUP_DELAY,SCAN_INTERVAL);
  if (_clusterMgr.isAgentRebalanceEnabled()) {
    s_transferExecutor.scheduleAtFixedRate(getTransferScanTask(),ClusteredAgentRebalanceService.DEFAULT_TRANSFER_CHECK_INTERVAL,ClusteredAgentRebalanceService.DEFAULT_TRANSFER_CHECK_INTERVAL,TimeUnit.MILLISECONDS);
  }
  return true;
}
