{
  boolean res;
  if (!investigate) {
    res=super.handleDisconnectWithoutInvestigation(agent,event,true);
  }
 else {
    res=super.handleDisconnectWithInvestigation(agent,event);
  }
  if (res) {
    if (broadcast) {
      notifyNodesInCluster(agent);
    }
    return true;
  }
 else {
    return false;
  }
}
