{
  GlobalLock scanLock=GlobalLock.getInternLock(this.getClass().getName());
  try {
    if (scanLock.lock(ACQUIRE_GLOBAL_LOCK_TIMEOUT_FOR_COOPERATION)) {
      try {
        scanDirectAgentToLoad();
      }
  finally {
        scanLock.unlock();
      }
    }
  }
  finally {
    scanLock.releaseRef();
  }
}
