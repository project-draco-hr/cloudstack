{
  GlobalLock scanLock=GlobalLock.getInternLock("clustermgr.scan");
  try {
    if (scanLock.lock(ACQUIRE_GLOBAL_LOCK_TIMEOUT_FOR_COOPERATION)) {
      try {
        scanDirectAgentToLoad();
      }
  finally {
        scanLock.unlock();
      }
    }
  }
  finally {
    scanLock.releaseRef();
  }
}
