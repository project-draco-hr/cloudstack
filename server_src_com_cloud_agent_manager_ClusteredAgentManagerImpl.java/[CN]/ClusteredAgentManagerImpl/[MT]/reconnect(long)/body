{
  Boolean result;
  try {
    result=propagateAgentEvent(hostId,Event.ShutdownRequested);
    if (result != null) {
      return result;
    }
  }
 catch (  AgentUnavailableException e) {
    s_logger.debug("cannot propagate agent reconnect because agent is not available",e);
    return false;
  }
  return super.reconnect(hostId);
}
