{
  String pattern;
  if (resourceBundleName != null) {
    try {
      ResourceBundle bundle=ResourceBundle.getBundle(resourceBundleName);
      pattern=bundle.getString(key);
    }
 catch (    Exception ex) {
      pattern=key;
    }
  }
 else {
    pattern=key;
  }
  return format(pattern,arguments);
}
