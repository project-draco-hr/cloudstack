def __init__(self, cmd='', failcmd='', primary='', file='', timeout='120', interval=1, logger='', check=False):
    self.file = file
    self.cmd = cmd
    self.failcmd = failcmd
    self.primary = primary
    self.timeout = timeout
    self.interval = interval
    self.process = None
    self.logger = logger
    self.check = check
    self.ok = None
    self.results = {}
