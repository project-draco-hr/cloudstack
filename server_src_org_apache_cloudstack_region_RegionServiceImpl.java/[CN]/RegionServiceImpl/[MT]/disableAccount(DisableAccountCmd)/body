{
  Account result=null;
  if (checkIsPropagate(cmd.getIsPropagate())) {
    if (cmd.getLockRequested())     result=_accountMgr.lockAccount(cmd.getAccountName(),cmd.getDomainId(),cmd.getId());
 else     result=_accountMgr.disableAccount(cmd.getAccountName(),cmd.getDomainId(),cmd.getId());
  }
 else {
    result=_regionMgr.disableAccount(cmd.getAccountName(),cmd.getDomainId(),cmd.getId(),cmd.getLockRequested());
  }
  return result;
}
