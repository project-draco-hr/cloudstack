{
  try (PreparedStatement pstmt=conn.prepareStatement("SELECT domain_id FROM account WHERE id=?")){
    pstmt.setLong(1,accountId);
    try (ResultSet domainRs=pstmt.executeQuery()){
      Long domainId=1L;
      while (domainRs.next()) {
        domainId=domainRs.getLong(1);
      }
      return domainId;
    }
   }
 }
