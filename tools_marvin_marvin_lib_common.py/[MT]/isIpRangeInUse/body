def isIpRangeInUse(api_client, publicIpRange):
    ' Check that if any Ip in the IP Range is in use\n        currently\n    '
    vmList = VirtualMachine.list(api_client, zoneid=publicIpRange.zoneid, listall=True)
    if (not vmList):
        return False
    for vm in vmList:
        for nic in vm.nic:
            publicIpAddresses = PublicIPAddress.list(api_client, associatednetworkid=nic.networkid, listall=True)
            if (validateList(publicIpAddresses)[0] == PASS):
                for ipaddress in publicIpAddresses:
                    if (IPAddress(publicIpRange.startip) <= IPAddress(ipaddress.ipaddress) <= IPAddress(publicIpRange.endip)):
                        return True
    return False
