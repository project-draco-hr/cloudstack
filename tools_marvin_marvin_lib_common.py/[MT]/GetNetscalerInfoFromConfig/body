def GetNetscalerInfoFromConfig(config):
    ' Read netscaler data from config file and\n        return it\n\n    Input: Config\n    Output: [netscalerInfoDict, isExceptionOccured]\n    '
    try:
        netscalerInfo = config.__dict__['netscalerDevice'].__dict__
        return [netscalerInfo, None]
    except KeyError:
        exceptionMessage = 'Please make sure you have included netscalerDevice                    dict in your config file'
        return [None, exceptionMessage]
    except Exception as e:
        return [None, e]
