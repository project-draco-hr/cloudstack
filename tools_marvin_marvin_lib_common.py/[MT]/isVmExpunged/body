def isVmExpunged(apiclient, vmid, projectid=None, timeout=600):
    'Verify if VM is expunged or not'
    vmExpunged = False
    while (timeout >= 0):
        try:
            vms = VirtualMachine.list(apiclient, id=vmid, projectid=projectid)
            if (vms is None):
                vmExpunged = True
                break
            timeout -= 60
            time.sleep(60)
        except Exception:
            vmExpunged = True
            break
    return vmExpunged
