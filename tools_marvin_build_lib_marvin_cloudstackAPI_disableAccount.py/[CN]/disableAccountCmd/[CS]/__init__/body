def __init__(self):
    self.isAsync = 'true'
    'If true, only lock the account; else disable the account'
    'Required'
    self.lock = None
    'Disables specified account.'
    self.account = None
    'Disables specified account in this domain.'
    self.domainid = None
    'Account id'
    self.id = None
    self.required = ['lock']
