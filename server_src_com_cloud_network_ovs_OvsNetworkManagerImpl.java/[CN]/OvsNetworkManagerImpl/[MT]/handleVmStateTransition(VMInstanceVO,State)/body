{
  if (!_isEnabled) {
    return;
  }
switch (vmState) {
case Creating:
case Destroyed:
case Error:
case Migrating:
case Expunging:
case Starting:
case Unknown:
    return;
case Running:
  handleVmStateChange(instance);
break;
case Stopping:
case Stopped:
checkAndRemove(instance);
handleVmStateChange(instance);
break;
}
}
