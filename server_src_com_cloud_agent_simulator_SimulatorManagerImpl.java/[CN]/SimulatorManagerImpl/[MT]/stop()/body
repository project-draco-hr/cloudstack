{
  try {
    saveAllResourceStates(getAgentPath());
  }
 catch (  FileNotFoundException e) {
    s_logger.info("Failed to stop simulator because of " + e.getStackTrace());
  }
catch (  IOException e) {
    s_logger.info("Failed to stop simulator because of " + e.getStackTrace());
  }
  s_logger.info("Simulator stopped successfully");
  return true;
}
