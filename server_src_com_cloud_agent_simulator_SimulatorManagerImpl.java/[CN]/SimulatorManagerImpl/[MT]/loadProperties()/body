{
  File file=PropertiesUtil.findConfigFile("simulator.properties");
  if (file != null) {
    try {
      properties.load(new FileInputStream(file));
    }
 catch (    FileNotFoundException ex) {
      s_logger.info("simulator.properties file was not found. using defaults");
    }
catch (    IOException ex) {
    }
  }
 else {
    s_logger.warn("Could not find simulator.properties for loading simulator args");
  }
}
