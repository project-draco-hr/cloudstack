{
  if (_api == null) {
    return true;
  }
  _dbSyncTimer=new Timer("DBSyncTimer");
  try {
    _dbSyncTimer.schedule(new DBSyncTask(),0,_dbSyncInterval);
  }
 catch (  Exception ex) {
    logger.debug("Unable to start DB Sync timer " + ex.getMessage());
    logger.debug("timer start",ex);
  }
  return true;
}
